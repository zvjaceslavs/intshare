          <key>printer.tray.status[1]</key>
          <delay>180</delay>
          <description>返回值为0表示余量充足，返回值为9表示纸张已用尽。</description>
          <applications>
            <application>
              <name>Tray</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{count(5m,"9","ge")}>3</expression>
              <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
              <recovery_expression>{count(5m,"0","eq")}>3</recovery_expression>
