          <color>BB0000</color>
          <item>
            <host>Template SNMP QNAP NAS</host>
            <key>CPU-TemperatureEX</key>
          </item>
        </graph_item>
        <graph_item>
          <sortorder>1</sortorder>
          <color>0000BB</color>
          <item>
            <host>Template SNMP QNAP NAS</host>
            <key>SystemTemperatureEX</key>
          </item>
