<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T10:54:15Z</date>
  <groups>
    <group>
      <name>INFRA</name>
    </group>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>NETAPP_FAS_ONTAP_9.1</template>
      <name>NETAPP_FAS_ONTAP_9.1</name>
      <description>Template Ontap 9.1</description>
      <groups>
        <group>
          <name>INFRA</name>
        </group>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <items>
        <item>
          <name>Auto-support status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.789.1.2.7.1.0</snmp_oid>
          <key>autosupportStatus</key>
          <delay>3600</delay>
          <triggers>
            <trigger>
              <expression>{last()}&lt;>1</expression>
              <name>{HOST.NAME} Autosupport disable</name>
              <priority>WARNING</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>CPU %</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.789.1.2.1.3.0</snmp_oid>
          <key>cpuTimePerCent</key>
          <units>%</units>
          <triggers>
            <trigger>
              <expression>{last()}>90</expression>
              <name>{HOST.NAME} Cpu is over 90 %</name>
              <priority>WARNING</priority>
            </trigger>
            <trigger>
              <expression>{nodata(20m)}=1</expression>
              <name>{HOST.NAME} No SNMP data since 10min</name>
              <priority>AVERAGE</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Disk fail</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.789.1.6.4.7.0</snmp_oid>
          <key>failDisk</key>
          <delay>30s</delay>
          <triggers>
            <trigger>
              <expression>{last()}&lt;>0</expression>
              <name>{HOST.NAME} Disk fail</name>
              <priority>AVERAGE</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Total disk number</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.789.1.6.4.1.0</snmp_oid>
          <key>nbTotalDisk</key>
          <delay>3600</delay>
          <triggers>
            <trigger>
              <expression>{abschange()}&lt;>0</expression>
              <name>{HOST.NAME} Number of disk change</name>
              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Disk pre-fail</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.789.1.6.4.11.0</snmp_oid>
          <key>preFail</key>
          <delay>30s</delay>
          <triggers>
            <trigger>
              <expression>{last()}&lt;>0</expression>
              <name>{HOST.NAME} DIsk in preFail mode</name>
              <priority>WARNING</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Firmware</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.789.1.1.6.0</snmp_oid>
          <key>productFirmwareVersion</key>
          <delay>60m</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <triggers>
            <trigger>
              <expression>{diff()}=1</expression>
              <name>{HOSTNAME} Firmware have change</name>
              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.3.0</snmp_oid>
          <key>uptime</key>
          <delay>5m</delay>
          <triggers>
            <trigger>
              <expression>{last()}&lt;10m</expression>
              <name>Redemarrage de {HOST.NAME}</name>
              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Enclosure info</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.789.1.21.1.2.1.1]</snmp_oid>
          <key>enclosure</key>
          <delay>1h</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#SNMPVALUE}</macro>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <item_prototypes>
            <item_prototype>
              <name>Enclosure {#SNMPINDEX} power fail</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.789.1.21.1.2.1.33.{#SNMPINDEX}</snmp_oid>
              <key>ElecFail.[{#SNMPINDEX}]</key>
              <delay>60</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{strlen()}>0</expression>
                  <name>{HOST.NAME} Enclosure {#SNMPINDEX} Electronic failure</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Enclosure {#SNMPINDEX} fail</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.789.1.21.1.2.1.18.{#SNMPINDEX}</snmp_oid>
              <key>FanFail.[{#SNMPINDEX}]</key>
              <delay>30s</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{strlen()}>0</expression>
                  <name>{HOST.NAME} Enclosure {#SNMPINDEX} Fan fail</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Enclosure {#SNMPINDEX} sensors fail</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.789.1.21.1.2.1.50.{#SNMPINDEX}</snmp_oid>
              <key>SensorFail.[{#SNMPINDEX}]</key>
              <delay>60</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{strlen()}>0</expression>
                  <name>{HOST.NAME} Enclosure {#SNMPINDEX} Sensor failure</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Enclosure {#SNMPINDEX} temp over</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.789.1.21.1.2.1.21.{#SNMPINDEX}</snmp_oid>
              <key>TempOver.[{#SNMPINDEX}]</key>
              <delay>60</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{strlen()}>0</expression>
                  <name>{HOST.NAME} Enclosure {#SNMPINDEX} Temp Alert</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Enclosure {#SNMPINDEX} power fail</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.789.1.21.1.2.1.24.{#SNMPINDEX}</snmp_oid>
              <key>TempWarn.[{#SNMPINDEX}]</key>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{strlen()}>0</expression>
                  <name>{HOST.NAME} Enclosure {#SNMPINDEX} Temperature Warning</name>
                  <priority>AVERAGE</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Volumes</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#VOLNAME}, .1.3.6.1.4.1.789.1.4.4.1.2]</snmp_oid>
          <key>volume</key>
          <delay>3600</delay>
          <item_prototypes>
            <item_prototype>
              <name>Volume[{#VOLNAME}] TotalSize 64 bits</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.789.1.5.4.1.29.{#SNMPINDEX}</snmp_oid>
              <key>df64TotalKBytesVol[{#SNMPINDEX}]</key>
              <delay>3600</delay>
            </item_prototype>
            <item_prototype>
              <name>Volume[{#VOLNAME}] Used 64 bits</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.789.1.5.4.1.30.{#SNMPINDEX}</snmp_oid>
              <key>df64UsedKBytesVol[{#SNMPINDEX}]</key>
              <delay>3600</delay>
            </item_prototype>
            <item_prototype>
              <name>Pourcentage [{#VOLNAME}] vol used</name>
              <type>CALCULATED</type>
              <key>Pvolused-[{#VOLNAME}]</key>
              <delay>3600</delay>
              <units>%</units>
              <params>(100*last(df64UsedKBytesVol[{#SNMPINDEX}]))/last(df64TotalKBytesVol[{#SNMPINDEX}])</params>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>95</expression>
                  <name>{HOST.NAME} [{#VOLNAME}] used over 95 %</name>
                  <priority>AVERAGE</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>[{#VOLNAME}] used %</name>
              <graph_items>
                <graph_item>
                  <color>1A7C11</color>
                  <item>
                    <host>NETAPP_FAS_ONTAP_9.1</host>
                    <key>Pvolused-[{#VOLNAME}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
  <graphs>
    <graph>
      <name>CPU %</name>
      <graph_items>
        <graph_item>
          <color>1A7C11</color>
          <item>
            <host>NETAPP_FAS_ONTAP_9.1</host>
            <key>cpuTimePerCent</key>
          </item>
        </graph_item>
      </graph_items>
    </graph>
  </graphs>
</zabbix_export>