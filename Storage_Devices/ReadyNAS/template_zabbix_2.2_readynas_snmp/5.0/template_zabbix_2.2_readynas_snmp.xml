<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T10:04:01Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template SNMP ReadyNas</template>
      <name>Template SNMP ReadyNas</name>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Disk</name>
        </application>
        <application>
          <name>Fan</name>
        </application>
        <application>
          <name>General</name>
        </application>
        <application>
          <name>Volume</name>
        </application>
      </applications>
      <items>
        <item>
          <name>fanRPM</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.4526.22.4.1.2.1</snmp_oid>
          <key>fanRPM</key>
          <delay>30</delay>
          <applications>
            <application>
              <name>Fan</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}&lt;2000</expression>
              <name>Fan is too slow on {HOST.NAME}</name>
              <priority>DISASTER</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>fanStatus</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.4526.22.4.1.3.1</snmp_oid>
          <key>fanStatus</key>
          <delay>30</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>Fan</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{str("ok")}=0</expression>
              <name>Fan is not OK on {HOST.NAME}</name>
              <priority>DISASTER</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>S/N</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.4526.22.2.0</snmp_oid>
          <key>nasMgrSerialNUM.0</key>
          <delay>30</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Firmware</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.4526.22.1.0</snmp_oid>
          <key>nasMgrSoftwareVersion.0</key>
          <delay>30</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>System uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>HOST-RESOURCES-MIB::hrSystemUptime.0</snmp_oid>
          <key>sysUptime</key>
          <delay>30</delay>
          <units>uptime</units>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.01</params>
            </step>
          </preprocessing>
          <triggers>
            <trigger>
              <expression>{change(0)}&lt;0</expression>
              <name>{HOST.NAME} has just been restarted</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Temperature</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.4526.22.5.1.2.1</snmp_oid>
          <key>temperatureValue.1</key>
          <delay>30</delay>
          <units>°</units>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0)}>55</expression>
              <name>Temperature CPU is High on {HOST.NAME}</name>
              <priority>DISASTER</priority>
            </trigger>
          </triggers>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Discovery_Disk</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.4526.22.3.1.1]</snmp_oid>
          <key>Disk</key>
          <delay>600</delay>
          <item_prototypes>
            <item_prototype>
              <name>[{#SNMPVALUE}]_DiskCapacity</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.3.1.7.{#SNMPINDEX}</snmp_oid>
              <key>DiskCapacity[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <units>B</units>
              <applications>
                <application>
                  <name>Disk</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>[{#SNMPVALUE}]_DiskModel</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.3.1.5.{#SNMPINDEX}</snmp_oid>
              <key>DiskModel[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Disk</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>[{#SNMPVALUE}]_DiskSerial</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.3.1.4.{#SNMPINDEX}</snmp_oid>
              <key>DiskSerial[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Disk</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>[{#SNMPVALUE}]_DiskState</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.3.1.9.{#SNMPINDEX}</snmp_oid>
              <key>DiskState[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Disk</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{regexp("(Offline)")}=1</expression>
                  <name>Disk [{#SNMPVALUE}] on {HOST.NAME} is Offline</name>
                  <priority>DISASTER</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>[{#SNMPVALUE}]_DiskTemperature</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.3.1.10.{#SNMPINDEX}</snmp_oid>
              <key>DiskTemperature[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <units>°</units>
              <applications>
                <application>
                  <name>Disk</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(0)}>45</expression>
                  <name>Temperature Disk [{#SNMPVALUE}] is High on {HOST.NAME}</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>Disk Temperature {#SNMPVALUE}</name>
              <show_work_period>NO</show_work_period>
              <show_triggers>NO</show_triggers>
              <graph_items>
                <graph_item>
                  <drawtype>GRADIENT_LINE</drawtype>
                  <color>00C800</color>
                  <item>
                    <host>Template SNMP ReadyNas</host>
                    <key>DiskTemperature[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Discovery_Volume</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.4526.22.7.1.1]</snmp_oid>
          <key>Volume</key>
          <delay>600</delay>
          <item_prototypes>
            <item_prototype>
              <name>[{#SNMPVALUE}]_VolumeFreeSpace</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.7.1.6.{#SNMPINDEX}</snmp_oid>
              <key>volumeFreeSpace[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <value_type>FLOAT</value_type>
              <applications>
                <application>
                  <name>Volume</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>[{#SNMPVALUE}]_VolumeName</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.7.1.2.{#SNMPINDEX}</snmp_oid>
              <key>volumeName[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Volume</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>[{#SNMPVALUE}]_volumeRAIDLevel</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.7.1.3.{#SNMPINDEX}</snmp_oid>
              <key>volumeRAIDLevel[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <applications>
                <application>
                  <name>Volume</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>[{#SNMPVALUE}]_VolumeSize</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.7.1.5.{#SNMPINDEX}</snmp_oid>
              <key>volumeSize[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <applications>
                <application>
                  <name>Volume</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>[{#SNMPVALUE}]_VolumeStatus</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.4526.22.7.1.4.{#SNMPINDEX}</snmp_oid>
              <key>volumeStatus[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Volume</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{regexp("(UNPROTECTED|DEGRADED|DEAD|INACTIVE|UNKNOW)")}=1</expression>
                  <name>Raid [{#SNMPVALUE}] on {HOST.NAME} - Problem</name>
                  <priority>DISASTER</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
          <trigger_prototypes>
            <trigger_prototype>
              <expression>({Template SNMP ReadyNas:volumeFreeSpace[{#SNMPVALUE}].last()} / {Template SNMP ReadyNas:volumeSize[{#SNMPVALUE}].last()} * 100) &lt; 10</expression>
              <name>Volume {#SNMPVALUE} - FreeSpace is less than 10% on {HOST.NAME}</name>
              <priority>HIGH</priority>
            </trigger_prototype>
          </trigger_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>Volumen Size {#SNMPVALUE}</name>
              <ymax_type_1>ITEM</ymax_type_1>
              <ymax_item_1>
                <host>Template SNMP ReadyNas</host>
                <key>volumeSize[{#SNMPVALUE}]</key>
              </ymax_item_1>
              <graph_items>
                <graph_item>
                  <drawtype>GRADIENT_LINE</drawtype>
                  <color>EE0000</color>
                  <item>
                    <host>Template SNMP ReadyNas</host>
                    <key>volumeFreeSpace[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>1</sortorder>
                  <drawtype>GRADIENT_LINE</drawtype>
                  <color>00DD00</color>
                  <item>
                    <host>Template SNMP ReadyNas</host>
                    <key>volumeSize[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
