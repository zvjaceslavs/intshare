              </applications>
            </item_prototype>
            <item_prototype>
              <name>UPS No{#SNMPINDEX} Product</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.5127.1.1.1.1.1.11.1.4.{#SNMPINDEX}</snmp_oid>
