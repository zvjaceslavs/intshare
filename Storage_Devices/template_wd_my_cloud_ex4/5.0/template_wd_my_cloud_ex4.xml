                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>UPS No{#SNMPINDEX} Status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.5127.1.1.1.1.1.11.1.6.{#SNMPINDEX}</snmp_oid>
              <key>upsStatus[{#SNMPINDEX}]</key>
              <history>30d</history>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
