          <snmp_oid>.1.3.6.1.4.1.5127.1.1.1.1.1.7.0</snmp_oid>
          <key>temperature</key>
          <history>1w</history>
          <trends>30d</trends>
          <applications>
            <application>
              <name>System</name>
