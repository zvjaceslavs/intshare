            <discovery_rules>
                <discovery_rule>
                    <name>ONT Active Count Discovery</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#ONTACTIVECOUNT}, .1.3.6.1.4.1.35265.1.22.2.1.1.4.1]</snmp_oid>
                    <key>net.pon.ont.active.count.discovery</key>
                    <lifetime>7d</lifetime>
                    <description>Discovering how many active ONT</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>Interface PON {#SNMPINDEX}: ONT active</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.35265.1.22.2.1.1.4.1.{#SNMPINDEX}</snmp_oid>
                            <key>net.pon.ont.active.count[{#SNMPINDEX}]</key>
                            <history>2w</history>
                            <trends>0</trends>
                            <description>How many active ONT on port</description>
                            <applications>
                                <application>
                                    <name>ONT count</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>ONT Interfaces Discovery</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#ONTCHANNEL}, .1.3.6.1.4.1.35265.1.22.3.1.1.3, {#ONTID}, .1.3.6.1.4.1.35265.1.22.3.1.1.4, {#ONTNAME}, .1.3.6.1.4.1.35265.1.22.3.4.1.8]</snmp_oid>
                    <key>net.pon.ont.if.discovery</key>
                    <lifetime>7d</lifetime>
                    <description>Discovering ONT interfaces for Eltex LTP</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>Interface {#ONTCHANNEL}/{#ONTID} ({#ONTNAME}): Distance</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.35265.1.22.3.1.1.10.{#SNMPINDEX}</snmp_oid>
                            <key>net.pon.if.distance[{#SNMPINDEX}]</key>
                            <delay>3h</delay>
                            <history>30d</history>
                            <trends>0</trends>
                            <units>m</units>
                            <description>Distance in meters</description>
                            <applications>
                                <application>
                                    <name>ONT interfaces</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface {#ONTCHANNEL}/{#ONTID} ({#ONTNAME}): Bits received</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.35265.1.22.3.3.11.1.6.{#SNMPINDEX}.1.1</snmp_oid>
                            <key>net.pon.if.in[{#SNMPINDEX}]</key>
                            <delay>3m</delay>
                            <history>30d</history>
                            <units>bps</units>
                            <description>PON Interface: Bits received</description>
                            <applications>
                                <application>
                                    <name>ONT interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface {#ONTCHANNEL}/{#ONTID} ({#ONTNAME}): Bits sent</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.35265.1.22.3.3.10.1.6.{#SNMPINDEX}.1.1</snmp_oid>
                            <key>net.pon.if.out[{#SNMPINDEX}]</key>
                            <delay>3m</delay>
                            <history>30d</history>
                            <units>bps</units>
                            <description>PON Interface: Bits sent</description>
                            <applications>
                                <application>
                                    <name>ONT interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface {#ONTCHANNEL}/{#ONTID} ({#ONTNAME}): RX power</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.35265.1.22.3.1.1.14.{#SNMPINDEX}</snmp_oid>
                            <key>net.pon.if.rxpower[{#SNMPINDEX}]</key>
                            <delay>1h</delay>
                            <history>2w</history>
                            <trends>0</trends>
                            <value_type>FLOAT</value_type>
                            <description>RX power</description>
                            <applications>
                                <application>
                                    <name>ONT interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.001</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface {#ONTCHANNEL}/{#ONTID} ({#ONTNAME}): Operational status</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.35265.1.22.3.1.1.5.{#SNMPINDEX}</snmp_oid>
                            <key>net.pon.if.status[{#SNMPINDEX}]</key>
                            <delay>1h</delay>
                            <history>2w</history>
                            <trends>0</trends>
                            <description>(0) - free, (1) - allocated, (2) - authInProgress, (3) - authFailed&#13;
(4) - authOk, (5) - cfgInProgress, (6) - cfgFailed, (7) - ok&#13;
(8) - failed, (9) - blocked, (10) - mibreset, (11) - preconfig&#13;
(12) - fwUpdating, (13) - unactivated, (14) - redundant, (15) - disabled&#13;
(16) - unknown</description>
                            <applications>
                                <application>
                                    <name>ONT interfaces</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface {#ONTCHANNEL}/{#ONTID} ({#ONTNAME}): Temperature</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.35265.1.22.3.1.1.15.{#SNMPINDEX}</snmp_oid>
                            <key>net.pon.if.temp[{#SNMPINDEX}]</key>
                            <delay>1h</delay>
                            <history>2w</history>
                            <trends>0</trends>
                            <value_type>FLOAT</value_type>
                            <units>°C</units>
                            <description>Laser temperature</description>
                            <applications>
                                <application>
                                    <name>ONT interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.1</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Interface {#ONTCHANNEL}/{#ONTID} ({#ONTNAME}): TX power</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.35265.1.22.3.1.1.13.{#SNMPINDEX}</snmp_oid>
                            <key>net.pon.if.txpower[{#SNMPINDEX}]</key>
                            <delay>1h</delay>
                            <history>2w</history>
                            <trends>0</trends>
                            <value_type>FLOAT</value_type>
                            <description>TX power</description>
                            <applications>
                                <application>
                                    <name>ONT interfaces</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.001</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Interface {#ONTCHANNEL}/{#ONTID}({#ONTNAME}): PON traffic</name>
                            <graph_items>
                                <graph_item>
                                    <drawtype>GRADIENT_LINE</drawtype>
                                    <color>1A7C11</color>
                                    <item>
                                        <host>Eltex LTP-4X</host>
                                        <key>net.pon.if.in[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <drawtype>BOLD_LINE</drawtype>
                                    <color>2774A4</color>
                                    <item>
                                        <host>Eltex LTP-4X</host>
                                        <key>net.pon.if.out[{#SNMPINDEX}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
