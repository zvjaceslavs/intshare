                    <snmpv3_privpassphrase>{$PRIV_PASSPHRASE}</snmpv3_privpassphrase>
                    <description>Can be:&#13;
- Primary unit (this device)&#13;
- Secondary unit (this device)</description>
                    <applications>
                        <application>
                            <name>General</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <params>Primary.*unit.*this.*device.
\0</params>
