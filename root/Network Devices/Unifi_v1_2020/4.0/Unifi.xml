<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-01-17T00:16:38Z</date>
    <groups>
        <group>
            <name>Ubiquiti</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Unifi</template>
            <name>Unifi</name>
            <description>By Deyrick J. Santos</description>
            <groups>
                <group>
                    <name>Ubiquiti</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Health</name>
                </application>
                <application>
                    <name>ICMP</name>
                </application>
                <application>
                    <name>Inventory</name>
                </application>
                <application>
                    <name>Memory</name>
                </application>
                <application>
                    <name>Network</name>
                </application>
                <application>
                    <name>Signal</name>
                </application>
                <application>
                    <name>Virtual Interfaces</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>ICMP ping</name>
                    <type>SIMPLE</type>
                    <key>icmpping</key>
                    <delay>30s</delay>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>ICMP</name>
                        </application>
                    </applications>
                    <request_method>POST</request_method>
                </item>
                <item>
                    <name>ICMP loss</name>
                    <type>SIMPLE</type>
                    <key>icmppingloss</key>
                    <delay>30s</delay>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>ICMP</name>
                        </application>
                    </applications>
                    <request_method>POST</request_method>
                </item>
                <item>
                    <name>ICMP response time</name>
                    <type>SIMPLE</type>
                    <key>icmppingsec</key>
                    <delay>30s</delay>
                    <history>30d</history>
                    <value_type>FLOAT</value_type>
                    <units>s</units>
                    <applications>
                        <application>
                            <name>ICMP</name>
                        </application>
                    </applications>
                    <request_method>POST</request_method>
                </item>
                <item>
                    <name>Out Traffic</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.2.2.1.11.2</snmp_oid>
                    <key>ifOutOctets</key>
                    <delay>120s</delay>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>Network</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>CHANGE_PER_SECOND</type>
                            <params/>
                        </step>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>8</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>MAC</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>iso.3.6.1.2.1.2.2.1.6.2</snmp_oid>
                    <key>iso.3.6.1.2.1.2.2.1.6.2</key>
                    <delay>3m</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Network</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>In Traffic</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>iso.3.6.1.2.1.2.2.1.10.2</snmp_oid>
                    <key>iso.3.6.1.2.1.2.2.1.10.2</key>
                    <delay>120s</delay>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>Network</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>CHANGE_PER_SECOND</type>
                            <params/>
                        </step>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>8</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>Porta Lan Status</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.2.2.1.5.2</snmp_oid>
                    <key>porta.lan</key>
                    <delay>300s</delay>
                    <units>pbs</units>
                    <applications>
                        <application>
                            <name>Network</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Número de estações conectadas no rádio</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.8.1</snmp_oid>
                    <key>station.listubnt</key>
                    <delay>300s</delay>
                    <history>7d</history>
                    <trends>90d</trends>
                    <applications>
                        <application>
                            <name>Health</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Localização do Dispositivo</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>SNMPv2-MIB::sysLocation.0</snmp_oid>
                    <key>sysLocation</key>
                    <delay>3600</delay>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <applications>
                        <application>
                            <name>Health</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>CPU utilization</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.10002.1.1.1.4.2.1.3.2</snmp_oid>
                    <key>system.cpu.util[loadValue.2]</key>
                    <delay>30s</delay>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>Health</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Firmware version</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.41112.1.6.3.6.0</snmp_oid>
                    <key>system.hw.firmware</key>
                    <delay>1h</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Hardware model name</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.1.1.0</snmp_oid>
                    <key>system.hw.model</key>
                    <delay>1h</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Device name</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>iso.3.6.1.2.1.1.5.0</snmp_oid>
                    <key>system.name</key>
                    <delay>30s</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Health</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Device uptime</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.1.3.0</snmp_oid>
                    <key>sysUpTime</key>
                    <delay>3000s</delay>
                    <history>7d</history>
                    <trends>90d</trends>
                    <units>s</units>
                    <applications>
                        <application>
                            <name>Health</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>0.01</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>Model</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.41112.1.6.3.3.0</snmp_oid>
                    <key>unifiApSystemModel.0</key>
                    <delay>1h</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Inventory</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>SSID 2.4Ghz</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.6.1</snmp_oid>
                    <key>unifiVapEssId.1</key>
                    <delay>12h</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Network</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>SSID 5Ghz</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.41112.1.6.1.2.1.6.3</snmp_oid>
                    <key>unifiVapEssId.3</key>
                    <delay>12h</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Network</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Available memory</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.10002.1.1.1.1.2.0</snmp_oid>
                    <key>vm.memory.free[memFree]</key>
                    <delay>3m</delay>
                    <history>7d</history>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>Memory</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Memory utilization</name>
                    <type>CALCULATED</type>
                    <key>vm.memory.pused[memoryUsedPercentage]</key>
                    <delay>3m</delay>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <params>((last(vm.memory.total[memTotal])-last(vm.memory.free[memFree]))/last(vm.memory.total[memTotal]))*100</params>
                    <description>Memory utilization in %</description>
                    <applications>
                        <application>
                            <name>Memory</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Total memory</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.10002.1.1.1.1.1.0</snmp_oid>
                    <key>vm.memory.total[memTotal]</key>
                    <delay>3m</delay>
                    <history>7d</history>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>Memory</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Wifi Virtual Interfaces</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#UNIFIVAPESSID},.1.3.6.1.4.1.41112.1.6.1.2.1.6, {#UNIVAPRADIO},.1.3.6.1.4.1.41112.1.6.1.2.1.9]</snmp_oid>
                    <key>unifiVapEssId</key>
                    <item_prototypes>
                        <item_prototype>
                            <name>WIFI Channel $2 on $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.4.{#SNMPINDEX}</snmp_oid>
                            <key>unifiVapChannel[{#UNIFIVAPESSID},{#UNIVAPRADIO}]</key>
                            <applications>
                                <application>
                                    <name>Virtual Interfaces</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Users $2 on $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.8.{#SNMPINDEX}</snmp_oid>
                            <key>unifiVapNumStations[{#UNIFIVAPESSID},{#UNIVAPRADIO}]</key>
                            <units>Users</units>
                            <applications>
                                <application>
                                    <name>Virtual Interfaces</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Users {#UNIVAPRADIO} connected on {#UNIFIVAPESSID}</name>
                            <show_work_period>NO</show_work_period>
                            <show_triggers>NO</show_triggers>
                            <type>STACKED</type>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>199C0D</color>
                                    <item>
                                        <host>Unifi - Sinal Br</host>
                                        <key>unifiVapNumStations[{#UNIFIVAPESSID},{#UNIVAPRADIO}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
