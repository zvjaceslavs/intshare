                <item>
                    <name>ByPass Frequency</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.232.165.3.5.1.0</snmp_oid>
                    <key>upsByPassFrequency1</key>
                    <delay>30</delay>
                    <units>Hz</units>
                    <applications>
