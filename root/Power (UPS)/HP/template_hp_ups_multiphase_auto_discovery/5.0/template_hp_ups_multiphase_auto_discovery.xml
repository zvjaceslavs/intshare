                <item>
                    <name>Segment #2</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.232.165.3.10.2.1.2.2</snmp_oid>
                    <key>upsOutputSegment2</key>
                    <delay>30s</delay>
                    <applications>
