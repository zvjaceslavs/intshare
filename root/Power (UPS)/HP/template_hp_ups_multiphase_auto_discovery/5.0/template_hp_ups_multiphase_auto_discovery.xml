                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                            <tags>
                                <tag>
                                    <tag>Loc</tag>
                                    <value>{INVENTORY.LOCATION1}</value>
                                </tag>
                            </tags>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Estimated minutes remaining if no power</name>
