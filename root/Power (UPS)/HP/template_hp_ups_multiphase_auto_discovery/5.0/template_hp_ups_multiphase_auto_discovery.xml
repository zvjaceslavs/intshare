                    <key>upsOutputLoad</key>
                    <delay>30</delay>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>Output</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Segment #1</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.232.165.3.10.2.1.2.1</snmp_oid>
                    <key>upsOutputSegment1</key>
                    <delay>30s</delay>
                    <applications>
