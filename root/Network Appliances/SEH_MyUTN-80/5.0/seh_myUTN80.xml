<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-07-30T09:32:40Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Device SEH UTN-80</template>
            <name>Device SEH UTN-80</name>
            <templates>
                <template>
                    <name>Template Module Generic SNMPv2</name>
                </template>
            </templates>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>USB Ports</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>USB Ports</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#PORTTAG},1.3.6.1.4.1.1229.2.50.2.1.10,{#PORTSLOT},1.3.6.1.4.1.1229.2.50.2.1.27,{#PORTSERIAL},1.3.6.1.4.1.1229.2.50.2.1.25]</snmp_oid>
                    <key>porttag</key>
                    <delay>1h</delay>
                    <filter>
                        <evaltype>AND</evaltype>
                        <conditions>
                            <condition>
                                <macro>{#PORTTAG}</macro>
                                <value>{$PORT.TAG.MATCHES}</value>
                                <formulaid>E</formulaid>
                            </condition>
                            <condition>
                                <macro>{#PORTTAG}</macro>
                                <value>{$PORT.TAG.NOT_MATCHES}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>F</formulaid>
                            </condition>
                            <condition>
                                <macro>{#PORTSLOT}</macro>
                                <value>{$PORT.SLOT.MATCHES}</value>
                                <formulaid>C</formulaid>
                            </condition>
                            <condition>
                                <macro>{#PORTSLOT}</macro>
                                <value>{$PORT.SLOT.NOT_MATCHES}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>D</formulaid>
                            </condition>
                            <condition>
                                <macro>{#PORTSERIAL}</macro>
                                <value>{$PORT.SERIAL.MATCHES}</value>
                                <formulaid>A</formulaid>
                            </condition>
                            <condition>
                                <macro>{#PORTSERIAL}</macro>
                                <value>{$PORT.SERIAL.NOT_MATCHES}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>B</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>USB {#PORTSLOT} ({#PORTTAG}) Connection Status</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.1229.2.50.2.1.26.{#SNMPINDEX}</snmp_oid>
                            <key>utnPortUsbOwn[{#SNMPINDEX}]</key>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <applications>
                                <application>
                                    <name>USB Ports</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{diff()}=1</expression>
                                    <name>Connection Status of  {#PORTSLOT} ({#PORTTAG}) Changed</name>
                                    <opdata>{ITEM.VALUE}</opdata>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>USB {#PORTSLOT} ({#PORTTAG}) Product String</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.1229.2.50.2.1.22.{#SNMPINDEX}</snmp_oid>
                            <key>utnPortUsbProd[{#SNMPINDEX}]</key>
                            <delay>15m</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <applications>
                                <application>
                                    <name>USB Ports</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>USB {#PORTSLOT} ({#PORTTAG}) Serial Number</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.1229.2.50.2.1.25.{#SNMPINDEX}</snmp_oid>
                            <key>utnPortUsbSer[{#SNMPINDEX}]</key>
                            <delay>15m</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <applications>
                                <application>
                                    <name>USB Ports</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$PORT.SERIAL.MATCHES}</macro>
                    <value>.*</value>
                    <description>This macro is used in USB Port discovery. Matches all Ports which are be included.</description>
                </macro>
                <macro>
                    <macro>{$PORT.SERIAL.NOT_MATCHES}</macro>
                    <value>^-$</value>
                    <description>This macro is used in USB Port discovery. Matches all Ports which are excluded.</description>
                </macro>
                <macro>
                    <macro>{$PORT.SLOT.MATCHES}</macro>
                    <value>.*</value>
                    <description>This macro is used in USB Port discovery. Matches all Ports which are be included.</description>
                </macro>
                <macro>
                    <macro>{$PORT.SLOT.NOT_MATCHES}</macro>
                    <value>CHANGE_ME</value>
                    <description>This macro is used in USB Port discovery. Matches all Ports which are excluded.</description>
                </macro>
                <macro>
                    <macro>{$PORT.TAG.MATCHES}</macro>
                    <value>.*</value>
                    <description>This macro is used in USB Port discovery. Matches all Ports which are be included.</description>
                </macro>
                <macro>
                    <macro>{$PORT.TAG.NOT_MATCHES}</macro>
                    <value>CHANGE_ME</value>
                    <description>This macro is used in USB Port discovery. Matches all Ports which are excluded.</description>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
