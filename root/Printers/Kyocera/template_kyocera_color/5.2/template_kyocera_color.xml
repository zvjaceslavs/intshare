<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.2</version>
    <date>2021-04-08T06:56:30Z</date>
    <groups>
        <group>
            <name>Printers</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Kyocera color</template>
            <name>Kyocera color</name>
            <groups>
                <group>
                    <name>Printers</name>
                </group>
            </groups>
            <items>
                <item>
                    <name>Device description</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.25.3.2.1.3.1</snmp_oid>
                    <key>hrDeviceDescr</key>
                    <delay>1h</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>toners</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.2.1.43.12.1.1.4.1]</snmp_oid>
                    <key>toners</key>
                    <item_prototypes>
                        <item_prototype>
                            <name>persent_{#SNMPVALUE}</name>
                            <type>CALCULATED</type>
                            <key>percent_[{#SNMPVALUE}]</key>
                            <params>100*(last(&quot;toner_current_[{#SNMPVALUE}]&quot;)/last(&quot;toner_default_[{#SNMPVALUE}]&quot;))</params>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;5</expression>
                                    <name>{#SNMPVALUE} тонер менее 5%</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>toner current {#SNMPVALUE}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.43.11.1.1.9.1.{#SNMPINDEX}</snmp_oid>
                            <key>toner_current_[{#SNMPVALUE}]</key>
                        </item_prototype>
                        <item_prototype>
                            <name>toner default {#SNMPVALUE}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.2.1.43.11.1.1.8.1.{#SNMPINDEX}</snmp_oid>
                            <key>toner_default_[{#SNMPVALUE}]</key>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>toners {#SNMPVALUE}</name>
                            <show_work_period>NO</show_work_period>
                            <show_triggers>NO</show_triggers>
                            <type>STACKED</type>
                            <show_legend>NO</show_legend>
                            <ymin_type_1>FIXED</ymin_type_1>
                            <ymax_type_1>FIXED</ymax_type_1>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>199C0D</color>
                                    <item>
                                        <host>Kyocera color</host>
                                        <key>percent_[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
