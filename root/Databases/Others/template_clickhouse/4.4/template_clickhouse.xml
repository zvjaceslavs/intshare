                    <history>31d</history>
                    <description>Number of inactive parts&#13;
&#13;
SELECT COUNT(*) AS c FROM system.parts WHERE active = 0 FORMAT JSON</description>
                    <applications>
                        <application>
                            <name>Clickhouse</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>return JSON.parse(value.substring(value.indexOf(&quot;{\n&quot;)).trim()).data[0].c;</params>
