<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2019-11-12T06:17:26Z</date>
    <groups>
        <group>
            <name>Templates Hardware</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template SNMP XSCF Sun M5000</template>
            <name>Template SNMP XSCF Sun M5000</name>
            <description>please set {$SNMP_COMMUNITY} macros</description>
            <groups>
                <group>
                    <name>Templates Hardware</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Hardware</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>scfFirmwareState</name>
                    <type>SNMPV2</type>
                    <snmp_community>public</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.2.2.0</snmp_oid>
                    <key>scfFirmwareState</key>
                    <delay>3600</delay>
                    <history>7d</history>
                    <description>SCF software state&#13;
normal(1),&#13;
degraded(2),&#13;
faulted(3),&#13;
change(254),&#13;
unknown(255)</description>
                    <applications>
                        <application>
                            <name>Hardware</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;&gt;1</expression>
                            <name>Firmware State Faulted</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>scfHardwareState</name>
                    <type>SNMPV2</type>
                    <snmp_community>public</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.2.3.0</snmp_oid>
                    <key>scfHardwareState</key>
                    <delay>3600</delay>
                    <history>7d</history>
                    <description>Overall system hardware state&#13;
normal(1),&#13;
degraded(2),&#13;
faulted(3),&#13;
change(254),&#13;
unknown(255)</description>
                    <applications>
                        <application>
                            <name>Hardware</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;&gt;1</expression>
                            <name>Hardware State Faulted</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>scfSystemAmbientTemperatureValue</name>
                    <type>SNMPV2</type>
                    <snmp_community>public</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.4.12.1.0</snmp_oid>
                    <key>scfSystemAmbientTemperatureValue</key>
                    <delay>600</delay>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <units>degC</units>
                    <applications>
                        <application>
                            <name>Hardware</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last()}&gt;50</expression>
                            <name>Ambient Temperature on server extremly HI - is over 50 degrees</name>
                            <priority>DISASTER</priority>
                        </trigger>
                        <trigger>
                            <expression>{last()}&gt;30</expression>
                            <name>Ambient Temperature on server extremly is over 30 degrees</name>
                            <priority>AVERAGE</priority>
                        </trigger>
                        <trigger>
                            <expression>{last()}&gt;40</expression>
                            <name>Ambient Temperature on server is over 40 degrees</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>scfSystemSerialNumber</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.4.3.0</snmp_oid>
                    <key>scfSystemSerialNumber</key>
                    <delay>86400</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>SERIALNO_A</inventory_link>
                    <applications>
                        <application>
                            <name>Hardware</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>scfSystemState</name>
                    <type>SNMPV2</type>
                    <snmp_community>public</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.2.1.0</snmp_oid>
                    <key>scfSystemState</key>
                    <delay>3600</delay>
                    <history>7d</history>
                    <description>Overall system state&#13;
normal(1),&#13;
degraded(2),&#13;
faulted(3),&#13;
change(254),&#13;
unknown(255)</description>
                    <applications>
                        <application>
                            <name>Hardware</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;&gt;1</expression>
                            <name>System State Faulted</name>
                            <priority>DISASTER</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>scfSystemType</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.4.2.0</snmp_oid>
                    <key>scfSystemType</key>
                    <delay>86400</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>CHASSIS</inventory_link>
                    <applications>
                        <application>
                            <name>Hardware</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>CPU discovery</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#TYPE}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.3, {#MODULEID}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.6, {#BOARDID}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.2]</snmp_oid>
                    <key>scfComponentTable.cpu</key>
                    <delay>86400</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>3</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>CPU {#BOARDID}-{#MODULEID} status</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.14.{#SNMPINDEX}</snmp_oid>
                            <key>cpu.status[{#BOARDID}-{#MODULEID}]</key>
                            <delay>60</delay>
                            <history>7d</history>
                            <applications>
                                <application>
                                    <name>Hardware</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;1</expression>
                                    <name>CPU {#BOARDID}-{#MODULEID} Faulted</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Fan discovery</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#TYPE}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.1, {#BOARDID}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.4]</snmp_oid>
                    <key>scfComponentTable.fan</key>
                    <delay>86400</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>48</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Fan #{#BOARDID} status</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.14.{#SNMPINDEX}</snmp_oid>
                            <key>fan.status[{#BOARDID}]</key>
                            <delay>60</delay>
                            <history>7d</history>
                            <applications>
                                <application>
                                    <name>Hardware</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;1</expression>
                                    <name>Fan #{$BOARDID} Failed</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Memory unit discovery</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#TYPE}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.5, {#BOARDID}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.6]</snmp_oid>
                    <key>scfComponentTable.mem</key>
                    <delay>86400</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>9</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Memory unit {#BOARDID} status</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.14.{#SNMPINDEX}</snmp_oid>
                            <key>mem.statsu[{#BOARDID}]</key>
                            <delay>60</delay>
                            <history>7d</history>
                            <applications>
                                <application>
                                    <name>Hardware</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;1</expression>
                                    <name>Memory unit {#BOARDID} Faulted</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Memory Board discovery</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#TYPE}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.1, {#MTYPE}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.3, {#ID}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.4]</snmp_oid>
                    <key>scfComponentTable.memb</key>
                    <delay>86400</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>115</value>
                                <formulaid>B</formulaid>
                            </condition>
                            <condition>
                                <macro>{#MTYPE}</macro>
                                <value>110</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Memory Board #{#ID} status</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.14.{#SNMPINDEX}</snmp_oid>
                            <key>memb.status[{#ID}]</key>
                            <delay>60</delay>
                            <history>7d</history>
                            <applications>
                                <application>
                                    <name>Hardware</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;1</expression>
                                    <name>Memory Board #{#ID} Faulted</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>PSU discovery</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#TYPE}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.1, {#BOARDID}, .1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.2]</snmp_oid>
                    <key>scfComponentTable.psu</key>
                    <delay>86400</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#TYPE}</macro>
                                <value>42</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Power Supply #{#BOARDID} status</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.211.1.15.3.1.1.12.2.1.14.{#SNMPINDEX}</snmp_oid>
                            <key>psu.status[{#BOARDID}]</key>
                            <delay>60</delay>
                            <history>7d</history>
                            <applications>
                                <application>
                                    <name>Hardware</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;1</expression>
                                    <name>Power Supply #{#BOARDID} Faulted</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
    <graphs>
        <graph>
            <name>Ambient temperature</name>
            <graph_items>
                <graph_item>
                    <color>1A7C11</color>
                    <item>
                        <host>Template SNMP XSCF Sun M5000</host>
                        <key>scfSystemAmbientTemperatureValue</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
    </graphs>
</zabbix_export>
