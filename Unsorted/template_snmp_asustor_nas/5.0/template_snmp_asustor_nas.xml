                <application>
                  <name>Hard disks</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Disk #{#SNMPINDEX} status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.44738.4.1.1.5.{#SNMPINDEX}</snmp_oid>
              <key>storage.diskStatus[{#SNMPINDEX}]</key>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>S.M.A.R.T. статус диска (Healthy, Normal, Risky, Dangerous, Bad)</description>
              <applications>
                <application>
                  <name>Hard disks</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{regexp("bad")}=1</expression>
                  <name>Hard Disk #{#SNMPINDEX} status: Bad</name>
                  <priority>DISASTER</priority>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{regexp("Risky")}=1</expression>
                  <name>Hard Disk #{#SNMPINDEX} status: Risky</name>
                  <priority>WARNING</priority>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{regexp("Dangerous")}=1</expression>
                  <name>Hard Disk #{#SNMPINDEX} status: Warning</name>
                  <priority>AVERAGE</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Disk #{#SNMPINDEX} temperature</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.44738.4.1.1.6.{#SNMPINDEX}</snmp_oid>
