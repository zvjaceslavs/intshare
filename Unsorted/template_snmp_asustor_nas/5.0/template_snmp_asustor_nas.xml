              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>60</expression>
                  <name>Hard Disk #{#SNMPINDEX} Temperature to High > 60℃</name>
                  <priority>AVERAGE</priority>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
