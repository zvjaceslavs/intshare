<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-26T04:40:13Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>DEVA SmartGen 6</template>
      <name>DEVA SmartGen 6</name>
      <description>Template for SNMP-enabled RDS coder DEVA SmartGen 6 http://www.devabroadcast.com v1.0: 2017-10-30 Roman Ermakov, r.ermakov@emg.fm</description>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>GPS Monitor</name>
        </application>
        <application>
          <name>Hardware Status</name>
        </application>
        <application>
          <name>RDS Monitor</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Internal Diskspace Free</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.4.3.5.0</snmp_oid>
          <key>device.diskSpaceFree</key>
          <delay>20</delay>
          <value_type>FLOAT</value_type>
          <units>B</units>
          <description>"Free Disk Space (KiB)"</description>
          <applications>
            <application>
              <name>Hardware Status</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>1000</params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>Internal Diskspace Free Percent</name>
          <type>CALCULATED</type>
          <key>device.diskSpaceFree.Percent</key>
          <delay>20</delay>
          <units>%</units>
          <params>last("device.diskSpaceFree")*100/last("device.diskSpaceTotal")</params>
          <applications>
            <application>
              <name>Hardware Status</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#5)}&lt;5</expression>
              <name>Free disk space is less than 5%</name>
              <priority>AVERAGE</priority>
            </trigger>
            <trigger>
              <expression>{last(#5)}&lt;20</expression>
              <name>Free disk space is less than 20%</name>
              <priority>WARNING</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Internal Diskspace Total</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.4.3.4.0</snmp_oid>
          <key>device.diskSpaceTotal</key>
          <delay>20</delay>
          <value_type>FLOAT</value_type>
          <units>B</units>
          <description>"Total Disk Space (KiB)"</description>
          <applications>
            <application>
              <name>Hardware Status</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>1000</params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>RDS Device Localtime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.2.1.3.1.0</snmp_oid>
          <key>device.RDS.Localtime</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>"Encoder time"</description>
          <applications>
            <application>
              <name>Hardware Status</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Device Serial Number</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.1.3.0</snmp_oid>
          <key>device.serNum</key>
          <delay>86400</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>device.serNum</description>
          <inventory_link>SERIALNO_A</inventory_link>
          <applications>
            <application>
              <name>Hardware Status</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Internal Temperature</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.4.2.0</snmp_oid>
          <key>device.Temperature</key>
          <delay>1200</delay>
          <units>C</units>
          <description>"Device internal temperature EXAMPLE:355 = 35.5 deg C"</description>
          <applications>
            <application>
              <name>Hardware Status</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.1</params>
            </step>
          </preprocessing>
          <triggers>
            <trigger>
              <expression>{last(#10)}>49</expression>
              <name>Device is overheat</name>
              <priority>WARNING</priority>
              <description>Last 10 measurements: The temperature is over 45C</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>GPS Fix Mode</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.4.4.7.0</snmp_oid>
          <key>mntrGPS.FixMode</key>
          <delay>1200</delay>
          <description>mntrGPS.FixMode INTEGER { fixModeUnknown ( 0 ) , fixNotAvailable ( 1 ) , fix2D ( 2 ) , fix3D ( 3 ) }</description>
          <applications>
            <application>
              <name>GPS Monitor</name>
            </application>
          </applications>
        </item>
        <item>
          <name>GPS Satellites In Use</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.4.4.6.0</snmp_oid>
          <key>mntrGPS.SatsInUse</key>
          <delay>1200</delay>
          <description>"Satellites in use"</description>
          <applications>
            <application>
              <name>GPS Monitor</name>
            </application>
          </applications>
        </item>
        <item>
          <name>GPS Satellites In View</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.4.4.5.0</snmp_oid>
          <key>mntrGPS.SatsInView</key>
          <delay>1200</delay>
          <description>"Satellites in view"</description>
          <applications>
            <application>
              <name>GPS Monitor</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Status PTT</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.4.0</snmp_oid>
          <key>mntrRDS.PTT.Status</key>
          <delay>1200</delay>
          <description>"Pilot Status"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>BOOL_TO_DECIMAL</type>
              <params></params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>Status RDS Level</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.6.0</snmp_oid>
          <key>mntrRDS.RDS.Level</key>
          <delay>60</delay>
          <value_type>FLOAT</value_type>
          <units>V</units>
          <description>"monitor RDS Level Value"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>.001</params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>Status PI</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.8.0</snmp_oid>
          <key>mntrRDS.RDS.PI</key>
          <delay>1200</delay>
          <description>"monitor RDS PI"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{diff()}=1</expression>
              <name>PI Code has changed</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Status PS</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.10.0</snmp_oid>
          <key>mntrRDS.RDS.PS</key>
          <delay>45</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>"monitor RDS PS"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{diff(10m)}=0</expression>
              <name>PS is not changing</name>
              <priority>WARNING</priority>
              <description>PS is the same for 10 minutes. Disable this trigger on static PS data.</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Status PTY</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.9.0</snmp_oid>
          <key>mntrRDS.RDS.PTY</key>
          <delay>1200</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>"monitor RDS PTY"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Status RT</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.11.0</snmp_oid>
          <key>mntrRDS.RDS.RT</key>
          <delay>180</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>"monitor RDS RT"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{diff(10m)}=0</expression>
              <name>RT is not changing</name>
              <priority>WARNING</priority>
              <description>RT is the same for 10 minutes. Disable this trigger on static RT data.</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Status TA</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.12.0</snmp_oid>
          <key>mntrRDS.RDS.TA</key>
          <delay>60</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>"monitor RDS TA"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Status TP</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.12.0</snmp_oid>
          <key>mntrRDS.RDS.TP</key>
          <delay>60</delay>
          <description>"monitor RDS TP"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Status RDS Sync</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.1.0</snmp_oid>
          <key>mntrRDS.Sync</key>
          <delay>60</delay>
          <description>.1.3.6.1.4.1.35833.14.3.2</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>BOOL_TO_DECIMAL</type>
              <params></params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>Status TA EON Input</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.3.0</snmp_oid>
          <key>mntrRDS.TA.EON.Input</key>
          <delay>60</delay>
          <description>"TA EON Input State"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
          <valuemap>
            <name>NA-Off-On</name>
          </valuemap>
        </item>
        <item>
          <name>Status TA Input</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.35833.14.3.2.0</snmp_oid>
          <key>mntrRDS.TA.Input</key>
          <delay>60</delay>
          <description>"TA Input State"</description>
          <applications>
            <application>
              <name>RDS Monitor</name>
            </application>
          </applications>
          <valuemap>
            <name>NA-Off-On</name>
          </valuemap>
        </item>
      </items>
    </template>
  </templates>
  <graphs>
    <graph>
      <name>Device temperature</name>
      <graph_items>
        <graph_item>
          <drawtype>FILLED_REGION</drawtype>
          <color>1A7C11</color>
          <item>
            <host>DEVA SmartGen 6</host>
            <key>device.Temperature</key>
          </item>
        </graph_item>
      </graph_items>
    </graph>
    <graph>
      <name>RDS Level</name>
      <graph_items>
        <graph_item>
          <drawtype>GRADIENT_LINE</drawtype>
          <color>FFA500</color>
          <item>
            <host>DEVA SmartGen 6</host>
            <key>mntrRDS.RDS.Level</key>
          </item>
        </graph_item>
      </graph_items>
    </graph>
  </graphs>
  <value_maps>
    <value_map>
      <name>NA-Off-On</name>
      <mappings>
        <mapping>
          <value>Not Assigned</value>
          <newvalue>0</newvalue>
        </mapping>
        <mapping>
          <value>Off</value>
          <newvalue>1</newvalue>
        </mapping>
        <mapping>
          <value>On</value>
          <newvalue>2</newvalue>
        </mapping>
      </mappings>
    </value_map>
  </value_maps>
</zabbix_export>