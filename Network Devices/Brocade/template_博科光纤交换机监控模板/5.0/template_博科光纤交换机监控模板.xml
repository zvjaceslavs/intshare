<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-19T21:25:29Z</date>
  <groups>
    <group>
      <name>Templates/DiskArray</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>monitor_SANSwitch_Brocade</template>
      <name>博科光纤交换机监控模板</name>
      <groups>
        <group>
          <name>Templates/DiskArray</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>CPU情况</name>
        </application>
        <application>
          <name>ping检测</name>
        </application>
        <application>
          <name>传感器情况</name>
        </application>
        <application>
          <name>内存情况</name>
        </application>
        <application>
          <name>电源情况</name>
        </application>
        <application>
          <name>端口状态</name>
        </application>
        <application>
          <name>系统信息</name>
        </application>
      </applications>
      <items>
        <item>
          <name>CPU利用率</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1588.2.1.1.1.26.1.0</snmp_oid>
          <key>CPUUsage</key>
          <delay>5m</delay>
          <history>30d</history>
          <trends>60d</trends>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <description>MIB: SW-MIB System's CPU usage.</description>
          <applications>
            <application>
              <name>CPU情况</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{min(#2)}>{$CPU_USAGE_THRESHOLD}</expression>
              <name>CPU利用率高</name>
              <priority>HIGH</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>固件版本</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1588.2.1.1.1.1.6.0</snmp_oid>
          <key>FirmwareVersion</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ping丢包率</name>
          <type>SIMPLE</type>
          <key>icmppingloss[,10,,,]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(10m)}>{$ICMP_LOSS_WARN}</expression>
              <name>ICMP ping 丢包</name>
              <priority>AVERAGE</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_SANSwitch_Brocade:icmpping[,5,,,].last(#1)}=0 and {monitor_SANSwitch_Brocade:icmpping[,5,,,].last(#2)}=1</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ping时延</name>
          <type>SIMPLE</type>
          <key>icmppingsec[,10,,,,avg]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{min(10m)}>{$ICMP_RESPONSE_TIME_WARN}</expression>
              <name>ICMP ping 时延高</name>
              <priority>AVERAGE</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_SANSwitch_Brocade:icmpping[,5,,,].last(#1)}=0 and {monitor_SANSwitch_Brocade:icmpping[,5,,,].last(#2)}=1</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ICMP ping</name>
          <type>SIMPLE</type>
          <key>icmpping[,5,,,]</key>
          <history>7d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}=0 and {last(#2)}=1</expression>
              <name>ICMP ping不可达</name>
              <priority>HIGH</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{last(#1)}=0</expression>
              <name>监控失效（ping不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>ping不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>内存利用率</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1588.2.1.1.1.26.6.0</snmp_oid>
          <key>MemoryUsage</key>
          <delay>5m</delay>
          <history>30d</history>
          <trends>60d</trends>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <description>MIB: SW-MIB Memory utilization in %</description>
          <applications>
            <application>
              <name>内存情况</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{min(#2)}>{$MEMORY_USAGE_THRESHOLD}</expression>
              <name>内存利用率高</name>
              <priority>HIGH</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>系统信息</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.1.0</snmp_oid>
          <key>SystemDescription</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{nodata(12h)}=1</expression>
              <name>监控失效（snmp不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>snmp不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>运行时间</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.3.0</snmp_oid>
          <key>SystemUpTime</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>端口发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#IFNAME},1.3.6.1.4.1.1588.2.1.1.1.6.2.1.37,{#IFDESC},1.3.6.1.4.1.1588.2.1.1.1.6.2.1.36,{#IFWWN},1.3.6.1.4.1.1588.2.1.1.1.6.2.1.34]</snmp_oid>
          <key>BrocadeInterfaceDiscovery_status</key>
          <delay>12h</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>端口:{#IFNAME}({#IFDESC},{#IFWWN})状态</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1588.2.1.1.1.6.2.1.4.{#SNMPINDEX}</snmp_oid>
              <key>BrocadeInterfaceOperationStatus.[{#SNMPINDEX},{#IFNAME}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <description>unknow即没插 (0)，online（1），offline（2），testing（3），fault故障（4）</description>
              <applications>
                <application>
                  <name>端口状态</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#2)}=1 and {last(#1)}=2</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{last(#2)}=1 and {last(#1)}=1</recovery_expression>
                  <name>端口:{#IFNAME} down</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFDESC}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{last(#1)}=4</expression>
                  <name>端口:{#IFNAME} faulty</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>传感器发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.1588.2.1.1.1.1.22.1.5]</snmp_oid>
          <key>SensorDiscovery</key>
          <delay>12h</delay>
          <lifetime>0d</lifetime>
          <description>temp（1），fan（2），power（3） unknown 1，faulty 2，below-min 3，nominal 4，above-max 5，absent 6</description>
          <item_prototypes>
            <item_prototype>
              <name>传感器状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1588.2.1.1.1.1.22.1.3.{#SNMPINDEX}</snmp_oid>
              <key>SensorStatus.[{#SNMPINDEX}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <description>MIB: SW-MIB</description>
              <applications>
                <application>
                  <name>传感器情况</name>
                </application>
              </applications>
              <valuemap>
                <name>SW-MIB::swSensorStatus</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}=2</expression>
                  <name>传感器故障</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{last(#1)}=3</expression>
                  <name>传感器数值低于下限</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{last(#1)}=5</expression>
                  <name>传感器数值高于上限</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
      <macros>
        <macro>
          <macro>{$CLOUD_SNMP_COMMUNITY}</macro>
          <value>public</value>
        </macro>
        <macro>
          <macro>{$CPU_USAGE_THRESHOLD}</macro>
          <value>90</value>
        </macro>
        <macro>
          <macro>{$ICMP_LOSS_WARN}</macro>
          <value>30</value>
        </macro>
        <macro>
          <macro>{$ICMP_RESPONSE_TIME_WARN}</macro>
          <value>0.1</value>
        </macro>
        <macro>
          <macro>{$MEMORY_USAGE_THRESHOLD}</macro>
          <value>90</value>
        </macro>
      </macros>
    </template>
  </templates>
  <value_maps>
    <value_map>
      <name>SW-MIB::swSensorStatus</name>
      <mappings>
        <mapping>
          <value>1</value>
          <newvalue>unknown</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>faulty</newvalue>
        </mapping>
        <mapping>
          <value>3</value>
          <newvalue>below-min</newvalue>
        </mapping>
        <mapping>
          <value>4</value>
          <newvalue>nominal</newvalue>
        </mapping>
        <mapping>
          <value>5</value>
          <newvalue>above-max</newvalue>
        </mapping>
        <mapping>
          <value>6</value>
          <newvalue>absent</newvalue>
        </mapping>
      </mappings>
    </value_map>
  </value_maps>
</zabbix_export>