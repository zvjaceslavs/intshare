<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-19T20:41:18Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template SNMP Furukawa OLT-ONU G4S - G8S - G2500 - G1040 - LD3032</template>
      <name>Template SNMP Furukawa OLT-ONU G4S - G8S - G2500 - G1040 - LD3032</name>
      <description>Created by Samuel Martins (https://www.linkedin.com/in/slmartins/) Last update: 2021-03-08 Based on Furukawa user's guide - NT-GPONREV00–Março2019 https://github.com/slunart Compatbile with: OLT G4S Firmware: 6.11 OLT G8S Firmware: 1.12 OLT G2500 Firmware: 6.12 OLT G1040 OLT LD3032 Firmware: 2.02</description>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>DEVICE INFO</name>
        </application>
        <application>
          <name>DEVICE STATUS MONITORING</name>
        </application>
        <application>
          <name>DEVICE STATUS TROUBLESHOOTING</name>
        </application>
        <application>
          <name>DEVICE UPGRADE</name>
        </application>
      </applications>
      <discovery_rules>
        <discovery_rule>
          <name>ONU Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.10428.9.1.101.23.3.1.1.4]</snmp_oid>
          <key>fkGponOnu</key>
          <delay>1h</delay>
          <item_prototypes>
            <item_prototype>
              <name>ONU Deactive Reason [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.45.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuDeactiveReason[{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <description>NTEGER {none(1), dgi(2), lofi(4), adminReset(10), adminOltConfiguration(12), adminTxOffOptic(18), adminDeactivate(19), adminOltDeactivate(20), adminRemoveOnu(23), los(100), onuDeactReasonRanging(101)}*</description>
              <applications>
                <application>
                  <name>DEVICE STATUS TROUBLESHOOTING</name>
                </application>
              </applications>
              <valuemap>
                <name>ONU Deactive Reason</name>
              </valuemap>
            </item_prototype>
            <item_prototype>
              <name>ONU Description [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.18.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuDescription[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>Returns the description registered at ONT</description>
              <applications>
                <application>
                  <name>DEVICE INFO</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>ONU Distance [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.10.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuDistance[{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <units>meters</units>
              <description>Returns the distance of the fiber in meters from OLT to ONT</description>
              <applications>
                <application>
                  <name>DEVICE STATUS MONITORING</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>ONU Mac Address [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.11.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuHwAddress[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>Returns the MAC of the ONT</description>
              <applications>
                <application>
                  <name>DEVICE INFO</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>ONU Link UpTime [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.61.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuLinkUpTime[{#SNMPINDEX}]</key>
              <units>s</units>
              <description>Returns the time in seconds since the last ONT activation</description>
              <applications>
                <application>
                  <name>DEVICE STATUS MONITORING</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>ONU Model Name [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.17.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuModelName[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>Returns the ONT model</description>
              <applications>
                <application>
                  <name>DEVICE INFO</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>ONU Firmware Active Version [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.12.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuNosActiveVersion[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>Returns the active firmware version in the ONT</description>
              <applications>
                <application>
                  <name>DEVICE INFO</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>ONU Nos Upgrade Status [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.14.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuNosUpgradeStatus[{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <applications>
                <application>
                  <name>DEVICE UPGRADE</name>
                </application>
              </applications>
              <valuemap>
                <name>ONU Nos Upgrade Status</name>
              </valuemap>
              <preprocessing>
                <step>
                  <type>MULTIPLIER</type>
                  <params>0.1</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>ONU Profile [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.8.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuProfile[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>Returns the name of the ONU profile associated with the ONT</description>
              <applications>
                <application>
                  <name>DEVICE INFO</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>ONU Rx Power [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.16.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuRxPower[{#SNMPINDEX}]</key>
              <value_type>FLOAT</value_type>
              <units>dBm</units>
              <description>Returns the optical power Rx of the ONT in deca dBm (101dBm). Limit values from -8.0 to -28.0dBm and it is recommended to define monitoring thresholds of -11.0 and -25.0dBm</description>
              <applications>
                <application>
                  <name>DEVICE STATUS MONITORING</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>MULTIPLIER</type>
                  <params>0.1</params>
                </step>
              </preprocessing>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>-11 or {last()}&lt;-25</expression>
                  <name>ONU [{#SNMPINDEX}] Rx Optical Power Outside Recommended Standards</name>
                  <priority>AVERAGE</priority>
                  <description>Valores limitesde -8,0 a -28,0dBm sendo recomendada a definição de thresholds de monitoração de -11,0 e -25,0dBm</description>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>ONU Serial [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.4.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuSerial[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>Returns the Serial Number of the ONT</description>
              <applications>
                <application>
                  <name>DEVICE INFO</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>ONU Status [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.2.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuStatus[{#SNMPINDEX}]</key>
              <description>Returns the ONT activation state</description>
              <applications>
                <application>
                  <name>DEVICE STATUS MONITORING</name>
                </application>
              </applications>
              <valuemap>
                <name>ONU Status</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#3)}=1</expression>
                  <name>ONU [OltId.OnuId]: [{#SNMPINDEX}] is Inactive</name>
                  <priority>HIGH</priority>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>ONU SysUpTime [OltId.OnuId]: [{#SNMPINDEX}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.10428.9.1.101.23.3.1.1.61.{#SNMPINDEX}</snmp_oid>
              <key>fkGponOnuSysUpTime[{#SNMPINDEX}]</key>
              <units>s</units>
              <description>Returns the time in seconds since the last reset of the ONT</description>
              <applications>
                <application>
                  <name>DEVICE STATUS MONITORING</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
