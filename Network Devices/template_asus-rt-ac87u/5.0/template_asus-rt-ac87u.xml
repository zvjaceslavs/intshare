          <type>DEPENDENT</type>
          <key>cpu.nic</key>
          <delay>0</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>Resource</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)?% nic \0</params>
            </step>
            <step>
