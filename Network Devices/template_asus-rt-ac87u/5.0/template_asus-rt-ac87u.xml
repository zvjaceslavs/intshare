          <applications>
            <application>
              <name>Resource</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)?% io \0</params>
            </step>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)? \0</params>
            </step>
          </preprocessing>
          <master_item>
            <key>ssh.run[top]</key>
          </master_item>
        </item>
