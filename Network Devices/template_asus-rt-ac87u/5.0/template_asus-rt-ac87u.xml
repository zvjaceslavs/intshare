          <delay>0</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>Resource</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)?% irq \0</params>
            </step>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)? \0</params>
            </step>
          </preprocessing>
          <master_item>
            <key>ssh.run[top]</key>
          </master_item>
        </item>
        <item>
