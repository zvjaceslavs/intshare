              <params>\d+(?:\.\d+)? \0</params>
            </step>
          </preprocessing>
          <master_item>
            <key>ssh.run[top]</key>
          </master_item>
        </item>
        <item>
          <name>CPU(sirq)</name>
          <type>DEPENDENT</type>
          <key>cpu.sirq</key>
          <delay>0</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>Resource</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
