        </item>
        <item>
          <name>CPU(sys)</name>
          <type>DEPENDENT</type>
          <key>cpu.sys</key>
          <delay>0</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>Resource</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)?% sys \0</params>
            </step>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)? \0</params>
            </step>
          </preprocessing>
          <master_item>
            <key>ssh.run[top]</key>
          </master_item>
        </item>
        <item>
          <name>CPU(usr)</name>
          <type>DEPENDENT</type>
          <key>cpu.usr</key>
          <delay>0</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>Resource</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)?% usr \0</params>
            </step>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)? \0</params>
            </step>
          </preprocessing>
          <master_item>
            <key>ssh.run[top]</key>
          </master_item>
        </item>
        <item>
          <name>google ping</name>
          <type>SSH</type>
          <key>ssh.run[ping_to_google]</key>
          <delay>10s</delay>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <params>ping google.com -c1</params>
          <username>{$SSH_USER}</username>
          <password>{$SSH_PASS}</password>
          <applications>
            <application>
              <name>Network</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>time=\d+(?:\.\d+)? \0</params>
            </step>
            <step>
              <type>REGEX</type>
              <params>\d+(?:\.\d+)? \0</params>
            </step>
            <step>
              <type>MULTIPLIER</type>
              <params>0.001</params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>top</name>
          <type>SSH</type>
          <key>ssh.run[top]</key>
          <delay>10s</delay>
          <history>0</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <params>top -b -n1</params>
          <username>{$SSH_USER}</username>
          <password>{$SSH_PASS}</password>
          <applications>
            <application>
              <name>Resource</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>CPU:.* \0</params>
            </step>
          </preprocessing>
        </item>
      </items>
      <macros>
        <macro>
          <macro>{$SSH_PASS}</macro>
          <type>SECRET_TEXT</type>
        </macro>
        <macro>
          <macro>{$SSH_USER}</macro>
        </macro>
      </macros>
    </template>
