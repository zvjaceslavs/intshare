      <items>
        <item>
          <name>ping丢包率</name>
          <type>SIMPLE</type>
          <key>icmppingloss[,10,,,]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(10m)}>{$ICMP_LOSS_WARN}</expression>
              <name>ICMP ping 丢包</name>
              <priority>AVERAGE</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_Firewall_Juniper_SRX:icmpping[,5,,,].last(#1)}=0 and {monitor_Firewall_Juniper_SRX:icmpping[,5,,,].last(#2)}=1</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ping时延</name>
          <type>SIMPLE</type>
          <key>icmppingsec[,10,,,,avg]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{min(10m)}>{$ICMP_RESPONSE_TIME_WARN}</expression>
              <name>ICMP ping 时延高</name>
              <priority>AVERAGE</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_Firewall_Juniper_SRX:icmpping[,5,,,].last(#1)}=0 and {monitor_Firewall_Juniper_SRX:icmpping[,5,,,].last(#2)}=1</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ICMP ping</name>
          <type>SIMPLE</type>
          <key>icmpping[,5,,,]</key>
          <history>7d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}=0 and {last(#2)}=1</expression>
              <name>ICMP ping不可达</name>
              <priority>HIGH</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{last(#1)}=0</expression>
              <name>监控失效（ping不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>ping不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
