              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{min(#2)}>{$CPU_USAGE_THRESHOLD}</expression>
                  <name>CPU利用率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>node{#SNMPVALUE} {#SNMPINDEX}号FPC板卡flow会话</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2636.3.39.1.12.1.1.1.6.{#SNMPINDEX}</snmp_oid>
              <key>FlowSession.[{#SNMPVALUE},{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <units>个</units>
              <applications>
                <application>
                  <name>flow会话数</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>node{#SNMPVALUE} {#SNMPINDEX}号FPC板卡flow会话利用率</name>
              <type>CALCULATED</type>
              <key>FlowSessionUsage.[{#SNMPVALUE},{#SNMPINDEX}]</key>
