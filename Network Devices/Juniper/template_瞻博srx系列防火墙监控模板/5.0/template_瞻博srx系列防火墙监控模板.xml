              <applications>
                <application>
                  <name>CP会话数</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>node{#SNMPVALUE} {#SNMPINDEX}号FPC板卡最大flow会话</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2636.3.39.1.12.1.1.1.7.{#SNMPINDEX}</snmp_oid>
              <key>MaxFlowSession.[{#SNMPVALUE},{#SNMPINDEX}]</key>
              <delay>12h</delay>
              <history>3d</history>
              <trends>0d</trends>
              <units>个</units>
              <applications>
                <application>
                  <name>flow会话数</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>node{#SNMPVALUE} {#SNMPINDEX}号FPC板卡内存利用率</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2636.3.39.1.12.1.1.1.5.{#SNMPINDEX}</snmp_oid>
              <key>MemUsage.[{#SNMPVALUE},{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <units>%</units>
              <applications>
                <application>
                  <name>内存利用率</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{min(#2)}>{$MEMORY_USAGE_THRESHOLD}</expression>
                  <name>内存利用率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>端口发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#IFDESCR},IF-MIB::ifDescr,{#IFALIAS},IF-MIB::ifAlias]</snmp_oid>
          <key>InterfaceDiscovery_status</key>
          <delay>12h</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*Vlan.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <description>You may also consider using IF-MIB::ifType or IF-MIB::ifAlias for discovery depending on your filtering needs. {$CLOUD_SNMP_COMMUNITY} is a global macro.</description>
          <item_prototypes>
            <item_prototype>
              <name>端口:{#IFDESCR}({#IFALIAS})状态</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifOperStatus.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceOperationStatus.[{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <description>The current operational state of the interface.</description>
              <applications>
                <application>
                  <name>端口状态</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#2)}=1 and {last(#1)}=2</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{last(#2)}=1 and {last(#1)}=1</recovery_expression>
                  <name>端口 {#IFDESCR} down</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>端口发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#IFDESCR},IF-MIB::ifDescr,{#IFSTATUS},IF-MIB::ifOperStatus,{#IFALIAS},IF-MIB::ifAlias]</snmp_oid>
          <key>InterfaceDiscovery_traffic</key>
          <delay>12h</delay>
          <filter>
            <evaltype>AND</evaltype>
            <conditions>
              <condition>
                <macro>{#IFSTATUS}</macro>
                <value>1</value>
                <formulaid>E</formulaid>
              </condition>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*Vlan.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>B</formulaid>
              </condition>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*Loop.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>A</formulaid>
              </condition>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*VLAN.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>C</formulaid>
              </condition>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*NULL.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>D</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>端口带宽 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.15.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceBandwidth.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>12h</delay>
              <history>3d</history>
              <trends>0d</trends>
              <units>Mbps</units>
              <applications>
                <application>
                  <name>端口带宽</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>入方向带宽利用率 {#IFDESCR}({#IFALIAS})</name>
              <type>CALCULATED</type>
              <key>InterfaceInBandwidthUsage.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <value_type>FLOAT</value_type>
              <units>%</units>
              <params>last("InterfaceInTraffic.[{#SNMPINDEX},{#IFDESCR}]")/last("InterfaceBandwidth.[{#SNMPINDEX},{#IFDESCR}]")/10000</params>
              <applications>
                <application>
                  <name>入方向带宽利用率</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>{$BANDWIDTH_USAGE_IN_THRESHOLD}</expression>
                  <name>端口 {#IFDESCR} 入方向带宽利用率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>入方向广播报文速率 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.9.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceInBroadcastPacketsTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <units>pps</units>
              <applications>
                <application>
                  <name>入方向广播包流量</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>入方向组播报文速率 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.9.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceInMulticastPacketsTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>入方向组播报文速率</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>入方向错包 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.14.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceInPacketError.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>30m</delay>
              <history>7d</history>
              <trends>0d</trends>
              <units>pps</units>
              <applications>
                <application>
                  <name>入方向错包</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>{$ERROR_PACKETS_IN_THRESHOLD}</expression>
                  <name>端口 {#IFDESCR} 入方向错包率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
