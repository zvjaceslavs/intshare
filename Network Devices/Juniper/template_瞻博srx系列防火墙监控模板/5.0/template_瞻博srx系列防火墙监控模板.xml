              <units>%</units>
              <params>last("FlowSession.[{#SNMPVALUE},{#SNMPINDEX}]")/last("MaxFlowSession.[{#SNMPVALUE},{#SNMPINDEX}]")*100</params>
              <applications>
                <application>
                  <name>flow会话利用率</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{min(#2)}>{$FLOW_SESSION_USAGE_THRESHOLD}</expression>
                  <name>flow会话利用率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>node{#SNMPVALUE} {#SNMPINDEX}号FPC板卡最大CP会话</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2636.3.39.1.12.1.1.1.9.{#SNMPINDEX}</snmp_oid>
              <key>MaxCPSession.[{#SNMPVALUE},{#SNMPINDEX}]</key>
              <delay>12h</delay>
              <history>3d</history>
              <trends>0d</trends>
