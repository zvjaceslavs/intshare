      <discovery_rules>
        <discovery_rule>
          <name>FPC板卡发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.2636.3.39.1.12.1.1.1.10]</snmp_oid>
          <key>FPCBoardDiscovery</key>
          <delay>12h</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>node{#SNMPVALUE} {#SNMPINDEX}号FPC板卡CP会话</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2636.3.39.1.12.1.1.1.8.{#SNMPINDEX}</snmp_oid>
              <key>CPSession.[{#SNMPVALUE},{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <units>个</units>
              <applications>
                <application>
                  <name>CP会话数</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>node{#SNMPVALUE} {#SNMPINDEX}号FPC板卡CP会话利用率</name>
              <type>CALCULATED</type>
              <key>CPSessionUsage.[{#SNMPVALUE},{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <value_type>FLOAT</value_type>
              <units>%</units>
              <params>last("CPSession.[{#SNMPVALUE},{#SNMPINDEX}]")/last("MaxCPSession.[{#SNMPVALUE},{#SNMPINDEX}]")*100</params>
              <applications>
                <application>
                  <name>CP会话利用率</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{min(#2)}>{$CP_SESSION_USAGE_THRESHOLD}</expression>
                  <name>CP会话利用率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>node{#SNMPVALUE} {#SNMPINDEX}号FPC板卡CPU利用率</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2636.3.39.1.12.1.1.1.4.{#SNMPINDEX}</snmp_oid>
              <key>CPUUsage.[{#SNMPVALUE},{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <units>%</units>
              <applications>
