<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T07:57:49Z</date>
  <groups>
    <group>
      <name>Networking Hardware</name>
    </group>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template Juniper EX</template>
      <name>Template Juniper EX</name>
      <groups>
        <group>
          <name>Networking Hardware</name>
        </group>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Device Health</name>
        </application>
        <application>
          <name>Interfaces</name>
        </application>
        <application>
          <name>Performance</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Ping check</name>
          <type>SIMPLE</type>
          <key>icmpping</key>
          <delay>30</delay>
          <applications>
            <application>
              <name>Device Health</name>
            </application>
          </applications>
          <valuemap>
            <name>Service state</name>
          </valuemap>
          <triggers>
            <trigger>
              <expression>{sum(#3)}=0 or ({TRIGGER.VALUE}=1 and {sum(#3)}&lt;>3)</expression>
              <name>Ping check</name>
              <priority>DISASTER</priority>
              <description>The switch has stopped responding to ICMP echo requests, and is likely offline. Please investigate.</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Red alarm</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.2636.3.4.2.3.1.0</snmp_oid>
          <key>RedAlarm</key>
          <delay>30</delay>
          <units>alarm(s)</units>
          <applications>
            <application>
              <name>Device Health</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0)}&lt;>2</expression>
              <name>Red Alarm on {HOSTNAME}</name>
              <priority>HIGH</priority>
              <description>A red alarm is reporting on this device. Please login to the CLI and check what this alert has been caused by.</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.3.0</snmp_oid>
          <key>Uptime</key>
          <delay>60</delay>
          <units>s</units>
          <applications>
            <application>
              <name>Device Health</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.01</params>
            </step>
          </preprocessing>
          <triggers>
            <trigger>
              <expression>{last(0)}&lt;600</expression>
              <name>{HOSTNAME} has just been restarted</name>
              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Yellow alarm</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.2636.3.4.2.2.1.0</snmp_oid>
          <key>YellowAlarm</key>
          <delay>60</delay>
          <units>alarm(s)</units>
          <applications>
            <application>
              <name>Device Health</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0)}&lt;>2</expression>
              <name>Yellow Alarm on {HOSTNAME}</name>
              <priority>AVERAGE</priority>
              <description>A yellow alarm is reporting on this device. Please login to the CLI and check what this alert has been caused by.</description>
            </trigger>
          </triggers>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>JunOS Interfaces</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.2.1.2.2.1.2]</snmp_oid>
          <key>discovery.ifDescr</key>
          <delay>300</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#SNMPVALUE}</macro>
                <value>^(fe|ge|xe)-[0-9]+\/[0-9]+\/[0-9]+\.[0-9]+$|^(fe|ge|xe)-[0-9]+\/[0-9]+\/[0-9]+$|^(ae|reth|st)[0-9]+\.[0-9]+$|^(ae|reth|st)[0-9]$|^vcp-[0-9]+$|^vlan.[0-9]+$</value>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <item_prototypes>
            <item_prototype>
              <name>{#SNMPVALUE} - Inbound discarded packets</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.2.1.2.2.1.13.{#SNMPINDEX}</snmp_oid>
              <key>.1.3.6.1.2.1.2.2.1.13.[{#SNMPINDEX}]</key>
              <delay>30</delay>
              <history>7d</history>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>{#SNMPVALUE} - Inbound traffic</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.10.{#SNMPINDEX}</snmp_oid>
              <key>1.3.6.1.2.1.2.2.1.10.[{#SNMPINDEX}]</key>
              <delay>30</delay>
              <history>7d</history>
              <value_type>FLOAT</value_type>
              <units>bps</units>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
                <step>
                  <type>MULTIPLIER</type>
                  <params>8</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>{#SNMPVALUE} - Inbound errors</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.14.{#SNMPINDEX}</snmp_oid>
              <key>1.3.6.1.2.1.2.2.1.14.[{#SNMPINDEX}]</key>
              <delay>30</delay>
              <history>7d</history>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>{#SNMPVALUE} - Outbound traffic</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.16.{#SNMPINDEX}</snmp_oid>
              <key>1.3.6.1.2.1.2.2.1.16.[{#SNMPINDEX}]</key>
              <delay>30</delay>
              <history>7d</history>
              <value_type>FLOAT</value_type>
              <units>bps</units>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
                <step>
                  <type>MULTIPLIER</type>
                  <params>8</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>{#SNMPVALUE} - Outbound discarded packets</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.19.{#SNMPINDEX}</snmp_oid>
              <key>1.3.6.1.2.1.2.2.1.19.[{#SNMPINDEX}]</key>
              <delay>30</delay>
              <history>7d</history>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>{#SNMPVALUE} - Outbound errors</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.20.{#SNMPINDEX}</snmp_oid>
              <key>1.3.6.1.2.1.2.2.1.20.[{#SNMPINDEX}]</key>
              <delay>30</delay>
              <history>7d</history>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
          </item_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>Interface {#SNMPVALUE}</name>
              <graph_items>
                <graph_item>
                  <drawtype>FILLED_REGION</drawtype>
                  <color>2618B1</color>
                  <yaxisside>RIGHT</yaxisside>
                  <item>
                    <host>Template Juniper EX</host>
                    <key>1.3.6.1.2.1.2.2.1.10.[{#SNMPINDEX}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>1</sortorder>
                  <color>594CD8</color>
                  <yaxisside>RIGHT</yaxisside>
                  <item>
                    <host>Template Juniper EX</host>
                    <key>.1.3.6.1.2.1.2.2.1.13.[{#SNMPINDEX}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>2</sortorder>
                  <color>7B72D8</color>
                  <yaxisside>RIGHT</yaxisside>
                  <item>
                    <host>Template Juniper EX</host>
                    <key>1.3.6.1.2.1.2.2.1.14.[{#SNMPINDEX}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>3</sortorder>
                  <drawtype>BOLD_LINE</drawtype>
                  <color>FFC600</color>
                  <yaxisside>RIGHT</yaxisside>
                  <item>
                    <host>Template Juniper EX</host>
                    <key>1.3.6.1.2.1.2.2.1.16.[{#SNMPINDEX}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>4</sortorder>
                  <color>FFD440</color>
                  <yaxisside>RIGHT</yaxisside>
                  <item>
                    <host>Template Juniper EX</host>
                    <key>1.3.6.1.2.1.2.2.1.19.[{#SNMPINDEX}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>5</sortorder>
                  <color>FFE073</color>
                  <yaxisside>RIGHT</yaxisside>
                  <item>
                    <host>Template Juniper EX</host>
                    <key>1.3.6.1.2.1.2.2.1.20.[{#SNMPINDEX}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>JunOS Hardware</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.2636.3.1.14.1.2.9]</snmp_oid>
          <key>jnxRedundancyL1Index</key>
          <delay>300</delay>
          <item_prototypes>
            <item_prototype>
              <name>Switch {#SNMPVALUE} - 5 min load avarage</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.2636.3.1.13.1.21.9.{#SNMPVALUE}.0.0</snmp_oid>
              <key>jnxOperating5MinLoadAvg.[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>7d</history>
              <applications>
                <application>
                  <name>Device Health</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Switch {#SNMPVALUE} - Memory usage</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.2636.3.1.13.1.11.9.{#SNMPVALUE}.0.0</snmp_oid>
              <key>jnxOperatingBuffer.[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>7d</history>
              <units>%</units>
              <applications>
                <application>
                  <name>Device Health</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(0)}>70 and {last(0)}&lt;95</expression>
                  <name>{HOSTNAME} {#SNMPVALUE} - Memory usage above 70%</name>
                  <priority>AVERAGE</priority>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{last(0)}>95</expression>
                  <name>{HOSTNAME} {#SNMPVALUE} - Memory usage above 95%</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Switch {#SNMPVALUE} - CPU usage</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.2636.3.1.13.1.8.9.{#SNMPVALUE}.0.0</snmp_oid>
              <key>jnxOperatingCPU.[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>7d</history>
              <units>%</units>
              <applications>
                <application>
                  <name>Device Health</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Switch {#SNMPVALUE} - Routing engine temperature</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.2636.3.1.13.1.7.9.{#SNMPVALUE}.0.0</snmp_oid>
              <key>jnxOperatingTemp.[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>7d</history>
              <units>°C</units>
              <applications>
                <application>
                  <name>Device Health</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{min(60)}>60</expression>
                  <name>{HOSTNAME} {#SNMPVALUE} - High temperature</name>
                  <priority>HIGH</priority>
                  <description>The temperature of this switch is above 60ºC. Please check the status of the fans and environmental conditions immediately.</description>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
          <trigger_prototypes>
            <trigger_prototype>
              <expression>{Template Juniper EX:jnxOperatingCPU.[{#SNMPVALUE}].min(30)}>70 and {Template Juniper EX:jnxOperatingCPU.[{#SNMPVALUE}].min(30)}&lt;95 and {Template Juniper EX:jnxOperating5MinLoadAvg.[{#SNMPVALUE}].last(30)}>100 and {Template Juniper EX:jnxOperating5MinLoadAvg.[{#SNMPVALUE}].last(0)}&lt;300</expression>
              <name>{HOSTNAME} {#SNMPVALUE} - CPU usage above 70%</name>
              <priority>AVERAGE</priority>
              <description>The CPU load on this node has increased to over 70%. If this is a sudden or unexpected increase, immediate investigation is advised.</description>
            </trigger_prototype>
            <trigger_prototype>
              <expression>{Template Juniper EX:jnxOperating5MinLoadAvg.[{#SNMPVALUE}].last(0)}>300 and {Template Juniper EX:jnxOperatingCPU.[{#SNMPVALUE}].last(0)}>95</expression>
              <name>{HOSTNAME} {#SNMPVALUE} - High CPU usage (95%)</name>
              <priority>HIGH</priority>
              <description>The CPU load on this node has increased to over 95%. If this is a sudden or unexpected increase, immediate investigation is advised.</description>
            </trigger_prototype>
          </trigger_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
  <value_maps>
    <value_map>
      <name>Service state</name>
      <mappings>
        <mapping>
          <value>0</value>
          <newvalue>Down</newvalue>
        </mapping>
        <mapping>
          <value>1</value>
          <newvalue>Up</newvalue>
        </mapping>
      </mappings>
    </value_map>
  </value_maps>
</zabbix_export>