        <item>
          <name>Total Memory</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.2021.4.5.0</snmp_oid>
          <key>memTotalReal</key>
          <delay>1h</delay>
          <applications>
