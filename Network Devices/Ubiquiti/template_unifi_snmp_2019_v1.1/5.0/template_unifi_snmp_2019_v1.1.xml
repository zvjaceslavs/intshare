              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
              <request_method>POST</request_method>
            </item_prototype>
            <item_prototype>
              <name>Traffic Incoming Errors per Second $2 on $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.13.{#SNMPINDEX}</snmp_oid>
