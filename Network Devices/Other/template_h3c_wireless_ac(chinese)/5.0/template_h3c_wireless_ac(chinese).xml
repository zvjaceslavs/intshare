      <discovery_rules>
        <discovery_rule>
          <name>AP Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#APNAME}, 1.3.6.1.4.1.2011.10.2.75.2.1.1.1.5]</snmp_oid>
          <key>AP</key>
          <delay>1200</delay>
          <lifetime>7d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>AP $1 关联认证在线用户数</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2011.10.2.75.2.2.29.1.3.{#SNMPINDEX}</snmp_oid>
              <key>ap.asso[{#APNAME}]</key>
              <delay>120</delay>
              <history>7d</history>
              <status>DISABLED</status>
              <applications>
                <application>
                  <name>AP</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>AP $1 免认证在线用户数</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2011.10.2.75.2.2.29.1.2.{#SNMPINDEX}</snmp_oid>
              <key>ap.free[{#APNAME}]</key>
              <delay>120</delay>
              <history>7d</history>
              <applications>
                <application>
                  <name>AP</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>AP $1 地址</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2011.10.2.75.2.1.1.1.2.{#SNMPINDEX}</snmp_oid>
              <key>ap.ip[{#APNAME}]</key>
              <delay>7200</delay>
              <history>7d</history>
              <trends>0</trends>
              <value_type>CHAR</value_type>
              <applications>
                <application>
                  <name>AP</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>AP $1 mac认证在线用户数</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2011.10.2.75.2.2.29.1.4.{#SNMPINDEX}</snmp_oid>
              <key>ap.mac[{#APNAME}]</key>
              <delay>30</delay>
              <history>7d</history>
              <status>DISABLED</status>
              <applications>
                <application>
                  <name>AP</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>AP $1 在线时间</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2011.10.2.75.2.1.10.1.1.{#SNMPINDEX}</snmp_oid>
              <key>ap.onlinetime[{#APNAME}]</key>
              <delay>120</delay>
              <history>7d</history>
              <value_type>FLOAT</value_type>
              <units>s</units>
              <applications>
                <application>
                  <name>AP</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>MULTIPLIER</type>
                  <params>0.01</params>
                </step>
              </preprocessing>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{max(1d)}=0</expression>
                  <name>AP {#APNAME} 一天不在线</name>
                  <priority>AVERAGE</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>AP $1 portal认证在线用户数</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2011.10.2.75.2.2.29.1.1.{#SNMPINDEX}</snmp_oid>
              <key>ap.portal[{#APNAME}]</key>
              <delay>120</delay>
              <history>7d</history>
              <status>DISABLED</status>
              <applications>
                <application>
                  <name>AP</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>DHCP Server Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#DHCPPOOL}, 1.3.6.1.4.1.2011.10.2.101.2.2.1.1]</snmp_oid>
          <key>dhcpserver</key>
          <delay>1200</delay>
          <lifetime>7d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>DHCP 地址池 $1 利用率</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.2011.10.2.101.2.6.1.1.{#SNMPINDEX}</snmp_oid>
              <key>dhcp.pool[{#DHCPPOOL}]</key>
              <delay>120</delay>
              <history>7d</history>
              <trends>0</trends>
              <units>%</units>
              <applications>
                <application>
                  <name>DHCP</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>{$DHCP_POOL_H}</expression>
                  <name>AC 地址池 {#DHCPPOOL} 快满</name>
                  <priority>HIGH</priority>
                  <description>地址池 {#DHCPPOOL} 使用率超过{$DHCP_POOL_H}%</description>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
      <macros>
        <macro>
          <macro>{$DHCP_POOL_H}</macro>
          <value>95</value>
        </macro>
