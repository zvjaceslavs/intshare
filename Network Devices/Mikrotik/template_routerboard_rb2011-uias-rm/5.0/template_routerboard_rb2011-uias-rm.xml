      <discovery_rules>
        <discovery_rule>
          <name>Network Interfaces Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.2.1.2.2.1.8,{#IFADMINSTATUS},1.3.6.1.2.1.2.2.1.7,{#IFALIAS},1.3.6.1.2.1.31.1.1.1.18,{#IFNAME},1.3.6.1.2.1.31.1.1.1.1,{#IFDESCR},1.3.6.1.2.1.2.2.1.2,{#IFTYPE},1.3.6.1.2.1.2.2.1.3]</snmp_oid>
          <key>net.if.discovery</key>
          <delay>3600</delay>
          <filter>
            <evaltype>AND</evaltype>
            <conditions>
              <condition>
                <macro>{#IFADMINSTATUS}</macro>
                <value>(1|3)</value>
                <formulaid>A</formulaid>
              </condition>
              <condition>
                <macro>{#IFNAME}</macro>
                <value>@Network interfaces for discovery</value>
                <formulaid>B</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>2d</lifetime>
          <description>.</description>
          <item_prototypes>
            <item_prototype>
              <name>Interface {#IFNAME}({#IFALIAS}): Discards In</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.13.{#SNMPINDEX}</snmp_oid>
              <key>net.if.in.discards[ifInDiscards.{#SNMPINDEX}]</key>
              <delay>300s</delay>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Interface {#IFNAME}({#IFALIAS}): Erros In</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.14.{#SNMPINDEX}</snmp_oid>
