                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Interface {#IFNAME}({#IFALIAS}): Bytes In</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.6.{#SNMPINDEX}</snmp_oid>
