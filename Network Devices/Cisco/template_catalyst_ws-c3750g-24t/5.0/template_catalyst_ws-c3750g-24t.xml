        <item>
          <name>Device CPU utilization</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.9.9.109.1.1.1.1.5.1</snmp_oid>
          <key>ciscoCpmCPUTotal5minRev</key>
          <delay>300</delay>
          <trends>30d</trends>
          <units>%</units>
          <description>The time scale is set at 5 minute intervals. The value will be between 0% (idle) and 100% (very busy).</description>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(600)}>50 and {avg(600)}&lt;60</expression>
              <name>CPU usage exceeded 50%</name>
              <priority>WARNING</priority>
            </trigger>
            <trigger>
              <expression>{avg(600)}>60 and {avg(600)}&lt;80</expression>
              <name>CPU usage exceeded 60%</name>
              <priority>AVERAGE</priority>
            </trigger>
            <trigger>
              <expression>{avg(600)}>80</expression>
              <name>CPU usage exceeded 80%</name>
              <priority>HIGH</priority>
