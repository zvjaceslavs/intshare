<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-19T21:19:40Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template SNMP Sonicwall 4600</template>
      <name>Template SNMP Sonicwall 4600</name>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Sonicwall General</name>
        </application>
        <application>
          <name>SonicWall Performance</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Current Connections</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.8741.1.3.1.2.0</snmp_oid>
          <key>sonicwallCurrentConnections</key>
          <delay>60</delay>
          <units>KC</units>
          <applications>
            <application>
              <name>SonicWall Performance</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>CHANGE_PER_SECOND</type>
              <params></params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>Current CPU Util</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.8741.1.3.1.3.0</snmp_oid>
          <key>sonicwallCurrentCPUUtil</key>
          <delay>60</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>SonicWall Performance</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>CHANGE_PER_SECOND</type>
              <params></params>
            </step>
            <step>
              <type>MULTIPLIER</type>
              <params>100</params>
            </step>
          </preprocessing>
          <triggers>
            <trigger>
              <expression>{avg(#5)}>95</expression>
              <name>Sonicwall {HOSTNAME} - Usage of CPU over 95%</name>
              <priority>AVERAGE</priority>
              <description>CPU activity on Sonicwall is more than 95% during the last five checks</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Current RAM Usage</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.8741.1.3.1.4.0</snmp_oid>
          <key>sonicwallCurrentRAMUtil</key>
          <delay>60</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>SonicWall Performance</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(#5)}>100</expression>
              <name>Sonicwall {HOSTNAME} - Memory Usage Over 100%</name>
              <priority>AVERAGE</priority>
              <description>Memory usage on Sonicwall is more than 100% during the last five checks</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Firmware Version</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.8741.2.1.1.3.0</snmp_oid>
          <key>SysmFirmwareVersion</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>OS</inventory_link>
          <applications>
            <application>
              <name>Sonicwall General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Sonicwall Model</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.8741.2.1.1.1.0</snmp_oid>
          <key>SysmModel</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>MODEL</inventory_link>
          <applications>
            <application>
              <name>Sonicwall General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Sonicwall Serial Number</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.8741.2.1.1.2.0</snmp_oid>
          <key>SysmSerialNumber</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <inventory_link>SERIALNO_A</inventory_link>
          <applications>
            <application>
              <name>Sonicwall General</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Interfaces</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},IF-MIB::ifDescr]</snmp_oid>
          <key>sonicwallifDescr</key>
          <delay>90</delay>
          <item_prototypes>
            <item_prototype>
              <name>Incoming Traffic on interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifInOctets.{#SNMPINDEX}</snmp_oid>
              <key>ifInOctets.[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <units>bps</units>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
                <step>
                  <type>MULTIPLIER</type>
                  <params>8</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Outgoing Traffic on interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifOutOctets.{#SNMPINDEX}</snmp_oid>
              <key>ifOutOctets.[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <units>bps</units>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
                <step>
                  <type>MULTIPLIER</type>
                  <params>8</params>
                </step>
              </preprocessing>
            </item_prototype>
          </item_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>Traffic on {#SNMPVALUE}</name>
              <graph_items>
                <graph_item>
                  <drawtype>GRADIENT_LINE</drawtype>
                  <color>1A7C11</color>
                  <item>
                    <host>Template SNMP Sonicwall 4600</host>
                    <key>ifInOctets.[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>1</sortorder>
                  <drawtype>GRADIENT_LINE</drawtype>
                  <color>F63100</color>
                  <item>
                    <host>Template SNMP Sonicwall 4600</host>
                    <key>ifOutOctets.[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
  <graphs>
    <graph>
      <name>CPU %</name>
      <show_work_period>NO</show_work_period>
      <graph_items>
