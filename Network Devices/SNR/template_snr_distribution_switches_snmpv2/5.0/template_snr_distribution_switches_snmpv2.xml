          <item_prototypes>
            <item_prototype>
              <name>Interface {#IFNAME}({#IFALIAS}): Inbound packets discarded</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.13.{#SNMPINDEX}</snmp_oid>
              <key>net.if.in.discards[ifInDiscards.{#SNMPINDEX}]</key>
              <delay>30</delay>
              <history>7d</history>
              <units>pps</units>
              <applications>
