  <groups>
    <group>
      <name>Fortigate</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Fortigate FG_200B</template>
      <name>FG_200B</name>
      <description>## Overview Template for Fortigate 200B Still - WIP! Use at your own risk Items: * Memory * CPU * SN * FW * Up time * Sessions Items Prototypes * Download * Upload * Status * Description Trigger Prototypes * Interface Up &amp; Down Any suggestion will be Welcomed :) ## Author JonatanRek</description>
      <groups>
        <group>
          <name>Fortigate</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>General</name>
        </application>
        <application>
          <name>Interface Status</name>
        </application>
        <application>
          <name>Network Traffic</name>
        </application>
        <application>
          <name>Port Status</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Connections</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.8.0</snmp_oid>
          <key>fortinetCurrentConnections</key>
          <delay>60</delay>
          <units>KC</units>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.20.0</snmp_oid>
          <key>fortinetUpTime</key>
          <delay>30</delay>
          <units>uptime</units>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>CPU</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.3.0</snmp_oid>
          <key>sysCpu</key>
          <units>percent</units>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Memory Usage</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.4.0</snmp_oid>
          <key>sysMemUsage</key>
          <units>percent</units>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Firmware Version</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.12356.101.4.1.1.0</snmp_oid>
          <key>SysmFirmwareVersion</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Serial Number</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.12356.100.1.1.1.0</snmp_oid>
          <key>SysmSerialNumber</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>SERIALNO_A</inventory_link>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>System Sessions</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.12356.1.10.0</snmp_oid>
          <key>sysSessions</key>
          <delay>30s</delay>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Interfaces</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.2.1.31.1.1.1.1]</snmp_oid>
          <key>ifname</key>
          <delay>30s</delay>
          <item_prototypes>
            <item_prototype>
              <name>Status {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.2.1.2.2.1.8.{#SNMPINDEX}</snmp_oid>
              <key>.1.3.6.1.2.1.2.2.1.8.[{#SNMPINDEX}]</key>
              <delay>30s</delay>
              <applications>
                <application>
                  <name>Interface Status</name>
                </application>
                <application>
                  <name>Network Traffic</name>
                </application>
              </applications>
              <valuemap>
                <name>FG_200B</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}=0</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{last()}=1</recovery_expression>
                  <name>Interface {#SNMPVALUE} Down</name>
                  <priority>DISASTER</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Interface Description {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.2.{#SNMPINDEX}</snmp_oid>
              <key>ifDescr[{#SNMPVALUE}]</key>
              <delay>30s</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Interface Status</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Download {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.10.{#SNMPINDEX}</snmp_oid>
              <key>ifInOctets[{#SNMPVALUE}]</key>
              <delay>30s</delay>
              <units>bps</units>
              <applications>
                <application>
                  <name>Network Traffic</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Upload $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.16.{#SNMPINDEX}</snmp_oid>
              <key>ifOutOctets[{#SNMPVALUE}]</key>
              <delay>30s</delay>
              <units>bps</units>
              <applications>
                <application>
                  <name>Network Traffic</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
