<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T10:09:43Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template Discovery Audemat</template>
      <name>Template Discovery Audemat</name>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Information</name>
        </application>
        <application>
          <name>Operacion</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Disponibilidad SNMP</name>
          <type>INTERNAL</type>
          <key>zabbix[host,snmp,available]</key>
          <delay>60s</delay>
          <applications>
            <application>
              <name>Information</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>AudematFM</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#INDEX},.1.3.6.1.4.1.5299.15.18.1.12.4.3.1.1,{#NAME},.1.3.6.1.4.1.5299.15.18.1.12.4.3.1.2]</snmp_oid>
          <key>Audemat.disco</key>
          <item_prototypes>
            <item_prototype>
              <name>Left Audio Level - {#NAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.5299.15.18.1.12.4.11.1.2.1.3.{#INDEX}.2</snmp_oid>
              <key>AudioLeft[{#INDEX}]</key>
              <delay>30s</delay>
              <value_type>FLOAT</value_type>
              <units>dBr*10</units>
              <applications>
                <application>
                  <name>Operacion</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Rigth Audio Level - {#NAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.5299.15.18.1.12.4.11.1.2.1.3.{#INDEX}.3</snmp_oid>
              <key>AudioRigth[{#INDEX}]</key>
              <delay>30s</delay>
              <value_type>FLOAT</value_type>
              <units>dBr*10</units>
              <applications>
                <application>
                  <name>Operacion</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>MPX Level - {#NAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.5299.15.18.1.12.4.11.1.2.1.3.{#INDEX}.1</snmp_oid>
              <key>mpx[{#INDEX}]</key>
              <delay>30s</delay>
              <units>%</units>
              <applications>
                <application>
                  <name>Operacion</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}&lt;40</expression>
                  <name>{#NAME} {HOST.NAME}: Modulacion (MPX) Baja</name>
                  <priority>AVERAGE</priority>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{last()}&lt;15 and {last(2)}&lt;15 and {last(3)}&lt;15 or {last()}&lt;5</expression>
                  <name>{#NAME} {HOST.NAME}: Modulacion (MPX) Demasiado Baja</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Frecuencia Radio {#INDEX}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.5299.15.18.1.12.4.3.1.4.{#INDEX}</snmp_oid>
              <key>RadioF[{#INDEX}]</key>
              <delay>30s</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Information</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Nombre Radio {#INDEX}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.5299.15.18.1.12.4.3.1.2.{#INDEX}</snmp_oid>
              <key>RadioN[{#INDEX}]</key>
              <delay>30s</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Information</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>RF Level - {#NAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.5299.15.18.1.12.4.11.1.2.1.3.{#INDEX}.0</snmp_oid>
              <key>RFLvl[{#INDEX}]</key>
              <delay>30s</delay>
              <units>dBuV*10</units>
              <applications>
                <application>
                  <name>Operacion</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
</zabbix_export>