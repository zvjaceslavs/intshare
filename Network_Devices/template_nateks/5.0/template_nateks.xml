              </applications>
              <valuemap>
                <name>NETRADIO-MIB::ChanOperStatus</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#5)}&lt;>4</expression>
                  <name>RadioChannel [{#CHANNUM}] Status is not connected</name>
                  <priority>WARNING</priority>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
