      <discovery_rules>
        <discovery_rule>
          <name>Netradio discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#CHANNUM},1.3.6.1.4.1.4249.1.31.3.1.1.1, {#CHANIP},1.3.6.1.4.1.4249.1.31.2.4.3.1.3, {#CHANSTATUS},1.3.6.1.4.1.4249.1.31.3.1.1.2]</snmp_oid>
