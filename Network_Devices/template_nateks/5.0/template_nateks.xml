              <snmp_oid>1.3.6.1.4.1.4249.1.31.3.1.1.2.{#SNMPINDEX}</snmp_oid>
              <key>ChanStatus[{#CHANNUM}]</key>
              <delay>60s</delay>
              <trends>180d</trends>
              <applications>
                <application>
                  <name>NETRADIO</name>
