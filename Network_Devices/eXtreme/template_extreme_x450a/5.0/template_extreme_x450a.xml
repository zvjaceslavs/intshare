<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T10:51:09Z</date>
  <groups>
    <group>
      <name>Templates SNMP Devices</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template_Extreme_X450a</template>
      <name>Template_Extreme_X450a</name>
      <groups>
        <group>
          <name>Templates SNMP Devices</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>CPU</name>
        </application>
        <application>
          <name>FAN Trays</name>
        </application>
        <application>
          <name>General</name>
        </application>
        <application>
          <name>Memory</name>
        </application>
        <application>
          <name>Power Supplies</name>
        </application>
        <application>
          <name>Temperature</name>
        </application>
      </applications>
      <items>
        <item>
          <name>CPU Utilization Threshold</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.32.1.5.0</snmp_oid>
          <key>extremeCpuMonitorThreshold</key>
          <delay>600</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeCpuMonitorThreshold Threshold for CPU Aggregation utilization trap</description>
          <applications>
            <application>
              <name>CPU</name>
            </application>
          </applications>
        </item>
        <item>
          <name>CPU Utilization</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.32.1.2.0</snmp_oid>
          <key>extremeCpuMonitorTotalUtilization</key>
          <delay>60</delay>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeCpuMonitorTotalUtilization Total CPU utlization (percentage) as of last sampling.</description>
          <applications>
            <application>
              <name>CPU</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Device Temperature</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.8.0</snmp_oid>
          <key>extremeCurrentTemperature</key>
          <delay>300</delay>
          <value_type>FLOAT</value_type>
          <units>Â°C</units>
          <description>EXTREME-SYSTEM-MIB::extremeCurrentTemperature Current temperature in degrees celcius measured inside device enclosure.</description>
          <applications>
            <application>
              <name>Temperature</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Over Temperature Status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.7.0</snmp_oid>
          <key>extremeOverTemperatureAlarm</key>
          <delay>60</delay>
          <description>EXTREME-SYSTEM-MIB::extremeOverTemperatureAlarm Alarm status of overtemperature sensor in device enclosure.</description>
          <applications>
            <application>
              <name>Temperature</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}=1</expression>
              <name>Over Temperature in device Enclosure</name>
              <priority>AVERAGE</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Operational Status of Primary Power Supply</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.10.0</snmp_oid>
          <key>extremePrimaryPowerOperational</key>
          <delay>60</delay>
          <description>EXTREME-SYSTEM-MIB::extremePrimaryPowerOperational Operational status of internal power supply of a device.</description>
          <applications>
            <application>
              <name>Power Supplies</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}=2</expression>
              <name>Primary Power Supply is nonoperational</name>
              <priority>AVERAGE</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Primary Software Revision</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.13.0</snmp_oid>
          <key>extremePrimarySoftwareRev</key>
          <delay>86400</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <description>EXTREME-SYSTEM-MIB::extremePrimarySoftwareRev The software revision of the primary image stored in this device.</description>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Status of Redundant Power Supply</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.12.0</snmp_oid>
          <key>extremeRedundantPowerAlarm</key>
          <delay>60</delay>
          <description>EXTREME-SYSTEM-MIB::extremeRedundantPowerAlarm Alarm state of an external redundant power supply to a device. Alarm state indicates either fan failure or overtemperature condition.</description>
          <applications>
            <application>
              <name>Power Supplies</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}=2</expression>
              <name>Redundant Power Supply is in problem status</name>
              <status>DISABLED</status>
              <priority>AVERAGE</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Operational Status of Redundant Power Supply</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.11.0</snmp_oid>
          <key>extremeRedundantPowerStatus</key>
          <delay>60</delay>
          <description>EXTREME-SYSTEM-MIB::extremeRedundantPowerStatus Operational status of an external redundant power supply to a device.</description>
          <applications>
            <application>
              <name>Power Supplies</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}=1</expression>
              <name>Redundant Power Supply does not present</name>
            </trigger>
            <trigger>
              <expression>{last()}=3</expression>
              <name>Redundant Power Supply is not OK</name>
              <priority>AVERAGE</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Secondary Software Revision</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.14.0</snmp_oid>
          <key>extremeSecondarySoftwareRev</key>
          <delay>86400</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <description>EXTREME-SYSTEM-MIB::extremeSecondarySoftwareRev The software revision of the secondary image stored in this device.</description>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>System ID</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.16.0</snmp_oid>
          <key>extremeSystemID</key>
          <delay>86400</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <description>EXTREME-SYSTEM-MIB::extremeSystemID This represents the System ID of the Summit Switch.</description>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>CPU</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.1916.1.32.1.4.1.1]</snmp_oid>
          <key>extremeCpuMonitorSystemSlotId</key>
          <delay>300</delay>
          <lifetime>0</lifetime>
          <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeCpuMonitorSystemSlotId Slot Id of the system monitored.</description>
          <item_prototypes>
            <item_prototype>
              <name>CPU #$1 Utilization (5min)</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.1916.1.32.1.4.1.9.{#SNMPINDEX}</snmp_oid>
              <key>extremeCpuMonitorSystemUtilization5mins[{#SNMPINDEX}]</key>
              <delay>300</delay>
              <value_type>FLOAT</value_type>
              <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeCpuMonitorSystemUtilization5mins The CPU utilization by system resources. This is the utilization of the system resourcesin the last 5 minutes.</description>
              <applications>
                <application>
                  <name>CPU</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
          <trigger_prototypes>
            <trigger_prototype>
              <expression>{Template_Extreme_X450a:extremeCpuMonitorSystemUtilization5mins[{#SNMPINDEX}].last()}>={Template_Extreme_X450a:extremeCpuMonitorThreshold.last()}</expression>
              <name>High CPU #{#SNMPVALUE} Usage</name>
              <priority>AVERAGE</priority>
            </trigger_prototype>
          </trigger_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>FAN</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.1916.1.1.1.9.1.3]</snmp_oid>
          <key>extremeFanEntPhysicalIndex</key>
          <delay>300</delay>
          <description>EXTREME-SYSTEM-MIB::extremeFanEntPhysicalIndex The entity index for this fan entity in the entityPhysicalTable table of the entity MIB.</description>
          <item_prototypes>
            <item_prototype>
              <name>Status of FAN #$1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.1916.1.1.1.9.1.2.{#SNMPINDEX}</snmp_oid>
              <key>extremeFanOperational[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <description>EXTREME-SYSTEM-MIB::extremeFanOperational Operational status of a cooling fan.</description>
              <applications>
                <application>
                  <name>FAN Trays</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}=2</expression>
                  <name>Status of FAN #{#SNMPVALUE} is failed</name>
                  <priority>AVERAGE</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Memory</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.1916.1.32.2.2.1.1]</snmp_oid>
          <key>extremeMemoryMonitorSystemSlotId</key>
          <delay>300</delay>
          <lifetime>0</lifetime>
          <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeMemoryMonitorSystemSlotId Slot Id of the MSM monitored.</description>
          <item_prototypes>
            <item_prototype>
              <name>Free Memory #$1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.1916.1.32.2.2.1.3.{#SNMPINDEX}</snmp_oid>
              <key>extremeMemoryMonitorSystemFree[{#SNMPVALUE}]</key>
              <delay>60</delay>
              <units>B</units>
              <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeMemoryMonitorSystemFree Total amount of free memory in Kbytes in the system.</description>
              <applications>
                <application>
                  <name>Memory</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>MULTIPLIER</type>
                  <params>1024</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Total Memory #$1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.1916.1.32.2.2.1.2.{#SNMPINDEX}</snmp_oid>
              <key>extremeMemoryMonitorSystemTotal[{#SNMPVALUE}]</key>
              <delay>3600</delay>
              <units>B</units>
              <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeMemoryMonitorSystemTotal Total amount of DRAM in Kbytes in the system.</description>
              <applications>
                <application>
                  <name>Memory</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>MULTIPLIER</type>
                  <params>1024</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Used Memory #$1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.1916.1.32.2.2.1.4.{#SNMPINDEX}</snmp_oid>
              <key>extremeMemoryMonitorSystemUsage[{#SNMPVALUE}]</key>
              <delay>60</delay>
              <units>B</units>
              <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeMemoryMonitorSystemUsage Total amount of memory used by system services in Kbytes in the system.</description>
              <applications>
                <application>
                  <name>Memory</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>MULTIPLIER</type>
                  <params>1024</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Used Memory #$1 by Applications</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.1916.1.32.2.2.1.5.{#SNMPINDEX}</snmp_oid>
              <key>extremeMemoryMonitorUserUsage[{#SNMPVALUE}]</key>
              <delay>300</delay>
              <units>B</units>
              <description>EXTREME-SOFTWARE-MONITOR-MIB::extremeMemoryMonitorUserUsage Total amount of memory used by applications in Kbytes in the system.</description>
              <applications>
                <application>
                  <name>Memory</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>MULTIPLIER</type>
                  <params>1024</params>
                </step>
              </preprocessing>
            </item_prototype>
          </item_prototypes>
          <trigger_prototypes>
            <trigger_prototype>
              <expression>{Template_Extreme_X450a:extremeMemoryMonitorSystemFree[{#SNMPVALUE}].last()}&lt;={Template_Extreme_X450a:extremeMemoryMonitorSystemTotal[{#SNMPVALUE}].last()}*0.05</expression>
              <name>High Memory #{#SNMPVALUE} Usage</name>
              <priority>AVERAGE</priority>
            </trigger_prototype>
          </trigger_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>Memory #{SNMPVALUE} Usage</name>
              <graph_items>
                <graph_item>
                  <drawtype>GRADIENT_LINE</drawtype>
                  <color>00C800</color>
                  <item>
                    <host>Template_Extreme_X450a</host>
                    <key>extremeMemoryMonitorSystemFree[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>1</sortorder>
                  <color>0000C8</color>
                  <item>
                    <host>Template_Extreme_X450a</host>
                    <key>extremeMemoryMonitorSystemUsage[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>2</sortorder>
                  <color>C800C8</color>
                  <item>
                    <host>Template_Extreme_X450a</host>
                    <key>extremeMemoryMonitorUserUsage[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>3</sortorder>
                  <color>C80000</color>
                  <item>
                    <host>Template_Extreme_X450a</host>
                    <key>extremeMemoryMonitorSystemTotal[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
</zabbix_export>