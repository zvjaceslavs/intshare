<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-11-09T04:53:08Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>SNMP Extreme EXOS</template>
      <name>SNMP Extreme EXOS</name>
      <description>## Overview HARDWARE FIRMWARE STACKING</description>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>CPU</name>
        </application>
        <application>
          <name>Fans</name>
        </application>
        <application>
          <name>Inventory</name>
        </application>
        <application>
          <name>Memory</name>
        </application>
        <application>
          <name>Power Supply</name>
        </application>
        <application>
          <name>Stack</name>
        </application>
        <application>
          <name>Temperature</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Device: Temperature status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1916.1.1.1.7.0</snmp_oid>
          <key>sensor.temp.status[extremeOverTemperatureAlarm.0]</key>
          <delay>60</delay>
          <description>MIB: EXTREME-SYSTEM-MIB Temperature status of testpoint: Device</description>
          <applications>
            <application>
              <name>Temperature</name>
            </application>
          </applications>
          <valuemap>
            <name>EXTREME-SYSTEM-MIB::extremeOverTemperatureAlarm</name>
          </valuemap>
        </item>
        <item>
          <name>Device: Temperature</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1916.1.1.1.8.0</snmp_oid>
          <key>sensor.temp.value[extremeCurrentTemperature.0]</key>
          <delay>60</delay>
          <units>Â°C</units>
          <description>MIB: EXTREME-SYSTEM-MIB Temperature readings of testpoint: Device Reference: https://gtacknowledge.extremenetworks.com/articles/Q_A/Does-EXOS-support-temperature-polling-via-SNMP-on-all-nodes-in-a-stack</description>
          <applications>
            <application>
              <name>Temperature</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(5m)}>65</expression>
              <name>Device: Temperature is above critical threshold</name>
              <priority>HIGH</priority>
            </trigger>
            <trigger>
              <expression>{avg(5m)}>55</expression>
              <name>Device: Temperature is above warning threshold</name>
              <priority>WARNING</priority>
            </trigger>
            <trigger>
              <expression>{avg(5m)}&lt;5</expression>
              <name>Device: Temperature is too low</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>CPU utilization</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1916.1.32.1.2.0</snmp_oid>
          <key>system.cpu.util[extremeCpuMonitorTotalUtilization.0]</key>
          <delay>60</delay>
          <units>%</units>
          <description>MIB: EXTREME-SOFTWARE-MONITOR-MIB Total CPU utlization (percentage) as of last sampling.</description>
          <applications>
            <application>
              <name>CPU</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(5m)}>80</expression>
              <name>Processor load is too high on {HOST.NAME}</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Firmware version</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.47.1.1.1.1.9.1</snmp_oid>
          <key>system.hw.firmware</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <inventory_link>SOFTWARE</inventory_link>
          <applications>
            <application>
              <name>Inventory</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Hardware model name</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.47.1.1.1.1.2.1</snmp_oid>
          <key>system.hw.model</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>Inventory</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Operating system</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1916.1.1.1.13.0</snmp_oid>
          <key>system.hw.os</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>MIB: EXTREME-SYSTEM-MIB The software revision of the primary image stored in this device. This string will have a zero length if the revision is unknown, invalid or not present. This will also be reported in RMON2 probeSoftwareRev if this is the software image currently running in the device</description>
          <inventory_link>SOFTWARE_FULL</inventory_link>
          <applications>
            <application>
              <name>Inventory</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Hardware serial number</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.47.1.1.1.1.11.1</snmp_oid>
          <key>system.hw.serialnumber</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <inventory_link>SERIALNO_A</inventory_link>
          <applications>
            <application>
              <name>Inventory</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Stack port Info</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE}, 1.3.6.1.4.1.1916.1.33.3.1.1]</snmp_oid>
          <key>extremeStackingPortIfIndex</key>
          <delay>3600</delay>
          <description>A unique value for each interface. Its value ranges between 1 and the value of ifNumber. The value for each interface must remain constant at least from one re-initialization of the entity's network management system to the next re- initialization.</description>
          <item_prototypes>
            <item_prototype>
              <name>Stack port {#SNMPINDEX} status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.33.3.1.4.{#SNMPINDEX}</snmp_oid>
              <key>extremeStackingPortLinkStatus[{#SNMPINDEX}]</key>
              <delay>30</delay>
              <description>The Link status of the stacking port link</description>
              <applications>
                <application>
                  <name>Stack</name>
                </application>
              </applications>
              <valuemap>
                <name>EXTREME-STACKING-MIB::extremeOperStatus</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}&lt;>1</expression>
                  <name>Stack port {#SNMPVALUE}: Member is in critical state</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Stack Info</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE}, 1.3.6.1.4.1.9.9.500.1.2.1.1.1]</snmp_oid>
          <key>extremeStackMemberSlotId</key>
          <delay>3600</delay>
          <description>The slot id of the stack member in the stack</description>
          <item_prototypes>
            <item_prototype>
              <name>#{#SNMPVALUE}: Member MAC address</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.33.2.1.6.{#SNMPINDEX}</snmp_oid>
              <key>extremeStackMemberMavAddress[{#SNMPVALUE}]</key>
              <delay>3600</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>The Mac address of the stack member</description>
              <applications>
                <application>
                  <name>Stack</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>#{#SNMPVALUE}: Member mgmt ip address</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.33.2.1.15.{#SNMPINDEX}</snmp_oid>
              <key>extremeStackMemberMgmtIpAddress[{#SNMPVALUE}]</key>
              <delay>3600</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>The fallback mgmt ip address which was configured through CLI for this stack member. This ipaddress can be used to communicate with this member, if it comes up as the master. The ipaddress is associated with the vlan configured</description>
              <applications>
                <application>
                  <name>Stack</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>#{#SNMPVALUE}: Member oper status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.33.2.1.3.{#SNMPINDEX}</snmp_oid>
              <key>extremeStackMemberOperStatus[{#SNMPVALUE}]</key>
              <delay>60</delay>
              <description>The operational status of the stack member. When the stack comes up, if there is a vector mismatch namely license mismatch or platform mismatch between the master and the member, the member remains in the mismatch state. Further communication with the member is not possible</description>
              <applications>
                <application>
                  <name>Stack</name>
                </application>
              </applications>
              <valuemap>
                <name>EXTREME-STACKING-MIB::extremeOperStatus</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}&lt;>1</expression>
                  <name>Stack {#SNMPVALUE}: Member is in critical state</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>#{#SNMPVALUE}: Member role</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.33.2.1.4.{#SNMPINDEX}</snmp_oid>
              <key>extremeStackMemberRole[{#SNMPVALUE}]</key>
              <delay>3600</delay>
              <description>The role of the stack member as to whether it is acting as a master,backup or a slave</description>
              <applications>
                <application>
                  <name>Stack</name>
                </application>
              </applications>
              <valuemap>
                <name>EXTREME-STACKING-MIB::extremeMemberRole</name>
              </valuemap>
            </item_prototype>
            <item_prototype>
              <name>#{#SNMPVALUE}: Member stack priority</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.33.2.1.14.{#SNMPINDEX}</snmp_oid>
              <key>extremeStackMemberStackPriority[{#SNMPVALUE}]</key>
              <delay>3600</delay>
              <description>The stack priority of the stack member. The stack priority is used for the master election algorithm. The member with the highest priority comes up as the master. If there are more than one switch with highest priority, the member with the least MAC among them comes up as the master</description>
              <applications>
                <application>
                  <name>Stack</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>FAN Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.1916.1.1.1.9.1.1]</snmp_oid>
          <key>fan.discovery</key>
          <delay>3600</delay>
          <item_prototypes>
            <item_prototype>
              <name>Fan {#SNMPVALUE}: Fan speed</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.1.1.9.1.4.{#SNMPINDEX}</snmp_oid>
              <key>sensor.fan.speed[extremeFanSpeed.{#SNMPINDEX}]</key>
              <delay>60</delay>
              <units>rpm</units>
              <description>MIB: EXTREME-SYSTEM-MIB The speed (RPM) of a cooling fan in the fantray {#SNMPVALUE}</description>
              <applications>
                <application>
                  <name>Fans</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Fan {#SNMPVALUE}: Fan status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.1.1.9.1.2.{#SNMPINDEX}</snmp_oid>
              <key>sensor.fan.status[extremeFanOperational.{#SNMPINDEX}]</key>
              <delay>60</delay>
              <description>MIB: EXTREME-SYSTEM-MIB Operational status of a cooling fan.</description>
              <applications>
                <application>
                  <name>Fans</name>
                </application>
              </applications>
              <valuemap>
                <name>EXTREME-SYSTEM-MIB::extremeFanOperational</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}&lt;>1</expression>
                  <name>Fan {#SNMPVALUE}: Fan is in critical state</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Memory Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.1916.1.32.2.2.1.1]</snmp_oid>
          <key>memory.discovery</key>
          <delay>3600</delay>
          <item_prototypes>
            <item_prototype>
              <name>#{#SNMPVALUE}: Available memory</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.32.2.2.1.3.{#SNMPINDEX}</snmp_oid>
              <key>vm.memory.free[extremeMemoryMonitorSystemFree.{#SNMPINDEX}]</key>
              <delay>60</delay>
              <units>B</units>
              <description>MIB: EXTREME-SOFTWARE-MONITOR-MIB Total amount of free memory in Kbytes in the system.</description>
              <applications>
                <application>
                  <name>Memory</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>#{#SNMPVALUE}: Memory utilization</name>
              <type>CALCULATED</type>
              <key>vm.memory.pused[{#SNMPVALUE}]</key>
              <delay>60</delay>
              <units>%</units>
              <params>((last(vm.memory.total[extremeMemoryMonitorSystemTotal.{#SNMPINDEX}])-last(vm.memory.free[extremeMemoryMonitorSystemFree.{#SNMPINDEX}]))/last(vm.memory.total[extremeMemoryMonitorSystemTotal.{#SNMPINDEX}]))*100</params>
              <description>Memory utilization in %</description>
              <applications>
                <application>
                  <name>Memory</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{avg(5m)}>90</expression>
                  <name>#{#SNMPVALUE}: High memory utilization</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>#{#SNMPVALUE}: Total memory</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.32.2.2.1.2.{#SNMPINDEX}</snmp_oid>
              <key>vm.memory.total[extremeMemoryMonitorSystemTotal.{#SNMPINDEX}]</key>
              <delay>60</delay>
              <units>B</units>
              <description>MIB: EXTREME-SOFTWARE-MONITOR-MIB Total amount of DRAM in Kbytes in the system.</description>
              <applications>
                <application>
                  <name>Memory</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>#{#SNMPVALUE}: Memory utilization</name>
              <graph_items>
                <graph_item>
                  <color>1A7C11</color>
                  <item>
                    <host>SNMP Extreme EXOS</host>
                    <key>vm.memory.pused[{#SNMPVALUE}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>PSU Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.1916.1.1.1.27.1.1]</snmp_oid>
          <key>psu.discovery</key>
          <delay>3600</delay>
          <description>Table of status of all power supplies in the system.</description>
          <item_prototypes>
            <item_prototype>
              <name>PSU {#SNMPVALUE}: Power supply status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.1916.1.1.1.27.1.2.{#SNMPINDEX}</snmp_oid>
              <key>sensor.psu.status[extremePowerSupplyStatus.{#SNMPINDEX}]</key>
              <delay>60</delay>
              <description>MIB: EXTREME-SYSTEM-MIB Status of the power supply {#SNMPVALUE}</description>
              <applications>
                <application>
                  <name>Power Supply</name>
                </application>
              </applications>
              <valuemap>
                <name>EXTREME-SYSTEM-MIB::extremePowerSupplyStatus</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}&lt;>3</expression>
                  <name>PSU {#SNMPVALUE}: Power supply is in critical state</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{last()}&lt;>4</expression>
                  <name>PSU {#SNMPVALUE}: Power supply power off</name>
                  <priority>WARNING</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
  <graphs>
    <graph>
      <name>CPU utilization</name>
      <graph_items>
        <graph_item>
          <color>1A7C11</color>
          <item>
            <host>SNMP Extreme EXOS</host>
            <key>system.cpu.util[extremeCpuMonitorTotalUtilization.0]</key>
          </item>
        </graph_item>
      </graph_items>
    </graph>
  </graphs>
  <value_maps>
    <value_map>
      <name>EXTREME-STACKING-MIB::extremeMemberRole</name>
      <mappings>
        <mapping>
          <value>1</value>
          <newvalue>master</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>slave</newvalue>
        </mapping>
        <mapping>
          <value>3</value>
          <newvalue>backup</newvalue>
        </mapping>
      </mappings>
    </value_map>
    <value_map>
      <name>EXTREME-STACKING-MIB::extremeOperStatus</name>
      <mappings>
        <mapping>
          <value>1</value>
          <newvalue>up</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>down</newvalue>
        </mapping>
        <mapping>
          <value>3</value>
          <newvalue>mismatch</newvalue>
        </mapping>
      </mappings>
    </value_map>
    <value_map>
      <name>EXTREME-SYSTEM-MIB::extremeFanOperational</name>
      <mappings>
        <mapping>
          <value>1</value>
          <newvalue>true - on</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>false - off</newvalue>
        </mapping>
      </mappings>
    </value_map>
    <value_map>
      <name>EXTREME-SYSTEM-MIB::extremeOverTemperatureAlarm</name>
      <mappings>
        <mapping>
          <value>1</value>
          <newvalue>alarm</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>normal</newvalue>
        </mapping>
      </mappings>
    </value_map>
    <value_map>
      <name>EXTREME-SYSTEM-MIB::extremePowerSupplyStatus</name>
      <mappings>
        <mapping>
          <value>1</value>
          <newvalue>notPresent</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>presentOK</newvalue>
        </mapping>
        <mapping>
          <value>3</value>
          <newvalue>presentNotOK</newvalue>
        </mapping>
        <mapping>
          <value>4</value>
          <newvalue>presentPowerOff</newvalue>
        </mapping>
      </mappings>
    </value_map>
  </value_maps>
</zabbix_export>