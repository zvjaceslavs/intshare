          </applications>
        </item>
        <item>
          <name>System status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.38243.1.1.4.0</snmp_oid>
          <key>systemStatus</key>
          <delay>60</delay>
          <history>30d</history>
