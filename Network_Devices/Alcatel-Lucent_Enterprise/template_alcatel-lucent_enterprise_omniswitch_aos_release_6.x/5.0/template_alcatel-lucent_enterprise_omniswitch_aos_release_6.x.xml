              <key>memory1hrmax.slot.[{#SNMPVALUE}]</key>
              <delay>60s</delay>
              <history>7d</history>
              <units>%</units>
              <applications>
                <application>
                  <name>Memory</name>
                </application>
              </applications>
              <request_method>POST</request_method>
            </item_prototype>
            <item_prototype>
              <name>Memory usage 1 min avg slot {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.6486.800.1.2.1.16.1.1.2.1.1.11.{#SNMPVALUE}</snmp_oid>
              <key>memory1minavg.slot.[{#SNMPVALUE}]</key>
              <delay>60s</delay>
              <history>7d</history>
