          <history>7d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <description>SNMPv2-MIB::sysLocation.0 - The physical location of this node (e.g., `telephone closet, 3rd floor'). If the location is unknown, the value is the zero-length string.</description>
          <inventory_link>LOCATION</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <request_method>POST</request_method>
