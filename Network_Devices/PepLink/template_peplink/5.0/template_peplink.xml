  <templates>
    <template>
      <template>Peplink</template>
      <name>Peplink</name>
      <groups>
        <group>
          <name>Template/Rede de dados</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>VPN</name>
        </application>
        <application>
          <name>WAN</name>
        </application>
      </applications>
      <discovery_rules>
        <discovery_rule>
          <name>Discovery_VPN</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#PEPVPNNAME}, .1.3.6.1.4.1.23695.200.1.10.1.1.2.1.2]</snmp_oid>
          <key>pepVpnStatusId</key>
          <delay>30s</delay>
          <item_prototypes>
            <item_prototype>
              <name>Status on {#PEPVPNNAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.23695.200.1.10.1.1.2.1.3.{#SNMPINDEX}</snmp_oid>
              <key>pepVpnStatusConnectionState[{#PEPVPNNAME}]</key>
              <delay>30s</delay>
              <history>30d</history>
              <trends>0</trends>
              <applications>
                <application>
                  <name>VPN</name>
                </application>
              </applications>
              <valuemap>
                <name>PEPVPNSTATUS</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}&lt;>4</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{last()}=4</recovery_expression>
                  <name>{HOST.NAME} Status SpeedFusion on {#PEPVPNNAME}</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Discovery_WAN</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#WANNAME}, 1.3.6.1.4.1.23695.2.1.2.1.2]</snmp_oid>
          <key>wanName</key>
          <delay>30s</delay>
          <item_prototypes>
            <item_prototype>
              <name>Health Check on {#WANNAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.23695.2.1.2.1.4.{#SNMPINDEX}</snmp_oid>
              <key>wanHealthCheckState[{#WANNAME}]</key>
              <delay>30s</delay>
              <history>30d</history>
              <applications>
                <application>
                  <name>WAN</name>
                </application>
              </applications>
              <valuemap>
                <name>PEPWANHEALTH</name>
              </valuemap>
            </item_prototype>
            <item_prototype>
              <name>Status on {#WANNAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.23695.2.1.2.1.3.{#SNMPINDEX}</snmp_oid>
              <key>wanState[{#WANNAME}]</key>
              <history>30d</history>
              <applications>
                <application>
                  <name>WAN</name>
                </application>
              </applications>
              <valuemap>
                <name>PEPWANSTATUS</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}&lt;>3 and {last()}&lt;>1</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{last()}=3 or {last()}=1</recovery_expression>
                  <name>{HOST.NAME} : {#WANNAME} Link down</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
