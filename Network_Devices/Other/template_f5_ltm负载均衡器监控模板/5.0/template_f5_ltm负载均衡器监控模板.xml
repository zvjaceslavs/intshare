        <item>
          <name>ping丢包率</name>
          <type>SIMPLE</type>
          <key>icmppingloss[,10,,,]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(10m)}>{$ICMP_LOSS_WARN}</expression>
              <name>ICMP ping 丢包</name>
              <priority>AVERAGE</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_loadbalancer_F5_LTM:icmpping[,5,,,].last(#1)}=0 and {monitor_loadbalancer_F5_LTM:icmpping[,5,,,].last(#2)}=1</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ping时延</name>
          <type>SIMPLE</type>
