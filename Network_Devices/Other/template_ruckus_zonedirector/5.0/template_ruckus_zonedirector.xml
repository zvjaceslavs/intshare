<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T10:51:30Z</date>
  <groups>
    <group>
      <name>Wireless</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template SNMP Ruckus Zonedirector</template>
      <name>Template SNMP Ruckus Zonedirector</name>
      <groups>
        <group>
          <name>Wireless</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Acces Points</name>
        </application>
        <application>
          <name>General</name>
        </application>
        <application>
          <name>ICMP</name>
        </application>
        <application>
          <name>Interfaces</name>
        </application>
        <application>
          <name>Users</name>
        </application>
      </applications>
      <items>
        <item>
          <name>ICMP ping</name>
          <type>SIMPLE</type>
          <key>icmpping</key>
          <delay>60</delay>
          <history>7d</history>
          <applications>
            <application>
              <name>ICMP</name>
            </application>
          </applications>
          <valuemap>
            <name>Service state</name>
          </valuemap>
          <triggers>
            <trigger>
              <expression>{max(#3)}=0</expression>
              <name>{HOST.NAME} is unavailable by ICMP</name>
              <priority>AVERAGE</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ICMP loss</name>
          <type>SIMPLE</type>
          <key>icmppingloss</key>
          <delay>60</delay>
          <history>7d</history>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>ICMP</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{min(5m)}>20</expression>
              <name>Ping loss is too high on {HOST.NAME}</name>
              <priority>WARNING</priority>
              <dependencies>
                <dependency>
                  <name>{HOST.NAME} is unavailable by ICMP</name>
                  <expression>{Template SNMP Ruckus Zonedirector:icmpping.max(#3)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ICMP response time</name>
          <type>SIMPLE</type>
          <key>icmppingsec</key>
          <delay>60</delay>
          <history>7d</history>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <applications>
            <application>
              <name>ICMP</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(5m)}>0.5</expression>
              <name>Response time is too high on {HOST.NAME}</name>
              <priority>WARNING</priority>
              <dependencies>
                <dependency>
                  <name>{HOST.NAME} is unavailable by ICMP</name>
                  <expression>{Template SNMP Ruckus Zonedirector:icmpping.max(#3)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ZoneDirector CPU Utilization</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.5.58.0</snmp_oid>
          <key>ruckusZDSystemCPUUTIL.0</key>
          <delay>30</delay>
          <units>%</units>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ZoneDirector MAC Adress</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.1.3.0</snmp_oid>
          <key>ruckusZDSystemMacAddr.0</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>MACADDRESS_A</inventory_link>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ZoneDirector System Model</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.1.9.0</snmp_oid>
