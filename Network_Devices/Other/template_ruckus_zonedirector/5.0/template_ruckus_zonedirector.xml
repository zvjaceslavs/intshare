          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>MODEL</inventory_link>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ZoneDirector System Name</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.1.1.0</snmp_oid>
          <key>ruckusZDSystemName.0</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>NAME</inventory_link>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ZoneDirector Serial Number</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.1.15.0</snmp_oid>
          <key>ruckusZDSystemSerialNumber.0</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>SERIALNO_A</inventory_link>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ZoneDirector Total Users</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.15.30.0</snmp_oid>
          <key>ruckusZDSystemStatsAllNumSta</key>
          <delay>30</delay>
          <units>Users</units>
          <applications>
            <application>
              <name>Users</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ZoneDirector Total Auth Clients</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.15.2.0</snmp_oid>
          <key>ruckusZDSystemStatsNumSta.0</key>
          <delay>30</delay>
          <units>Users</units>
          <applications>
            <application>
              <name>Users</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ZoneDirector System Version</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.1.18.0</snmp_oid>
          <key>ruckusZDSystemVersion.0</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>OS</inventory_link>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Ruckus APs</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.25053.1.2.2.1.1.2.1.1.2]</snmp_oid>
          <key>AP-[{#SNMPVALUE}]</key>
          <delay>3600</delay>
          <lifetime>1d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>AP $1 : Auth Clients</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.25053.1.2.2.1.1.2.1.1.15.{#SNMPINDEX}</snmp_oid>
              <key>ruckusZDWLANAPNumSta[{#SNMPVALUE}]</key>
              <delay>60</delay>
              <units>Users</units>
              <applications>
                <application>
                  <name>Users</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>AP Status $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.25053.1.2.2.1.1.2.1.1.3.{#SNMPINDEX}</snmp_oid>
              <key>ruckusZDWLANAPStatus[{#SNMPVALUE}]</key>
              <delay>60</delay>
              <trends>0</trends>
              <value_type>CHAR</value_type>
              <applications>
                <application>
                  <name>Acces Points</name>
                </application>
              </applications>
              <valuemap>
                <name>RuckusAPStatus</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(0)}=0</expression>
                  <name>Acces Point {#SNMPVALUE} is Offline</name>
                  <priority>AVERAGE</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>ZoneDirector Eth</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.25053.1.2.1.1.1.18.2.1.2]</snmp_oid>
          <key>ZDEthName[{#SNMPVALUE}]</key>
          <delay>3600</delay>
          <lifetime>1d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>Status $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.25053.1.2.1.1.1.18.2.1.5.{#SNMPINDEX}</snmp_oid>
              <key>ruckusZDEthStatus[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <valuemap>
                <name>ruckusZDEthStatus</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(0)}=2</expression>
                  <name>Port {#SNMPVALUE} is Down</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
