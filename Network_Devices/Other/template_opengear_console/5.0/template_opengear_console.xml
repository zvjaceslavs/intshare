          <type>SNMP_AGENT</type>
          <snmp_oid>OG-STATUSv2-MIB::ogFirmwareVersion</snmp_oid>
          <key>opengear.firmwareversion</key>
          <delay>3600</delay>
          <history>14d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <applications>
            <application>
              <name>OPENGEAR_SYSTEM</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{diff()}&lt;>0</expression>
              <name>Firmware Version Changed</name>
              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Serial Number</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>OG-STATUSv2-MIB::ogSerialNumber</snmp_oid>
          <key>opengear.serialnumber</key>
