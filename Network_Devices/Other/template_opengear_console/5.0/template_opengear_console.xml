              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>System Uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>HOST-RESOURCES-MIB::hrSystemUptime.0</snmp_oid>
          <key>opengear.systemuptime</key>
          <delay>60</delay>
          <history>14d</history>
          <description>System Uptime in centiseconds (hundredths of a second: 0.01s)</description>
          <applications>
            <application>
              <name>OPENGEAR_SYSTEM</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}&lt;{avg(15m)}</expression>
              <name>Device Restarted</name>
              <priority>AVERAGE</priority>
            </trigger>
          </triggers>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Network Interfaces</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#IFNAME}, IF-MIB::ifDescr]</snmp_oid>
          <key>opengear.snmp.if</key>
          <delay>3600</delay>
          <lifetime>7d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>Interface {#IFNAME} Inbound Discards</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifInDiscards.{#SNMPINDEX}</snmp_oid>
              <key>opengear.snmp.netif.indiscards[{#IFNAME}]</key>
              <delay>60</delay>
              <history>14d</history>
              <applications>
                <application>
                  <name>OPENGEAR_NETWORK</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>SIMPLE_CHANGE</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Interface {#IFNAME} Inbound Errors</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifInErrors.{#SNMPINDEX}</snmp_oid>
              <key>opengear.snmp.netif.inerr[{#IFNAME}]</key>
              <delay>60</delay>
              <history>14d</history>
              <applications>
