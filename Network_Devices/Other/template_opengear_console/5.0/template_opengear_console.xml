              </applications>
              <preprocessing>
                <step>
                  <type>SIMPLE_CHANGE</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Interface {#IFNAME} Outbound Errors</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifOutErrors.{#SNMPINDEX}</snmp_oid>
              <key>opengear.snmp.netif.outerr[{#IFNAME}]</key>
              <delay>60</delay>
              <history>14d</history>
              <applications>
