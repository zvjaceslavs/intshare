        <application>
          <name>出方向广播报文速率</name>
        </application>
        <application>
          <name>出方向流量</name>
        </application>
        <application>
          <name>出方向组播报文速率</name>
        </application>
        <application>
          <name>出方向错包</name>
        </application>
        <application>
          <name>电源情况</name>
        </application>
        <application>
          <name>端口带宽</name>
        </application>
        <application>
          <name>端口状态</name>
        </application>
        <application>
          <name>系统信息</name>
        </application>
        <application>
          <name>风扇情况</name>
        </application>
      </applications>
      <items>
        <item>
          <name>CPU 5分钟利用率</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.4881.1.1.10.2.36.1.1.3.0</snmp_oid>
          <key>CPUUsageIn5Min</key>
          <delay>10m</delay>
          <history>3d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>CPU情况</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}>{$CPU_USAGE_THRESHOLD}</expression>
              <name>CPU利用率高</name>
              <priority>HIGH</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ping丢包率</name>
          <type>SIMPLE</type>
          <key>icmppingloss[,10,,,]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(10m)}>{$ICMP_LOSS_WARN}</expression>
              <name>ICMP ping 丢包</name>
              <priority>AVERAGE</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_Switch_Ruijie:icmpping[,5,,,].last(#1)}=0 and {monitor_Switch_Ruijie:icmpping[,5,,,].last(#2)}=1</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ping时延</name>
          <type>SIMPLE</type>
          <key>icmppingsec[,10,,,,avg]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{min(10m)}>{$ICMP_RESPONSE_TIME_WARN}</expression>
              <name>ICMP ping 时延高</name>
              <priority>AVERAGE</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_Switch_Ruijie:icmpping[,5,,,].last(#1)}=0 and {monitor_Switch_Ruijie:icmpping[,5,,,].last(#2)}=1</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ICMP ping</name>
          <type>SIMPLE</type>
          <key>icmpping[,5,,,]</key>
          <history>7d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}=0 and {last(#2)}=1</expression>
              <name>ICMP ping不可达</name>
              <priority>HIGH</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{last(#1)}=0</expression>
              <name>监控失效（ping不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>ping不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>系统信息</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.1.0</snmp_oid>
          <key>SystemDescription</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{nodata(12h)}=1</expression>
              <name>监控失效（snmp不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>snmp不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>运行时间</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.3.0</snmp_oid>
          <key>SystemUpTime</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>风扇发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.4881.1.1.10.2.1.1.21.1.3]</snmp_oid>
          <key>FanDiscovery</key>
          <delay>1h</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>风扇运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.4881.1.1.10.2.1.1.21.1.2.{#SNMPINDEX}</snmp_oid>
              <key>FanOperationStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>风扇情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}&lt;>4</expression>
                  <name>风扇运行异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>端口发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#IFDESCR},IF-MIB::ifDescr,{#IFALIAS},IF-MIB::ifAlias]</snmp_oid>
          <key>InterfaceDiscovery_status</key>
          <delay>12h</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*Vlan.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <description>You may also consider using IF-MIB::ifType or IF-MIB::ifAlias for discovery depending on your filtering needs. {$CLOUD_SNMP_COMMUNITY} is a global macro.</description>
          <item_prototypes>
            <item_prototype>
              <name>端口 {#IFDESCR}({#IFALIAS})状态</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifOperStatus.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceOperationStatus.[{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <description>The current operational state of the interface.</description>
              <applications>
                <application>
                  <name>端口状态</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#2)}=1 and {last(#1)}=2</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{last(#2)}=1 and {last(#1)}=1</recovery_expression>
                  <name>端口:{#IFDESCR} down</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>端口发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#IFDESCR},IF-MIB::ifDescr,{#IFSTATUS},IF-MIB::ifOperStatus,{#IFALIAS},IF-MIB::ifAlias]</snmp_oid>
          <key>InterfaceDiscovery_traffic</key>
          <delay>12h</delay>
          <filter>
            <evaltype>AND</evaltype>
            <conditions>
              <condition>
                <macro>{#IFSTATUS}</macro>
                <value>1</value>
                <formulaid>E</formulaid>
              </condition>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*Vlan.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>B</formulaid>
              </condition>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*Loop.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>A</formulaid>
              </condition>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*VLAN.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>C</formulaid>
              </condition>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*NULL.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>D</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>端口带宽 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.15.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceBandwidth.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>12h</delay>
              <history>3d</history>
              <trends>0d</trends>
              <units>Mbps</units>
              <applications>
                <application>
                  <name>端口带宽</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>入方向带宽利用率 {#IFDESCR}({#IFALIAS})</name>
              <type>CALCULATED</type>
              <key>InterfaceInBandwidthUsage.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <value_type>FLOAT</value_type>
              <units>%</units>
              <params>last("InterfaceInTraffic.[{#SNMPINDEX},{#IFDESCR}]")/last("InterfaceBandwidth.[{#SNMPINDEX},{#IFDESCR}]")/10000</params>
              <applications>
                <application>
                  <name>入方向带宽利用率</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>{$BANDWIDTH_USAGE_IN_THRESHOLD}</expression>
                  <name>{#IFDESCR} 入方向带宽利用率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>入方向广播报文速率 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.9.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceInBroadcastPacketsTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <units>pps</units>
              <applications>
                <application>
                  <name>入方向广播包流量</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>入方向组播报文速率 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.9.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceInMulticastPacketsTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>入方向组播报文速率</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>入方向错包 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.14.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceInPacketError.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>30m</delay>
              <history>7d</history>
              <trends>0d</trends>
              <units>pps</units>
              <applications>
                <application>
                  <name>入方向错包</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>{$ERROR_PACKETS_IN_THRESHOLD}</expression>
                  <name>{#IFDESCR} 入方向错包率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>入方向流量 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.6.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceInTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <units>bps</units>
              <applications>
                <application>
                  <name>入方向流量</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>MULTIPLIER</type>
                  <params>8</params>
                </step>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>入方向单播报文速率 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.7.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceInUnicastPacketsTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <units>pps</units>
              <applications>
                <application>
                  <name>入方向单播报文速率</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>出方向带宽利用率 {#IFDESCR}({#IFALIAS})</name>
              <type>CALCULATED</type>
              <key>InterfaceOutBandwidthUsage[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <value_type>FLOAT</value_type>
              <units>%</units>
              <params>last("InterfaceOutTraffic.[{#SNMPINDEX},{#IFDESCR}]")/last("InterfaceBandwidth.[{#SNMPINDEX},{#IFDESCR}]")/10000</params>
              <applications>
                <application>
                  <name>出方向带宽利用率</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>{$BANDWIDTH_USAGE_OUT_THRESHOLD}</expression>
                  <name>{#IFDESCR} 出方向带宽利用率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>出方向广播报文速率 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.13.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceOutBroadcastPacketTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
              <trends>0d</trends>
              <units>pps</units>
              <applications>
                <application>
                  <name>出方向广播报文速率</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>出方向组播报文速率 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.12.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceOutMulticastPackestTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <units>pps</units>
              <applications>
                <application>
                  <name>出方向组播报文速率</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>出方向错包 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.2.2.1.20.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceOutPacketError.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>30m</delay>
              <history>7d</history>
              <trends>0d</trends>
              <units>pps</units>
              <applications>
                <application>
                  <name>出方向错包</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>{$ERROR_PACKETS_OUT_THRESHOLD}</expression>
                  <name>{#IFDESCR} 出方向错包率高</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>出方向流量 {#IFDESCR}({#IFALIAS})</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.2.1.31.1.1.1.10.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceOutTraffic.[{#SNMPINDEX},{#IFDESCR}]</key>
              <delay>10m</delay>
              <history>30d</history>
