          <description>Informa o modelo do aparelho</description>
          <inventory_link>CONTACT</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>System Description</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.1.0</snmp_oid>
          <key>sysDescr.0</key>
          <history>1w</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>Informa o modelo do aparelho</description>
          <inventory_link>HARDWARE_FULL</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
