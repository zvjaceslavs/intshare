          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.6.0</snmp_oid>
          <key>sysLocation.0</key>
          <history>1w</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>Informa o modelo do aparelho</description>
          <inventory_link>LOCATION</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.3.0</snmp_oid>
          <key>sysUpTime.0</key>
          <history>1w</history>
          <value_type>FLOAT</value_type>
          <units>uptime</units>
          <applications>
