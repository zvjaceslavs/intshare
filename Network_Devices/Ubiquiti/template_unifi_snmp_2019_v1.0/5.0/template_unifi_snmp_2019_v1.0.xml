            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>MAC Address</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.41112.1.6.2.1.1.4.1</snmp_oid>
