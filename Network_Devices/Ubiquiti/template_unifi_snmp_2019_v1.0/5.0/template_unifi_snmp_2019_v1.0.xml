          <preprocessing>
            <step>
              <type>CHANGE_PER_SECOND</type>
              <params></params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>AP Hostname</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.5.0</snmp_oid>
          <key>unifiSysName.0</key>
          <history>1w</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <inventory_link>NAME</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Wifi Interfaces</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#UNIFIVAPNAME},.1.3.6.1.4.1.41112.1.6.1.2.1.7]</snmp_oid>
          <key>unifiVapName</key>
          <delay>30s</delay>
          <item_prototypes>
            <item_prototype>
              <name>Alias of interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.6.{#SNMPINDEX}</snmp_oid>
              <key>unifiVapEssId[{#UNIFIVAPNAME}]</key>
              <delay>30s</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Virtual Interfaces</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Users on $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.8.{#SNMPINDEX}</snmp_oid>
              <key>unifiVapNumStations[{#UNIFIVAPNAME}]</key>
              <delay>30s</delay>
              <applications>
                <application>
                  <name>Virtual Interfaces</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Traffic Incoming on $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.10.{#SNMPINDEX}</snmp_oid>
              <key>unifiVapRxBytes[{#UNIFIVAPNAME}]</key>
              <delay>30s</delay>
              <units>Bps</units>
              <applications>
                <application>
                  <name>Virtual Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Traffic Outgoing on $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.41112.1.6.1.2.1.16.{#SNMPINDEX}</snmp_oid>
              <key>unifiVapTxBytes[{#UNIFIVAPNAME}]</key>
              <delay>30s</delay>
              <units>Bps</units>
              <applications>
                <application>
                  <name>Virtual Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
              </preprocessing>
            </item_prototype>
          </item_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>Traffic on interface {#UNIFIVAPNAME}</name>
              <graph_items>
                <graph_item>
                  <color>1A7C11</color>
                  <item>
                    <host>UNIFI SNMP 2019 V1.0</host>
                    <key>unifiVapRxBytes[{#UNIFIVAPNAME}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>1</sortorder>
                  <color>F63100</color>
                  <item>
                    <host>UNIFI SNMP 2019 V1.0</host>
                    <key>unifiVapTxBytes[{#UNIFIVAPNAME}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
            <graph_prototype>
              <name>Traffic on interface {#UNIFIVAPNAME} (stacked)</name>
              <type>STACKED</type>
              <graph_items>
                <graph_item>
                  <color>1A7C11</color>
                  <item>
                    <host>UNIFI SNMP 2019 V1.0</host>
                    <key>unifiVapRxBytes[{#UNIFIVAPNAME}]</key>
                  </item>
                </graph_item>
                <graph_item>
                  <sortorder>1</sortorder>
                  <color>F63100</color>
                  <item>
                    <host>UNIFI SNMP 2019 V1.0</host>
                    <key>unifiVapTxBytes[{#UNIFIVAPNAME}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
            <graph_prototype>
              <name>Users connected in {#UNIFIVAPNAME}</name>
              <graph_items>
                <graph_item>
                  <color>3333FF</color>
                  <item>
                    <host>UNIFI SNMP 2019 V1.0</host>
                    <key>unifiVapNumStations[{#UNIFIVAPNAME}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
        </discovery_rule>
      </discovery_rules>
