            </application>
          </applications>
        </item>
        <item>
          <name>Traffic Outgoing</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.41112.1.6.2.1.1.12.1</snmp_oid>
