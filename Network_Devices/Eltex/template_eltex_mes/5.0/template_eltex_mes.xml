              <priority>WARNING</priority>
              <dependencies>
                <dependency>
                  <name>{HOST.NAME} is unavailable by ICMP</name>
                  <expression>{Eltex MES SNMPv2:icmpping.max(#3)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
