<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-26T04:39:49Z</date>
  <groups>
    <group>
      <name>Switchs</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>SWITCH HP - EQUIPAMENTO</template>
      <name>SWITCH HP - EQUIPAMENTO</name>
      <groups>
        <group>
          <name>Switchs</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Informações</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Número Serial</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>SNMPv2-SMI::mib-2.47.1.1.1.1.11.1</snmp_oid>
          <key>2.47.1.1.1.1.11.1</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>SERIALNO_A</inventory_link>
          <applications>
            <application>
              <name>Informações</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{diff(0)}>0</expression>
              <name>Número de série do dispositivo foi alterado</name>
              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Modelo do Dispositivo</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.47.1.1.1.1.13.1</snmp_oid>
          <key>entPhysicalModelName</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>TYPE</inventory_link>
          <applications>
            <application>
              <name>Informações</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Acessibilidade do dispositivo usando ICMP</name>
          <type>SIMPLE</type>
          <key>icmpping</key>
          <delay>60</delay>
          <history>7d</history>
          <trends>180d</trends>
          <applications>
            <application>
              <name>Informações</name>
            </application>
          </applications>
          <valuemap>
            <name>Host status</name>
          </valuemap>
          <preprocessing>
            <step>
              <type>BOOL_TO_DECIMAL</type>
              <params></params>
            </step>
          </preprocessing>
          <triggers>
            <trigger>
              <expression>{last(0)}=0</expression>
              <name>{HOST.NAME} está inacessível ou DOWN</name>
              <priority>DISASTER</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Perda de pacotes de dispositivos</name>
          <type>SIMPLE</type>
          <key>icmppingloss</key>
          <delay>60</delay>
          <history>7d</history>
          <trends>180d</trends>
          <units>%</units>
          <applications>
            <application>
              <name>Informações</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Nome do Dispositivo</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>SNMPv2-MIB::sysDescr.0</snmp_oid>
          <key>sysDescr</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>NAME</inventory_link>
          <applications>
            <application>
              <name>Informações</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{diff(0)}>0</expression>
              <name>Hostname foi alterado</name>
              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>SNMPv2-MIB::sysUpTime.0</snmp_oid>
          <key>sysUpTime</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>180d</trends>
          <units>uptime</units>
          <applications>
            <application>
              <name>Informações</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.01</params>
            </step>
          </preprocessing>
          <triggers>
            <trigger>
              <expression>{change(0)}&lt;0</expression>
              <name>{HOST.NAME} acaba de ser reiniciado</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
      </items>
    </template>
  </templates>
  <triggers>
    <trigger>
      <expression>{SWITCH HP - EQUIPAMENTO:icmpping.avg(600)}>0 and {SWITCH HP - EQUIPAMENTO:icmppingloss.avg(600)}>1</expression>
      <name>A perda de pacotes detectados no {HOST.NAME}</name>
      <priority>WARNING</priority>
    </trigger>
  </triggers>
  <value_maps>
    <value_map>
      <name>Host status</name>
      <mappings>
        <mapping>
          <value>0</value>
          <newvalue>Up</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>Unreachable</newvalue>
        </mapping>
      </mappings>
    </value_map>
  </value_maps>
</zabbix_export>