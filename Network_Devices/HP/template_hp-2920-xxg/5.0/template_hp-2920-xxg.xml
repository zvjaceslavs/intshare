  <templates>
    <template>
      <template>HP Procurve 2920</template>
      <name>HP Procurve 2920</name>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Events</name>
        </application>
        <application>
          <name>Interfaces</name>
        </application>
        <application>
          <name>System</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Fan Status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.11.2.14.11.1.2.6.1.4.1</snmp_oid>
          <key>FAN-Status</key>
          <delay>80</delay>
          <history>7d</history>
          <trends>100d</trends>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{prev(0)}&lt;4</expression>
              <name>FAN status changed on {HOSTNAME}</name>
              <priority>DISASTER</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>HP: Base MAC</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.11.2.14.11.5.1.1.6.0</snmp_oid>
          <key>hpBaseMac</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>MACADDRESS_A</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>HP: Serial Number</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.11.2.36.1.1.2.9.0</snmp_oid>
          <key>hpHttpMgSerialNumber</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>SERIALNO_A</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>HP: CPU utilization</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.11.2.14.11.5.1.9.6.1.0</snmp_oid>
          <key>hpSwitchCpuStat</key>
          <delay>60</delay>
          <history>7d</history>
          <trends>90d</trends>
          <units>%</units>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0,60)}>94</expression>
              <name>95% CPU on {HOSTNAME}</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>HP: Switch Os Version</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.11.2.14.11.5.1.1.3.0</snmp_oid>
          <key>hpSwitchOsVersion</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>SOFTWARE</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>HP: Switch Type</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.4.1.11.2.14.11.1.2.4.1.4.1</snmp_oid>
          <key>hpSwitchType</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <inventory_link>HARDWARE</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ping</name>
          <type>SIMPLE</type>
          <key>icmpping</key>
          <delay>30</delay>
          <history>30d</history>
          <trends>90d</trends>
          <applications>
            <application>
              <name>Events</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{max(90)}=0</expression>
              <name>{HOSTNAME} Unreachable</name>
              <priority>DISASTER</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>SNMP Trap Events</name>
          <type>SNMP_TRAP</type>
          <key>snmptrap.fallback</key>
          <delay>0</delay>
          <trends>0</trends>
          <value_type>LOG</value_type>
          <applications>
            <application>
              <name>Events</name>
            </application>
          </applications>
          <logtimefmt>hh:mm:ss dd/mm/yyyy</logtimefmt>
        </item>
        <item>
          <name>Interface status change</name>
          <type>SNMP_TRAP</type>
          <key>snmptrap[@netIF_OperStatChange]</key>
          <delay>0</delay>
          <trends>0</trends>
          <value_type>LOG</value_type>
          <applications>
            <application>
              <name>Events</name>
            </application>
          </applications>
          <logtimefmt>hh:mm:ss dd/mm/yyyy</logtimefmt>
        </item>
        <item>
          <name>ColdStart detected</name>
          <type>SNMP_TRAP</type>
          <key>snmptrap[^.*coldStart.*]</key>
          <delay>0</delay>
          <trends>0</trends>
          <value_type>LOG</value_type>
          <applications>
            <application>
              <name>Events</name>
            </application>
            <application>
              <name>System</name>
            </application>
          </applications>
          <logtimefmt>hh:mm:ss dd/mm/yyyy</logtimefmt>
          <triggers>
            <trigger>
              <expression>(({regexp(^.*Failure.*$)})&lt;>0)</expression>
              <name>Coldstart detected</name>
              <priority>INFO</priority>
              <type>MULTIPLE</type>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>SysContact</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.4.0</snmp_oid>
          <key>SysContact</key>
          <delay>30</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <inventory_link>CONTACT</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>SysLocation</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.6.0</snmp_oid>
          <key>SysLocation</key>
          <delay>30</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <inventory_link>LOCATION</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>System Memory Free left</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>NETSWITCH-MIB::hpLocalMemFreeBytes.1</snmp_oid>
          <key>sysMemFreeLeft</key>
          <delay>20</delay>
          <history>7d</history>
          <units>bytes</units>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{max(#5)}&lt;8096</expression>
              <name>Low memory</name>
              <priority>WARNING</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>SysName</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.5.0</snmp_oid>
          <key>SysName</key>
          <delay>30</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <inventory_link>NAME</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>SysUptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.3.0</snmp_oid>
          <key>SysUptime</key>
          <delay>30</delay>
          <history>7d</history>
          <trends>60d</trends>
          <value_type>FLOAT</value_type>
          <units>uptime</units>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.01</params>
            </step>
          </preprocessing>
          <triggers>
            <trigger>
              <expression>{last(0)}&lt;1000</expression>
              <name>{HOSTNAME} Has been restarted</name>
              <priority>INFO</priority>
            </trigger>
          </triggers>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Interfaces</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},IF-MIB::ifDescr]</snmp_oid>
          <key>ifDescr</key>
          <delay>120</delay>
          <filter>
            <evaltype>OR</evaltype>
            <conditions>
              <condition>
                <macro>{#SNMPVALUE}</macro>
                <value>^[0-9]{1,3}$</value>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <item_prototypes>
            <item_prototype>
              <name>Admin status of interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifAdminStatus.{#SNMPINDEX}</snmp_oid>
              <key>ifAdminStatus[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>7d</history>
              <trends>90d</trends>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Alias of interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifAlias.{#SNMPINDEX}</snmp_oid>
              <key>ifAlias[{#SNMPVALUE}]</key>
              <delay>3600</delay>
              <history>7d</history>
              <trends>0</trends>
              <status>DISABLED</status>
              <value_type>CHAR</value_type>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Description of interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifDescr.{#SNMPINDEX}</snmp_oid>
              <key>ifDescr[{#SNMPVALUE}]</key>
              <delay>3600</delay>
              <history>7d</history>
              <trends>0</trends>
              <status>DISABLED</status>
              <value_type>CHAR</value_type>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Incoming traffic on interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifInOctets.{#SNMPINDEX}</snmp_oid>
              <key>ifInOctets[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>30d</history>
              <trends>90d</trends>
              <units>bps</units>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
                <step>
                  <type>MULTIPLIER</type>
                  <params>8</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>Operational status of interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifOperStatus.{#SNMPINDEX}</snmp_oid>
              <key>ifOperStatus[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>30d</history>
              <trends>90d</trends>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <valuemap>
                <name>SNMP interface status (ifOperStatus)</name>
              </valuemap>
            </item_prototype>
            <item_prototype>
              <name>Outgoing traffic on interface $1</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifOutOctets.{#SNMPINDEX}</snmp_oid>
              <key>ifOutOctets[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>30d</history>
              <trends>90d</trends>
              <units>bps</units>
              <applications>
                <application>
                  <name>Interfaces</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>CHANGE_PER_SECOND</type>
                  <params></params>
                </step>
                <step>
                  <type>MULTIPLIER</type>
                  <params>8</params>
                </step>
              </preprocessing>
            </item_prototype>
          </item_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>Traffic on interface {#SNMPVALUE}</name>
              <width>300</width>
              <graph_items>
                <graph_item>
                  <color>0000EE</color>
                  <item>
                    <host>HP Procurve 2920</host>
                    <key>ifInOctets[{#SNMPVALUE}]</key>
