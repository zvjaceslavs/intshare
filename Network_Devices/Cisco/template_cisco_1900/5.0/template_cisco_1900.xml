        <item>
          <name>Tempo Ligado</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>SNMPv2-MIB::sysUpTime.0</snmp_oid>
          <key>sysUpTime</key>
          <delay>60</delay>
          <history>7d</history>
          <units>uptime</units>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.01</params>
            </step>
          </preprocessing>
        </item>
      </items>
