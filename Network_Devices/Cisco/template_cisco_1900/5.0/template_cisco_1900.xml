      <items>
        <item>
          <name>Disponibilidade</name>
          <type>SIMPLE</type>
          <key>icmpping</key>
          <delay>30</delay>
          <history>7d</history>
          <value_type>FLOAT</value_type>
          <applications>
            <application>
              <name>Disponibilidade</name>
            </application>
          </applications>
        </item>
