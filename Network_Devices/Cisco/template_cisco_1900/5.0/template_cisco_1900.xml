        </item>
        <item>
          <name>Tráfego de Entrada GigabitEthernet 0/0</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>IF-MIB::ifInOctets.1</snmp_oid>
          <key>ifInOctets.1</key>
          <delay>60</delay>
          <units>bps</units>
          <applications>
            <application>
              <name>Tráfego de Entrada</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>CHANGE_PER_SECOND</type>
              <params></params>
            </step>
            <step>
              <type>MULTIPLIER</type>
              <params>8</params>
            </step>
