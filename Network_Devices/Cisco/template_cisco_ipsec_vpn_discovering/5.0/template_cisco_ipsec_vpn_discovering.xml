<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-26T04:41:33Z</date>
  <groups>
    <group>
      <name>Templates SNMP Devices</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Cisco IPsec VPN discovering</template>
      <name>Cisco IPsec VPN discovering</name>
      <groups>
        <group>
          <name>Templates SNMP Devices</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>IPsec VPN items</name>
        </application>
      </applications>
      <discovery_rules>
        <discovery_rule>
          <name>VPN Tunnels</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#LOCAL},CISCO-IPSEC-FLOW-MONITOR-MIB::cikeTunLocalValue,{#REMOTE},CISCO-IPSEC-FLOW-MONITOR-MIB::cikeTunRemoteValue]</snmp_oid>
          <key>cikeTunActiveTime</key>
          <delay>10m</delay>
          <lifetime>3d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>Active time of tunnel {#LOCAL}---{#REMOTE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>CISCO-IPSEC-FLOW-MONITOR-MIB::cikeTunActiveTime.{#SNMPINDEX}</snmp_oid>
              <key>cikeTunActiveTime[{#LOCAL}---{#REMOTE}]</key>
              <history>1w</history>
              <trends>90d</trends>
              <units>hundredths of seconds</units>
              <applications>
                <application>
                  <name>IPsec VPN items</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>In bytes of tunnel {#LOCAL}---{#REMOTE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>CISCO-IPSEC-FLOW-MONITOR-MIB::cikeTunInOctets.{#SNMPINDEX}</snmp_oid>
              <key>cikeTunInOctets[{#LOCAL}---{#REMOTE}]</key>
              <history>1w</history>
              <trends>90d</trends>
              <units>bytes</units>
              <applications>
                <application>
                  <name>IPsec VPN items</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>In packets of tunnel {#LOCAL}---{#REMOTE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>CISCO-IPSEC-FLOW-MONITOR-MIB::cikeTunInPkts.{#SNMPINDEX}</snmp_oid>
              <key>cikeTunInPkts[{#LOCAL}---{#REMOTE}]</key>
              <history>1w</history>
              <trends>90d</trends>
              <units>Packets</units>
              <applications>
                <application>
                  <name>IPsec VPN items</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Out bytes of tunnel {#LOCAL}---{#REMOTE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>CISCO-IPSEC-FLOW-MONITOR-MIB::cikeTunOutOctets.{#SNMPINDEX}</snmp_oid>
              <key>cikeTunOutOctets[{#LOCAL}---{#REMOTE}]</key>
              <history>1w</history>
              <trends>90d</trends>
              <units>bytes</units>
              <applications>
                <application>
                  <name>IPsec VPN items</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Out packets of tunnel {#LOCAL}---{#REMOTE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>CISCO-IPSEC-FLOW-MONITOR-MIB::cikeTunOutPkts.{#SNMPINDEX}</snmp_oid>
              <key>cikeTunOutPkts[{#LOCAL}---{#REMOTE}]</key>
              <history>1w</history>
              <trends>90d</trends>
              <units>Packets</units>
              <applications>
                <application>
                  <name>IPsec VPN items</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Status of tunnel {#LOCAL}---{#REMOTE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>CISCO-IPSEC-FLOW-MONITOR-MIB::cikeTunStatus.{#SNMPINDEX}</snmp_oid>
              <key>cikeTunStatus[{#LOCAL}---{#REMOTE}]</key>
              <history>1w</history>
              <trends>90d</trends>
              <applications>
                <application>
                  <name>IPsec VPN items</name>
                </application>
              </applications>
              <request_method>POST</request_method>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}=0 or {nodata(3m)}=1</expression>
                  <name>Status of tunnel {#LOCAL}---{#REMOTE} not up or no data received</name>
                  <priority>WARNING</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
          <request_method>POST</request_method>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
</zabbix_export>