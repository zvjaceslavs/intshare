<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-19T21:21:16Z</date>
  <groups>
    <group>
      <name>Templates Prod</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template SNMP Oracle SL3000</template>
      <name>Template SNMP Oracle SL3000</name>
      <groups>
        <group>
          <name>Templates Prod</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Drives</name>
        </application>
        <application>
          <name>General</name>
        </application>
        <application>
          <name>Robot</name>
        </application>
      </applications>
      <items>
        <item>
          <name>CapAccessibility (open allow/prevent)</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slCapAccessibility.1</snmp_oid>
          <key>slCapAccessibility</key>
          <delay>30</delay>
          <history>14d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>CapState</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slCapState.1</snmp_oid>
          <key>slCapState</key>
          <delay>30</delay>
          <history>14d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
        <item>
          <name>ControllerHaId[1] (0=active/1=standby)</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slControllerHaId.1</snmp_oid>
          <key>slControllerHaId</key>
          <delay>30</delay>
          <history>14d</history>
          <trends>30d</trends>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{abschange()}=1</expression>
              <name>ControllerHA_changestatus</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>TapeCount</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slTapeCount.0</snmp_oid>
          <key>slTapeCount</key>
          <delay>30</delay>
          <history>14d</history>
          <trends>30d</trends>
          <applications>
            <application>
              <name>General</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Drive</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},STREAMLINE-TAPE-LIBRARY-MIB::slDriveIndex]</snmp_oid>
          <key>DriveIndex</key>
          <delay>30</delay>
          <item_prototypes>
            <item_prototype>
              <name>DriveCellContentLabel[{#SNMPVALUE}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slDriveCellContentLabel.{#SNMPVALUE}</snmp_oid>
              <key>DriveCellContentLabel[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>30d</history>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <description>Label of the cartridge in the drive</description>
              <applications>
                <application>
                  <name>Drives</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>DriveFibreNodeName[{#SNMPVALUE}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slDriveFibreNodeName.{#SNMPVALUE}</snmp_oid>
              <key>DriveFibreNodeName[{#SNMPVALUE}]</key>
              <delay>600</delay>
              <history>30d</history>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Drives</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Drive[{#SNMPVALUE}]SerialNum</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slDriveSerialNum.{#SNMPVALUE}</snmp_oid>
              <key>DriveSerialNum[{#SNMPVALUE}]</key>
              <delay>600</delay>
              <history>30d</history>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Drives</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>DriveState[{#SNMPVALUE}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slDriveState.{#SNMPVALUE}</snmp_oid>
              <key>DriveState[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>30d</history>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Drives</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{str(empty)}&lt;>1 and {str(loaded)}&lt;>1 and {str(unloading)}&lt;>1</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{str(empty)}=1 or {str(loaded)}=1 or {str(unloading)}=1</recovery_expression>
                  <name>DriveState[{#SNMPVALUE}] is {ITEM.VALUE1}</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>DriveType[{#SNMPVALUE}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slDriveType.{#SNMPVALUE}</snmp_oid>
              <key>DriveType[{#SNMPVALUE}]</key>
              <delay>600</delay>
              <history>30d</history>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Drives</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Robot</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},STREAMLINE-TAPE-LIBRARY-MIB::slRobotIndex]</snmp_oid>
          <key>Robotindex</key>
          <delay>30</delay>
          <item_prototypes>
            <item_prototype>
              <name>State of the robot [{#SNMPVALUE}] hand (cartridge =1, no cartridge =0)</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slRobotHandCartStatus.{#SNMPVALUE}</snmp_oid>
              <key>RobotHandCartStatus[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>30d</history>
              <trends>90d</trends>
              <description>State of the robot hand regarding a cartridge (cartridge =1, no cartridge =0)</description>
              <applications>
                <application>
                  <name>Robot</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>RobotState[{#SNMPVALUE}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>STREAMLINE-TAPE-LIBRARY-MIB::slRobotState.{#SNMPVALUE}</snmp_oid>
              <key>RobotState[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <history>30d</history>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Robot</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{str(Error)}=1</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{str(Ready)}=1</recovery_expression>
                  <name>RobotState[{#SNMPVALUE}] is {ITEM.VALUE1}</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
      <macros>
        <macro>
          <macro>{$SNMP_COMMUNITY}</macro>
          <value>snmpTech50</value>
        </macro>
      </macros>
    </template>
  </templates>
</zabbix_export>