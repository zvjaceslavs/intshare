              <type>SNMP_AGENT</type>
              <snmp_oid>volumeStatus.{#SNMPINDEX}</snmp_oid>
              <key>volumeStatus[{#SNMPVALUE}]</key>
              <delay>30</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Volume</name>
