<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T10:56:34Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Mitel MiVoice Office 250</template>
      <name>Mitel MiVoice Office 250</name>
      <description>For Mitel MiVoice Office 250 phone system, fka Mitel 5000, fka Inter-Tel. Requires access from Zabbix server to the web interface of the phone system.</description>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <items>
        <item>
          <name>Active calls</name>
          <type>DEPENDENT</type>
          <key>mitel.mivo250.activecalls</key>
          <delay>0</delay>
          <history>7d</history>
          <description>MiVO 250 Active calls</description>
          <preprocessing>
            <step>
              <type>XMLPATH</type>
              <params>/diag/data/dump/num_active_calls/text()</params>
            </step>
          </preprocessing>
          <master_item>
            <key>mitel.mivo250.status</key>
          </master_item>
        </item>
        <item>
          <name>Alarm status</name>
          <type>DEPENDENT</type>
          <key>mitel.mivo250.alarm</key>
          <delay>0</delay>
          <history>7d</history>
          <description>MiVO 250 Alarm status</description>
          <preprocessing>
            <step>
              <type>XMLPATH</type>
              <params>/diag/data/dump/alarm/status/text()</params>
            </step>
          </preprocessing>
          <master_item>
            <key>mitel.mivo250.status</key>
          </master_item>
          <triggers>
            <trigger>
              <expression>{last()}&lt;>0</expression>
              <name>Alarm status is non-zero</name>
              <priority>WARNING</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Call Processing version</name>
          <type>DEPENDENT</type>
          <key>mitel.mivo250.cpver</key>
          <delay>0</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>MiVO 250 Call Processing version</description>
          <preprocessing>
            <step>
              <type>XMLPATH</type>
              <params>/diag/data/dump/cp_version/text()</params>
            </step>
          </preprocessing>
          <master_item>
            <key>mitel.mivo250.status</key>
          </master_item>
        </item>
        <item>
          <name>Percent free on flash card</name>
          <type>DEPENDENT</type>
          <key>mitel.mivo250.flashpercfree</key>
          <delay>0</delay>
          <history>7d</history>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <description>MiVO 250 free space on flash in bytes</description>
          <preprocessing>
            <step>
              <type>XMLPATH</type>
              <params>100 * (/diag/data/dump/flash/available div (/diag/data/dump/flash/used + /diag/data/dump/flash/available))</params>
            </step>
          </preprocessing>
          <master_item>
            <key>mitel.mivo250.status</key>
          </master_item>
          <triggers>
            <trigger>
              <expression>{last()}&lt;10</expression>
              <name>Less than 10% free on flash card</name>
              <priority>WARNING</priority>
              <description>less than 10% disk space on flash card remaining. Perhaps delete some voicemails.</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Free space on flash card</name>
          <type>DEPENDENT</type>
          <key>mitel.mivo250.flashspace</key>
          <delay>0</delay>
          <history>7d</history>
          <status>DISABLED</status>
          <units>B</units>
          <description>MiVO 250 free space on flash in bytes</description>
          <preprocessing>
            <step>
              <type>XMLPATH</type>
              <params>/diag/data/dump/flash/available/text()</params>
            </step>
          </preprocessing>
          <master_item>
            <key>mitel.mivo250.status</key>
          </master_item>
        </item>
        <item>
          <name>License status</name>
          <type>DEPENDENT</type>
          <key>mitel.mivo250.licstat</key>
          <delay>0</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>MiVO 250 License Status</description>
          <preprocessing>
            <step>
              <type>XMLPATH</type>
              <params>/diag/data/dump/license_status/text()</params>
            </step>
          </preprocessing>
          <master_item>
            <key>mitel.mivo250.status</key>
          </master_item>
        </item>
        <item>
          <name>Release version</name>
          <type>DEPENDENT</type>
          <key>mitel.mivo250.relver</key>
          <delay>0</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>MiVO 250 Release version</description>
          <preprocessing>
            <step>
              <type>XMLPATH</type>
              <params>/diag/data/dump/release_version/text()</params>
            </step>
          </preprocessing>
          <master_item>
            <key>mitel.mivo250.status</key>
          </master_item>
        </item>
        <item>
          <name>MiVO 250 Status blob</name>
          <type>HTTP_AGENT</type>
          <key>mitel.mivo250.status</key>
          <delay>180s</delay>
          <history>7d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <timeout>15s</timeout>
          <url>https://{HOST.CONN}:{$AWS.PORT}/diag_connect.cgi</url>
        </item>
      </items>
      <macros>
        <macro>
          <macro>{$AWS.PORT}</macro>
          <value>443</value>
        </macro>
      </macros>
    </template>
  </templates>
</zabbix_export>