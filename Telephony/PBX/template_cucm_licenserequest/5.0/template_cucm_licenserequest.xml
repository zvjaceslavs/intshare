          <type>HTTP_AGENT</type>
          <key>uclicense</key>
          <delay>300s</delay>
          <history>0</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <authtype>BASIC</authtype>
          <username>{$ZABBIX_APIUSER_UC}</username>
          <password>{$ZABBIX_APIPASS_UC}</password>
          <applications>
            <application>
              <name>API License monitoring</name>
            </application>
          </applications>
          <timeout>5s</timeout>
          <url>https://{HOST.IP}:8443/axl/</url>
          <posts>&lt;soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:ns="http://www.cisco.com/AXL/API/11.5"> &lt;soapenv:Header/> &lt;soapenv:Body> &lt;ns:executeSQLQuery> &lt;sql> SELECT name,value FROM TABLE (FUNCTION LicenseTotals()) (pkid,name,value,UserValue,DeviceValue) &lt;/sql> &lt;/ns:executeSQLQuery> &lt;/soapenv:Body> &lt;/soapenv:Envelope></posts>
          <post_type>XML</post_type>
          <headers>
            <header>
              <name>Content-Type</name>
              <value>text/xml</value>
            </header>
            <header>
              <name>SOAPAction</name>
              <value>CUCM:DB ver=11.5 executeSQLQuery</value>
            </header>
          </headers>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>Basic</name>
          <type>DEPENDENT</type>
          <key>uclicense_basic</key>
          <delay>0</delay>
          <history>2d</history>
          <trends>0</trends>
          <applications>
            <application>
              <name>API License monitoring</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>(.*&lt;name>Basic &lt;/name>&lt;value>)(\d?)(&lt;/value>.*) \2</params>
            </step>
            <step>
              <type>JAVASCRIPT</type>
              <params>value = Number(value) return value</params>
            </step>
          </preprocessing>
          <master_item>
            <key>uclicense</key>
          </master_item>
        </item>
