<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-11-09T08:39:00Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Mitel MiVoice Business</template>
      <name>Mitel MiVoice Business</name>
      <description>## Overview Template for Mitel MiVoice Business, formerly known as Mitel Communications Director and Mitel 3300. Uses SNMP to fetch various Mitel OIDs, most importantly the Alarm State. Tested with a range of firmware releases from 10.x to 14.x. Fetches: * Alarm state * Number of categories in alarm state * User licenses purchased * User licenses used * Disk reads * Disk writes * 1min CPU average * Hardware platform * Software version Triggers: * Alarm state</description>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <items>
        <item>
          <name>MCD alarm state</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1027.4.1.1.2.2.1.0</snmp_oid>
          <key>mitelIpera3000AlmLevel</key>
          <delay>2m</delay>
          <history>1w</history>
          <description>Alarm level as reported by MCD. Requires investigation.</description>
          <valuemap>
            <name>MCD alarm level</name>
          </valuemap>
          <request_method>POST</request_method>
          <triggers>
            <trigger>
              <expression>{last()}&lt;>1 and ({time(0)}>073000 and {time(0)}&lt;180000)</expression>
              <name>MCD alarm state on {HOSTNAME}</name>
              <priority>AVERAGE</priority>
              <description>The current overall alarm level for the call server. This trigger is constrained to only fire from 0730 to 1800</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Number of categories in alarm state</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.1027.4.1.1.2.2.3.0</snmp_oid>
          <key>mitelIpera3000AlmNbrCategories.0</key>
          <delay>2m</delay>
          <history>1w</history>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>First alarm category</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1027.4.1.1.2.2.4.1.8.1</snmp_oid>
          <key>mitelIpera3000CatTblName.1</key>
          <delay>300s</delay>
          <history>1w</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>User licenses purchased</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1027.4.1.1.2.1.2.1.0</snmp_oid>
          <key>mitelIpera3000IPUsrLicPurchased</key>
          <delay>2m</delay>
          <history>1w</history>
          <value_type>FLOAT</value_type>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>User licenses used</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1027.4.1.1.2.1.2.2.0</snmp_oid>
          <key>mitelIpera3000IPUsrLicUsed</key>
          <delay>2m</delay>
          <history>1w</history>
          <value_type>FLOAT</value_type>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>Disk reads</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1027.4.1.1.2.1.6.1.4.1.1.1</snmp_oid>
          <key>mitelResPerfDiskReads.1</key>
          <delay>2m</delay>
          <history>1w</history>
          <description>mitelResPerfDiskReads OBJECT-TYPE SYNTAX Unsigned32 MAX-ACCESS read-only STATUS current DESCRIPTION "The number of read accesses to this disk since the system reset. " ::= { mitelResDiskPerfEntry 1 }</description>
          <preprocessing>
            <step>
              <type>CHANGE_PER_SECOND</type>
              <params></params>
            </step>
          </preprocessing>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>Disk writes</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1027.4.1.1.2.1.6.1.4.1.2.1</snmp_oid>
          <key>mitelResPerfDiskWrites.1</key>
          <delay>2m</delay>
          <history>1w</history>
          <description>mitelResPerfDiskWrites OBJECT-TYPE SYNTAX Unsigned32 MAX-ACCESS read-only STATUS current DESCRIPTION "The number of write access to this disk since the system reset. " ::= { mitelResDiskPerfEntry 2 }</description>
          <preprocessing>
            <step>
              <type>CHANGE_PER_SECOND</type>
              <params></params>
            </step>
          </preprocessing>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>1 minute CPU average</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.1027.4.1.1.2.1.6.1.1.6.0</snmp_oid>
          <key>mitelResSystemCallLoad.0</key>
          <delay>2m</delay>
          <history>1w</history>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <description>DESCRIPTION "The average, over the last minute, the percentage of time that this processor was processing calls. Percentage value (from 0.0 to 100.0) including a single decimal place."</description>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>Mitel hardware platform</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.1.0</snmp_oid>
          <key>VerHw</key>
          <delay>1h</delay>
          <history>1w</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>VerHw substring from SNMPv2-MIB::sysDescr.0 = STRING: VerAg:07.00.00.01.00;VerSw:12.0.1.24;VerHw:CX;VerPl:3300 ICP;VerMCD:6.0 SP1</description>
          <inventory_link>HARDWARE_FULL</inventory_link>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>VerHw:(.+);VerPl \1</params>
            </step>
          </preprocessing>
          <request_method>POST</request_method>
        </item>
        <item>
          <name>Mitel Software platform</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.1.1.0</snmp_oid>
          <key>VerSw</key>
          <delay>1h</delay>
          <history>1w</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>VerSw substring from SNMPv2-MIB::sysDescr.0 = STRING: VerAg:07.00.00.01.00;VerSw:12.0.1.24;VerHw:CX;VerPl:3300 ICP;VerMCD:6.0 SP1</description>
          <inventory_link>SOFTWARE</inventory_link>
          <preprocessing>
            <step>
              <type>REGEX</type>
              <params>VerSw:(.+);VerHw \1</params>
            </step>
          </preprocessing>
          <request_method>POST</request_method>
        </item>
      </items>
    </template>
  </templates>
