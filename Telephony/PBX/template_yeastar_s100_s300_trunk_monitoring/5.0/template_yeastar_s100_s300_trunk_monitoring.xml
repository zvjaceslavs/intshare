      <discovery_rules>
        <discovery_rule>
          <name>Trunk Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#TRUNK_INDEX},YEASTAR-S-SERIES-V4::sTrunksIndex,{#TRUNK_NAME},YEASTAR-S-SERIES-V4::sTrunksName]</snmp_oid>
          <key>sTrunks</key>
          <item_prototypes>
            <item_prototype>
              <name>Trunk {#TRUNK_NAME} Status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>YEASTAR-S-SERIES-V4::sTrunksStatus.{#TRUNK_INDEX}</snmp_oid>
              <key>sTrunksStatus[{#TRUNK_INDEX}]</key>
              <trends>0</trends>
              <value_type>CHAR</value_type>
              <applications>
                <application>
                  <name>Trunks</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{diff()}=1</expression>
                  <name>{HOSTNAME} Trunk {#TRUNK_NAME} changed status</name>
                  <priority>HIGH</priority>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
