              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{diff()}=1</expression>
                  <name>Operational status was changed on {HOST.NAME} interface {#SNMPVALUE}</name>
                  <priority>INFO</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
            <item_prototype>
              <name>Type of device $1</name>
