        <item>
          <name>h150e load level</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.231.7.2.9.1.7.0</snmp_oid>
          <key>loadLevel</key>
          <delay>30</delay>
