          <inventory_link>SOFTWARE_FULL</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Devices</name>
          <type>SNMP_AGENT</type>
