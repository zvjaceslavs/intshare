          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>h150e sys state</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.231.7.2.9.1.1.0</snmp_oid>
          <key>sysState</key>
          <delay>30</delay>
          <description>The actual error state of the device. Contains the highest severity level of the recent error events. This object is updated automatically, but it can also be modified manually.</description>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <valuemap>
            <name>HiPath SysState</name>
          </valuemap>
          <triggers>
            <trigger>
              <expression>{last()}>1 and {last()}&lt;5</expression>
              <name>System error state</name>
              <priority>WARNING</priority>
              <description>The actual error state of the device. Contains the highest severity level of the recent error events. This object is updated automatically, but it can also be modified manually.</description>
            </trigger>
            <trigger>
              <expression>{last()}=5</expression>
              <name>System error state Critical</name>
              <priority>HIGH</priority>
              <description>The actual error state of the device. Contains the highest severity level of the recent error events. This object is updated automatically, but it can also be modified manually.</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>sys software version</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.231.7.2.9.3.2.0</snmp_oid>
          <key>sysVersion</key>
          <delay>3600</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
