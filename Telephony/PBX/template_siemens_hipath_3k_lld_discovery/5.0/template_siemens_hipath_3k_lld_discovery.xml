<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-26T04:41:02Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>aa Siemens Hipath</template>
      <name>aa Siemens Hipath</name>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Card</name>
        </application>
        <application>
          <name>Port</name>
        </application>
        <application>
          <name>System</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Processor Load Level</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.231.7.2.9.1.7.0</snmp_oid>
          <key>h150eLoadLevel</key>
          <delay>30s</delay>
          <units>%</units>
          <description>The actual CPU load level (per cent).</description>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Hardware Version</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.231.7.2.9.3.1.0</snmp_oid>
          <key>sysHardwareVersion</key>
          <delay>240s</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <inventory_link>HARDWARE_FULL</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Software Version</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.231.7.2.9.3.2.0</snmp_oid>
          <key>sysSoftwareVersion</key>
          <delay>240s</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>Contains the version string of the system software.</description>
          <inventory_link>SOFTWARE</inventory_link>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
        </item>
        <item>
          <name>System State</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.231.7.2.9.1.1.0</snmp_oid>
          <key>sysState</key>
          <delay>60s</delay>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <valuemap>
            <name>HiPath SysState</name>
          </valuemap>
          <triggers>
            <trigger>
              <expression>{last()}=2</expression>
              <name>System Error State 2 Warning</name>
              <priority>WARNING</priority>
            </trigger>
            <trigger>
              <expression>{last()}=3</expression>
              <name>System Error State 3 Minor</name>
              <priority>WARNING</priority>
            </trigger>
            <trigger>
              <expression>{last()}=4</expression>
              <name>System Error State 4 Major</name>
              <priority>WARNING</priority>
            </trigger>
            <trigger>
              <expression>{last()}=5</expression>
              <name>System Error State 5 Critical</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>iso.3.6.1.2.1.1.3.0</snmp_oid>
          <key>sysUpTime</key>
          <delay>90s</delay>
          <units>uptime</units>
          <applications>
            <application>
              <name>System</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.01</params>
            </step>
          </preprocessing>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Discovery Siemens Card</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#CARDTYPE},1.3.6.1.4.1.231.7.2.9.3.6.1.4,{#CARDSTATE},1.3.6.1.4.1.231.7.2.9.3.6.1.7,{#PORTCARDINDEX},1.3.6.1.4.1.231.7.2.9.3.8.1.2,{#PORTTYPE},1.3.6.1.4.1.231.7.2.9.3.8.1.3,{#PORTSTATE},1.3.6.1.4.1.231.7.2.9.3.8.1.4,{#CARDCODENUMBER},1.3.6.1.4.1.231.7.2.9.3.6.1.6]</snmp_oid>
          <key>h150eCards</key>
          <delay>60s</delay>
          <lifetime>2d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>Slot Card Serial Number {#CARDTYPE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.231.7.2.9.3.6.1.6.{#SNMPINDEX}</snmp_oid>
              <key>cardCodeNumber[{#CARDTYPE}]</key>
              <delay>60s</delay>
              <trends>0</trends>
              <value_type>TEXT</value_type>
              <applications>
                <application>
                  <name>Card</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Slot Card Number {#CARDTYPE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.231.7.2.9.3.6.1.3.{#SNMPINDEX}</snmp_oid>
              <key>cardSlotNum[{#CARDTYPE}]</key>
              <delay>60s</delay>
              <applications>
                <application>
                  <name>Card</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>Slot Card State {#CARDTYPE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.231.7.2.9.3.6.1.7.{#SNMPINDEX}</snmp_oid>
              <key>cardState[{#CARDTYPE}]</key>
              <delay>60s</delay>
              <applications>
                <application>
                  <name>Card</name>
                </application>
              </applications>
              <valuemap>
                <name>HiPath SysState</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{prev()}>1</expression>
                  <name>CardState Problem {#CARDTYPE}</name>
                  <priority>WARNING</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Discovery Siemens Port on TMCAS2</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#PORTCARDINDEX},1.3.6.1.4.1.231.7.2.9.3.8.1.2,{#PORTTYPE},1.3.6.1.4.1.231.7.2.9.3.8.1.3,{#PORTSTATE},1.3.6.1.4.1.231.7.2.9.3.8.1.4]</snmp_oid>
          <key>h150ePorts</key>
          <delay>60s</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#PORTSTATE}</macro>
                <value>2</value>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>2d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>StatusPort {#PORTTYPE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.231.7.2.9.3.8.1.4.{#SNMPINDEX}</snmp_oid>
              <key>portStatusIndex.[{#SNMPINDEX}]</key>
              <delay>30s</delay>
              <applications>
                <application>
                  <name>Port</name>
                </application>
              </applications>
              <valuemap>
                <name>HiPath State</name>
              </valuemap>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{prev()}&lt;>2</expression>
                  <name>StatusPort Inactive On TMCAS2 {#PORTTYPE}</name>
                  <priority>WARNING</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
      <macros>
        <macro>
          <macro>{$SNMP_COMMUNITY}</macro>
          <value>public</value>
        </macro>
      </macros>
    </template>
  </templates>
  <value_maps>
    <value_map>
      <name>HiPath State</name>
      <mappings>
        <mapping>
          <value>1</value>
          <newvalue>inactive</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>active</newvalue>
        </mapping>
      </mappings>
    </value_map>
    <value_map>
      <name>HiPath SysState</name>
      <mappings>
        <mapping>
          <value>1</value>
          <newvalue>normal</newvalue>
        </mapping>
        <mapping>
          <value>2</value>
          <newvalue>warning</newvalue>
        </mapping>
        <mapping>
          <value>3</value>
          <newvalue>minor</newvalue>
        </mapping>
        <mapping>
          <value>4</value>
          <newvalue>major</newvalue>
        </mapping>
        <mapping>
          <value>5</value>
          <newvalue>critical</newvalue>
        </mapping>
      </mappings>
    </value_map>
  </value_maps>
</zabbix_export>