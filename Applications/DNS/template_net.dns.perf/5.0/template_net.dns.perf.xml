<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T10:07:26Z</date>
  <groups>
    <group>
      <name>自宅ラックテンプレート</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>net.dns.perf</template>
      <name>net.dns.perf</name>
      <description>DNS応答時間測定</description>
      <groups>
        <group>
          <name>自宅ラックテンプレート</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>DNS</name>
        </application>
      </applications>
      <items>
        <item>
          <name>net.dns.perf</name>
          <type>ZABBIX_ACTIVE</type>
          <key>system.run[dig {$DNS_SERVER} {$DOMAIN_NAME} |grep Query |awk 'END{print$4}']</key>
          <delay>600</delay>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <applications>
            <application>
              <name>DNS</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.001</params>
            </step>
          </preprocessing>
        </item>
      </items>
      <macros>
        <macro>
          <macro>{$DNS_SERVER}</macro>
          <value>8.8.8.8</value>
        </macro>
        <macro>
          <macro>{$DOMAIN_NAME}</macro>
          <value>yahoo.co.jp</value>
        </macro>
      </macros>
    </template>
  </templates>
</zabbix_export>