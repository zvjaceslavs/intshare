  <date>2021-10-22T08:00:19Z</date>
  <groups>
    <group>
      <name>A-OVR_TPLGPR</name>
    </group>
    <group>
      <name>EXT_TPLGRP/Software</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template VEEAM-Agents</template>
      <name>Template VEEAM-Agents</name>
      <groups>
        <group>
          <name>A-OVR_TPLGPR</name>
        </group>
        <group>
          <name>EXT_TPLGRP/Software</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>VeeamAgent_EventLog</name>
        </application>
        <application>
          <name>VeeamAgent_Service</name>
        </application>
      </applications>
      <items>
        <item>
          <name>VeeamAgent_EventLog_JobResult</name>
          <type>ZABBIX_ACTIVE</type>
          <key>eventlog[Veeam Agent,,,Veeam Agent,190,,skip]</key>
          <delay>5m</delay>
          <history>30d</history>
          <trends>0</trends>
          <value_type>LOG</value_type>
          <applications>
            <application>
              <name>VeeamAgent_EventLog</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{nodata(604800)}=1</expression>
              <name>VeeamAgent - No job result since 1 Week</name>
              <priority>HIGH</priority>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{logeventid(190)}=1 and {logseverity()}=1</expression>
              <name>VeeamAgent Job completed successfull</name>
              <priority>INFO</priority>
              <description>{ITEM.LASTVALUE1} {ITEM.LASTVALUE2} {ITEM.LASTVALUE3}</description>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{logeventid(190)}=1 and {logseverity()}=2 and {iregexp(low on free disk space)}=1</expression>
              <name>VeeamAgent Job completed successfull (warning about free space)</name>
              <priority>INFO</priority>
              <description>{ITEM.LASTVALUE1} {ITEM.LASTVALUE2} {ITEM.LASTVALUE3}</description>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{logeventid(190)}=1 and {logseverity()}=4</expression>
              <name>VeeamAgent Job completed with error</name>
              <priority>HIGH</priority>
              <description>{ITEM.LASTVALUE1} {ITEM.LASTVALUE2} {ITEM.LASTVALUE3}</description>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{logeventid(190)}=1 and {logseverity()}=2 and not {logeventid(low on free disk space)}=0</expression>
              <name>VeeamAgent Job completed with warning</name>
              <priority>AVERAGE</priority>
              <description>{ITEM.LASTVALUE1} {ITEM.LASTVALUE2} {ITEM.LASTVALUE3}</description>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>VeeamAgent_EventLog_JobRetry</name>
          <type>ZABBIX_ACTIVE</type>
          <key>eventlog[Veeam Agent,,,Veeam Agent,191,,skip]</key>
          <delay>5m</delay>
          <history>30d</history>
          <trends>0</trends>
          <value_type>LOG</value_type>
          <applications>
            <application>
              <name>VeeamAgent_EventLog</name>
            </application>
          </applications>
        </item>
        <item>
          <name>VeeamAgent_EventLog_Synchronisation</name>
          <type>ZABBIX_ACTIVE</type>
          <key>eventlog[Veeam Agent,,,Veeam Agent,195,,skip]</key>
          <delay>5m</delay>
          <history>30d</history>
          <trends>0</trends>
          <value_type>LOG</value_type>
          <applications>
            <application>
              <name>VeeamAgent_EventLog</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{logeventid(195)}=1 and {logseverity()}=4</expression>
              <name>VeeamAgent Synchronization completed with error</name>
              <priority>HIGH</priority>
              <description>{ITEM.LASTVALUE1} {ITEM.LASTVALUE2} {ITEM.LASTVALUE3}</description>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{logeventid(195)}=1 and {logseverity()}=2</expression>
              <name>VeeamAgent Synchronization completed with warning</name>
              <priority>WARNING</priority>
              <description>{ITEM.LASTVALUE1} {ITEM.LASTVALUE2} {ITEM.LASTVALUE3}</description>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>VeeamAgent_Service</name>
          <type>ZABBIX_ACTIVE</type>
          <key>service.info[VeeamEndpointBackupSvc]</key>
          <delay>5m</delay>
          <history>30d</history>
          <trends>30d</trends>
          <applications>
            <application>
              <name>VeeamAgent_Service</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{nodata(86400)}=1</expression>
              <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
              <recovery_expression>{last()}=0 or {last()}&lt;>0</recovery_expression>
              <name>VeeamAgent - No result from service since 24h</name>
              <priority>HIGH</priority>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>VeeamAgent - No job result since 1 Week</name>
                  <expression>{Template VEEAM-Agents:eventlog[Veeam Agent,,,Veeam Agent,190,,skip].nodata(604800)}=1</expression>
                </dependency>
              </dependencies>
            </trigger>
            <trigger>
              <expression>{last()}&lt;>0</expression>
              <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
              <recovery_expression>{last()}=0</recovery_expression>
              <name>VeeamAgent Service Down</name>
              <priority>HIGH</priority>
              <description>{ITEM.LASTVALUE1} {ITEM.LASTVALUE2} {ITEM.LASTVALUE3}</description>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
      </items>
    </template>
  </templates>
  <triggers>
    <trigger>
      <expression>{Template VEEAM-Agents:eventlog[Veeam Agent,,,Veeam Agent,191,,skip].logeventid(191)}=1 and {Template VEEAM-Agents:eventlog[Veeam Agent,,,Veeam Agent,191,,skip].logseverity()}=2</expression>
      <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
      <recovery_expression>({Template VEEAM-Agents:eventlog[Veeam Agent,,,Veeam Agent,190,,skip].logeventid(190)}=1 and {Template VEEAM-Agents:eventlog[Veeam Agent,,,Veeam Agent,190,,skip].logseverity()}=1) or ({Template VEEAM-Agents:eventlog[Veeam Agent,,,Veeam Agent,190,,skip].logeventid(190)}=1 and {Template VEEAM-Agents:eventlog[Veeam Agent,,,Veeam Agent,190,,skip].logseverity()}=2 and {Template VEEAM-Agents:eventlog[Veeam Agent,,,Veeam Agent,190,,skip].iregexp(low on free disk space)}=1)</recovery_expression>
      <name>VeeamAgent Job Retry</name>
      <priority>WARNING</priority>
      <description>{ITEM.LASTVALUE1} {ITEM.LASTVALUE2} {ITEM.LASTVALUE3}</description>
      <manual_close>YES</manual_close>
    </trigger>
  </triggers>
