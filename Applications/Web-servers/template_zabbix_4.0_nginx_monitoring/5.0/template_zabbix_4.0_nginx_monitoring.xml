    <graph>
      <name>File descriptors</name>
      <graph_items>
        <graph_item>
          <color>FF0000</color>
          <item>
            <host>Nginx</host>
            <key>nginx[open_file_descriptors]</key>
          </item>
        </graph_item>
        <graph_item>
          <sortorder>1</sortorder>
          <color>0040FF</color>
          <item>
            <host>Nginx</host>
            <key>nginx[max_file_descriptors]</key>
          </item>
        </graph_item>
      </graph_items>
    </graph>
    <graph>
      <name>Read/Write/Wait Status</name>
      <graph_items>
        <graph_item>
          <color>1A7C11</color>
          <item>
            <host>Nginx</host>
            <key>nginx[reading]</key>
          </item>
        </graph_item>
        <graph_item>
          <sortorder>1</sortorder>
          <color>F63100</color>
          <item>
            <host>Nginx</host>
            <key>nginx[waiting]</key>
          </item>
        </graph_item>
        <graph_item>
          <sortorder>2</sortorder>
          <color>2774A4</color>
          <item>
            <host>Nginx</host>
            <key>nginx[writing]</key>
          </item>
        </graph_item>
      </graph_items>
    </graph>
    <graph>
      <name>Worker_Processes/Worker_Connections/Maximum_Supported Connections</name>
      <graph_items>
        <graph_item>
          <color>F63100</color>
          <item>
            <host>Nginx</host>
            <key>nginx_max_connctions</key>
          </item>
        </graph_item>
        <graph_item>
          <sortorder>1</sortorder>
          <color>2774A4</color>
          <item>
            <host>Nginx</host>
            <key>nginx[worker_connections]</key>
          </item>
        </graph_item>
        <graph_item>
          <sortorder>2</sortorder>
          <color>EF6C00</color>
          <item>
            <host>Nginx</host>
            <key>nginx[worker_processes]</key>
          </item>
        </graph_item>
      </graph_items>
    </graph>
  </graphs>
</zabbix_export>