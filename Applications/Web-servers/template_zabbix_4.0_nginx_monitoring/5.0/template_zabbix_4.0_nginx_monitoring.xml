<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-19T21:21:53Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Nginx</template>
      <name>Nginx</name>
      <description>Nginx template created by Alireza Zabihi zabihi.a.r@gmail.com</description>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Nginx</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Accepted connections</name>
          <key>nginx[accepts]</key>
          <delay>120s</delay>
          <description>The total number of accepted client connections. handled</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Active connections</name>
          <key>nginx[active]</key>
          <delay>120s</delay>
          <description>The current number of active client connections including Waiting connections.</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Nginx service status</name>
          <key>nginx[check]</key>
          <delay>60s</delay>
          <history>30d</history>
          <description>Nginx service status</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}=0</expression>
              <name>Nginx Service is down</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>CPU Cores</name>
          <key>nginx[cpu_cores]</key>
          <delay>24h</delay>
          <history>30d</history>
          <description>Number of all CPU Cores on the system</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Handled connections</name>
          <key>nginx[handled]</key>
          <delay>120s</delay>
          <description>The total number of handled connections. Generally, the parameter value is the same as accepts unless some resource limits have been reached.</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Maximum file descriptors</name>
          <key>nginx[max_file_descriptors]</key>
          <delay>24h</delay>
          <history>30d</history>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Current open file descriptors</name>
          <key>nginx[open_file_descriptors]</key>
          <delay>240s</delay>
          <history>30d</history>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Reading connections</name>
          <key>nginx[reading]</key>
          <delay>120s</delay>
          <description>The current number of connections where nginx is reading the request header.</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Handles requests</name>
          <key>nginx[requests]</key>
          <delay>120s</delay>
          <description>The total number of client requests.</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Number of requests per connection</name>
          <key>nginx[req_per_conn]</key>
          <delay>120s</delay>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Waiting connections</name>
          <key>nginx[waiting]</key>
          <delay>120s</delay>
          <description>The current number of idle client connections waiting for a request.</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Number of maximum worker_connections per Process</name>
          <key>nginx[worker_connections]</key>
          <delay>12h</delay>
          <history>30d</history>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Number of perfork Nginx processes</name>
          <key>nginx[worker_processes]</key>
          <delay>12h</delay>
          <history>30d</history>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Writing connections</name>
          <key>nginx[writing]</key>
          <delay>120s</delay>
          <description>The current number of connections where nginx is writing the response back to the client.</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Number of Maximum supported connection depend on Configuration</name>
          <type>CALCULATED</type>
          <key>nginx_max_connctions</key>
          <delay>12h</delay>
          <history>30d</history>
          <params>last(nginx[worker_connections])*last(nginx[worker_processes])</params>
          <description>worker_processes * worker_connections</description>
          <applications>
            <application>
              <name>Nginx</name>
            </application>
          </applications>
        </item>
      </items>
    </template>
  </templates>
  <triggers>
    <trigger>
      <expression>{Nginx:nginx[worker_processes].last()} > {Nginx:nginx[cpu_cores].last()}</expression>
      <name>Not recommended configuration for worker processes ( worker_processes > CPU Cores )</name>
      <priority>WARNING</priority>
    </trigger>
  </triggers>
  <graphs>
    <graph>
      <name>Active/Accepted/Handled/Requests/Request_per_conn Connections</name>
      <graph_items>
        <graph_item>
          <color>1A7C11</color>
