          <name>IIS Service порт 80 (HTTP)</name>
          <key>net.tcp.listen[80]</key>
          <delay>30</delay>
          <value_type>FLOAT</value_type>
          <applications>
            <application>
              <name>IIS Службы</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(,0)}=0</expression>
              <name>Port 80 (HTTP)</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
