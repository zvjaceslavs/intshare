<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-26T04:39:36Z</date>
  <groups>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>UPS HDS</template>
      <name>UPS HDS</name>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Bateria</name>
        </application>
        <application>
          <name>Energia</name>
        </application>
        <application>
          <name>UPS</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Device Location</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>RFC1213-MIB::sysLocation.0</snmp_oid>
          <key>sysLocation</key>
          <delay>86400</delay>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <description>Informa a filial do nobreak.</description>
          <inventory_link>LOCATION</inventory_link>
          <applications>
            <application>
              <name>UPS</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Name Device</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>RFC1213-MIB::sysName.0</snmp_oid>
          <key>sysName</key>
          <delay>86400</delay>
          <history>30d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <description>Informa nome do Dispositivo</description>
          <inventory_link>NAME</inventory_link>
          <applications>
            <application>
              <name>UPS</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Uptime</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>RFC1213-MIB::sysUpTime.0</snmp_oid>
          <key>sysUpTime</key>
          <delay>600</delay>
          <trends>180d</trends>
          <units>uptime</units>
          <description>Informa à quanto tempo está on-line</description>
          <applications>
            <application>
              <name>UPS</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Status bateria</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.935.1.1.1.2.1.1.0</snmp_oid>
          <key>upsBaseBatteryStatus.0</key>
          <delay>120</delay>
          <trends>180d</trends>
          <description>Informa o status da Bateria. { unknown ( 1 ) , batteryNormal ( 2 ) , batteryLow ( 3 ) }</description>
          <applications>
            <application>
              <name>Bateria</name>
            </application>
          </applications>
          <valuemap>
            <name>UPS Battery Status</name>
          </valuemap>
        </item>
        <item>
          <name>Status saída nobreak</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.935.1.1.1.4.1.1.0</snmp_oid>
          <key>upsBaseOutputStatus.0</key>
          <delay>900</delay>
          <trends>180d</trends>
          <description>Informa o status atual da UPS</description>
          <applications>
            <application>
              <name>Energia</name>
            </application>
          </applications>
          <valuemap>
            <name>Status saída bateria UPS</name>
          </valuemap>
        </item>
        <item>
          <name>Voltagem Bateria</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>UPS-MIB::upsBatteryVoltage.0</snmp_oid>
          <key>upsBatteryVoltage.0</key>
          <delay>1800</delay>
          <trends>180d</trends>
          <units>Volts</units>
          <description>Informa a voltagem da Bateria</description>
          <applications>
            <application>
              <name>Bateria</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Frequência da Energia de Entrada</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>UPS-MIB::upsInputFrequency.1</snmp_oid>
          <key>upsInputFrequency.1</key>
          <delay>120</delay>
          <trends>180d</trends>
          <units>Hz</units>
          <description>Informa o valor da frequência da enerfica Input</description>
          <applications>
            <application>
              <name>Energia</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.1</params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>Voltagem Entrada de Energia</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>UPS-MIB::upsInputVoltage.1</snmp_oid>
          <key>upsInputVoltage.1</key>
          <delay>120</delay>
          <trends>180d</trends>
          <units>Volts</units>
          <applications>
            <application>
              <name>Energia</name>
            </application>
          </applications>
        </item>
        <item>
          <name>Capacidade da Bateria</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>RFC1213-MIB::mib-2.33.1.2.4.0</snmp_oid>
          <key>upsSmartBatteryCapacity</key>
          <delay>120</delay>
          <history>180d</history>
          <trends>180d</trends>
          <units>%</units>
          <description>A capacidade restante da bateria expressa em percentagem da capacidade total</description>
          <applications>
            <application>
              <name>Bateria</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0)}&lt;20</expression>
              <name>Nível de carga em 20%</name>
              <priority>HIGH</priority>
            </trigger>
            <trigger>
              <expression>{last(0)}&lt;50 and {last()}>20</expression>
              <name>Nível de carga em 50%</name>
              <priority>AVERAGE</priority>
              <description>Capacidade de carga entre 50% e 20%</description>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Temperatura UPS</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.935.1.1.1.2.2.3.0</snmp_oid>
          <key>upsSmartBatteryTemperature.0</key>
          <delay>900</delay>
          <history>60d</history>
          <trends>180d</trends>
          <units>°C</units>
          <applications>
            <application>
              <name>UPS</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.1</params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>Carga da UPS</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>RFC1213-MIB::mib-2.33.1.4.4.1.5.1</snmp_oid>
          <key>upsSmartOutputLoad.0</key>
          <delay>1500</delay>
          <trends>180d</trends>
          <units>%</units>
          <description>A carga atual do UPS expressa em porcentagem da capacidade nominal.</description>
          <applications>
            <application>
              <name>Bateria</name>
            </application>
          </applications>
        </item>
      </items>
      <macros>
        <macro>
          <macro>{$SNMP_COMMUNITY}</macro>
        </macro>
      </macros>
    </template>
  </templates>
