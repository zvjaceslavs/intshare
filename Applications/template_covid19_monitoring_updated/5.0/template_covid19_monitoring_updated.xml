              </preprocessing>
              <master_item>
                <key>covid19_status.py</key>
              </master_item>
            </item_prototype>
            <item_prototype>
              <name>[ {#COUNTRY_NAME} | {#COUNTRY_IBAN} ] - TotalRecovered</name>
              <type>DEPENDENT</type>
