  <templates>
    <template>
      <template>Covid19</template>
      <name>Coronavirus - Covid19 Monitoring</name>
      <description>Getting cases from worldometers.info and plotting on Zabbix</description>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Covid19 Data</name>
        </application>
        <application>
          <name>Covid19 General Info</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Covid19 - Total of Active Cases</name>
          <type>DEPENDENT</type>
          <key>covid19.data[ActiveCases]</key>
          <delay>0</delay>
          <history>30d</history>
          <value_type>FLOAT</value_type>
          <applications>
            <application>
              <name>Covid19 General Info</name>
            </application>
          </applications>
          <preprocessing>
            <step>
              <type>JSONPATH</type>
              <params>$['Total:'].ActiveCases</params>
            </step>
          </preprocessing>
          <master_item>
            <key>covid19_status.py</key>
          </master_item>
        </item>
        <item>
          <name>Covid19 - Total Cases per 1M habitants</name>
          <type>DEPENDENT</type>
