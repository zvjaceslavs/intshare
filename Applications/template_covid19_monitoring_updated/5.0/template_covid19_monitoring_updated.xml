      <discovery_rules>
        <discovery_rule>
          <name>Covid19 - Selected Countries</name>
          <type>EXTERNAL</type>
          <key>covid19_affectedcountries.py</key>
          <delay>1h</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>[ {#COUNTRY_NAME} | {#COUNTRY_IBAN} ] - ActiveCases</name>
              <type>DEPENDENT</type>
              <key>covid19.data["{#COUNTRY_NAME}", ActiveCases]</key>
              <delay>0</delay>
              <value_type>FLOAT</value_type>
              <application_prototypes>
                <application_prototype>
                  <name>Country Data: {#COUNTRY_NAME}</name>
                </application_prototype>
              </application_prototypes>
              <preprocessing>
                <step>
                  <type>JSONPATH</type>
                  <params>$['{#COUNTRY_NAME}'].ActiveCases</params>
                </step>
              </preprocessing>
              <master_item>
                <key>covid19_status.py</key>
              </master_item>
            </item_prototype>
            <item_prototype>
              <name>[ {#COUNTRY_NAME} | {#COUNTRY_IBAN} ] - Cases per Million habitants</name>
              <type>DEPENDENT</type>
              <key>covid19.data["{#COUNTRY_NAME}", CasesperMillion]</key>
              <delay>0</delay>
              <value_type>FLOAT</value_type>
              <application_prototypes>
                <application_prototype>
                  <name>Country Data: {#COUNTRY_NAME}</name>
