              </master_item>
            </item_prototype>
            <item_prototype>
              <name>[ {#COUNTRY_NAME} | {#COUNTRY_IBAN} ] - NewCases</name>
              <type>DEPENDENT</type>
              <key>covid19.data[ "{#COUNTRY_NAME}", NewCases]</key>
              <delay>0</delay>
              <value_type>FLOAT</value_type>
              <application_prototypes>
                <application_prototype>
                  <name>Country Data: {#COUNTRY_NAME}</name>
                </application_prototype>
              </application_prototypes>
              <preprocessing>
                <step>
                  <type>JSONPATH</type>
                  <params>$['{#COUNTRY_NAME}'].NewCases</params>
                </step>
              </preprocessing>
              <master_item>
                <key>covid19_status.py</key>
              </master_item>
            </item_prototype>
            <item_prototype>
              <name>[ {#COUNTRY_NAME} | {#COUNTRY_IBAN} ] - NewDeaths</name>
              <type>DEPENDENT</type>
              <key>covid19.data["{#COUNTRY_NAME}", NewDeaths]</key>
              <delay>0</delay>
              <value_type>FLOAT</value_type>
              <application_prototypes>
                <application_prototype>
                  <name>Country Data: {#COUNTRY_NAME}</name>
                </application_prototype>
              </application_prototypes>
              <preprocessing>
                <step>
                  <type>JSONPATH</type>
                  <params>$['{#COUNTRY_NAME}'].NewDeaths</params>
                </step>
              </preprocessing>
              <master_item>
                <key>covid19_status.py</key>
              </master_item>
            </item_prototype>
            <item_prototype>
              <name>[ {#COUNTRY_NAME} | {#COUNTRY_IBAN} ] - Serious,Critical</name>
              <type>DEPENDENT</type>
              <key>covid19.data["{#COUNTRY_NAME}", Serious]</key>
              <delay>0</delay>
              <value_type>FLOAT</value_type>
              <application_prototypes>
                <application_prototype>
                  <name>Country Data: {#COUNTRY_NAME}</name>
                </application_prototype>
              </application_prototypes>
              <preprocessing>
                <step>
                  <type>JSONPATH</type>
                  <params>$['{#COUNTRY_NAME}']['Serious,Critical']</params>
                </step>
              </preprocessing>
              <master_item>
                <key>covid19_status.py</key>
              </master_item>
            </item_prototype>
            <item_prototype>
              <name>[ {#COUNTRY_NAME} | {#COUNTRY_IBAN} ] - TotalCases</name>
              <type>CALCULATED</type>
              <key>covid19.data["{#COUNTRY_NAME}", TotalCases]</key>
              <delay>30m</delay>
              <value_type>FLOAT</value_type>
              <params>last("covid19.data[\"{#COUNTRY_NAME}\", ActiveCases]") + last("covid19.data[\"{#COUNTRY_NAME}\", TotalDeaths]") + last("covid19.data[\"{#COUNTRY_NAME}\", TotalRecovered]")</params>
              <application_prototypes>
                <application_prototype>
                  <name>Country Data: {#COUNTRY_NAME}</name>
                </application_prototype>
              </application_prototypes>
            </item_prototype>
            <item_prototype>
              <name>[ {#COUNTRY_NAME} | {#COUNTRY_IBAN} ] - TotalDeaths</name>
