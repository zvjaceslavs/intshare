<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2019-11-19T14:40:24Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
        <group>
            <name>Templates/Applications</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template App Varnish</template>
            <name>Template App Varnish</name>
            <description>Home made varnish template&#13;
&#13;
Authors : &#13;
Corentin MONTEL @ corentin.montel@yooda.com&#13;
Robin TALMA @ robin.talma@yooda.com&#13;
&#13;
requirements : &#13;
&#13;
&#13;
echo &quot;zabbix ALL=(ALL) NOPASSWD: /bin/netstat&quot; &gt;&gt; /etc/sudoers</description>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
                <group>
                    <name>Templates/Applications</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Varnish</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Varnish unavailable backend</name>
                    <key>varnish.state[backend]</key>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Varnish</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{str(&quot;null&quot;)}=0</expression>
                            <name>Varnish can't contact backend named {ITEM.VALUE}</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Varnish cached requests</name>
                    <key>varnish.state[cached_requests]</key>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>Varnish</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Varnish cache entries</name>
                    <key>varnish.state[cache_entries]</key>
                    <history>7d</history>
                    <units>Entries</units>
                    <applications>
                        <application>
                            <name>Varnish</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Varnish cache size</name>
                    <key>varnish.state[cache_size]</key>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>Varnish</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Varnish state</name>
                    <key>varnish.state[ping]</key>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Varnish</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{str(&quot;PONG&quot;)}=0</expression>
                            <name>Varnish is down !</name>
                            <priority>DISASTER</priority>
                            <description>Varnish don't ping (service, not ICMP)</description>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Varnish public port listen</name>
                    <key>varnish.state[port]</key>
                    <history>7d</history>
                    <description>Witch port is in use for varnish ?</description>
                    <applications>
                        <application>
                            <name>Varnish</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>Varnish port as changed to {ITEM.VALUE}</name>
                            <priority>AVERAGE</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Varnish uncached requests</name>
                    <key>varnish.state[uncached_requests]</key>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>Varnish</name>
                        </application>
                    </applications>
                </item>
            </items>
        </template>
    </templates>
    <graphs>
        <graph>
            <name>Varnish cache status</name>
            <show_work_period>NO</show_work_period>
            <show_triggers>NO</show_triggers>
            <graph_items>
                <graph_item>
                    <drawtype>GRADIENT_LINE</drawtype>
                    <color>B4B4B4</color>
                    <yaxisside>RIGHT</yaxisside>
                    <item>
                        <host>Template App Varnish</host>
                        <key>varnish.state[cache_size]</key>
                    </item>
                </graph_item>
                <graph_item>
                    <sortorder>1</sortorder>
                    <color>BF00FF</color>
                    <item>
                        <host>Template App Varnish</host>
                        <key>varnish.state[cache_entries]</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
        <graph>
            <name>Varnish cache usage</name>
            <width>450</width>
            <height>450</height>
            <yaxismax>0.0000</yaxismax>
            <show_work_period>NO</show_work_period>
            <show_triggers>NO</show_triggers>
            <type>PIE</type>
            <show_3d>YES</show_3d>
            <graph_items>
                <graph_item>
                    <color>FF8000</color>
                    <item>
                        <host>Template App Varnish</host>
                        <key>varnish.state[cached_requests]</key>
                    </item>
                </graph_item>
                <graph_item>
                    <sortorder>1</sortorder>
                    <color>0040FF</color>
                    <item>
                        <host>Template App Varnish</host>
                        <key>varnish.state[uncached_requests]</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
        <graph>
            <name>Varnish cache usage history</name>
            <ymin_type_1>FIXED</ymin_type_1>
            <graph_items>
                <graph_item>
                    <color>FF8000</color>
                    <item>
                        <host>Template App Varnish</host>
                        <key>varnish.state[cached_requests]</key>
                    </item>
                </graph_item>
                <graph_item>
                    <sortorder>1</sortorder>
                    <color>0040FF</color>
                    <item>
                        <host>Template App Varnish</host>
                        <key>varnish.state[uncached_requests]</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
    </graphs>
</zabbix_export>
