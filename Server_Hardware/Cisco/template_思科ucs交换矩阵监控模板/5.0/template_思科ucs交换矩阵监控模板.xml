<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-11-09T08:54:32Z</date>
  <groups>
    <group>
      <name>Templates/Server hardware</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>monitor_FabricInterconnect_Cisco_UCS</template>
      <name>思科UCS交换矩阵监控模板</name>
      <description>## Overview 思科UCS交换矩阵监控模板 包括CPU、Raid卡、内存、刀片、电源、硬盘、风扇、端口状态 将宏{$CLOUD\_SNMP\_COMMUNITY}替换为实际团体字即可 某些情况下监控是浮动地址无效，需要监控实际地址 ## Author zhangzhefeng</description>
      <groups>
        <group>
          <name>Templates/Server hardware</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>CPU情况</name>
        </application>
        <application>
          <name>ping检测</name>
        </application>
        <application>
          <name>Raid卡情况</name>
        </application>
        <application>
          <name>trap消息</name>
        </application>
        <application>
          <name>内存情况</name>
        </application>
        <application>
          <name>刀片情况</name>
        </application>
        <application>
          <name>电源情况</name>
        </application>
        <application>
          <name>硬盘情况</name>
        </application>
        <application>
          <name>端口状态</name>
        </application>
        <application>
          <name>系统信息</name>
        </application>
        <application>
          <name>风扇情况</name>
        </application>
      </applications>
      <items>
        <item>
          <name>ping丢包率</name>
          <type>SIMPLE</type>
          <key>icmppingloss[,10,,,]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(10m)}>{$ICMP_LOSS_WARN}</expression>
              <name>ICMP ping 丢包</name>
              <priority>HIGH</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_FabricInterconnect_Cisco_UCS:icmpping[,5,,,].last(#1)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ping时延</name>
          <type>SIMPLE</type>
          <key>icmppingsec[,10,,,,avg]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{min(10m)}>{$ICMP_RESPONSE_TIME_WARN}</expression>
              <name>ICMP ping 时延高</name>
              <priority>HIGH</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_FabricInterconnect_Cisco_UCS:icmpping[,5,,,].last(#1)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ICMP ping</name>
          <type>SIMPLE</type>
          <key>icmpping[,5,,,]</key>
          <history>7d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}=0</expression>
              <name>ICMP ping不可达</name>
              <priority>HIGH</priority>
              <description>Last value: {ITEM.LASTVALUE1}. Last 2 attempts returned timeout. Please check device connectivity.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{last(#1)}=0</expression>
              <name>监控失效（ping不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>ping不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>系统信息</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.1.0</snmp_oid>
          <key>SystemDescription</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{nodata(12h)}=1</expression>
              <name>监控失效（snmp不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>snmp不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>运行时间</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.3.0</snmp_oid>
          <key>SystemUpTime</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>刀片发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.9.9.719.1.9.2.1.2]</snmp_oid>
          <key>BladeDiscovery</key>
          <delay>1d</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>刀片运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.9.9.719.1.9.2.1.43.{#SNMPINDEX}</snmp_oid>
              <key>BladeOperationStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>刀片情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}>1</expression>
                  <name>刀片异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>CPU发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE}, 1.3.6.1.4.1.9.9.719.1.41.9.1.2,{#PRESENSE}, 1.3.6.1.4.1.9.9.719.1.41.9.1.13]</snmp_oid>
          <key>CPUDiscovery</key>
          <delay>1d</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#PRESENSE}</macro>
                <value>10</value>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>CPU运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.9.9.719.1.41.9.1.9.{#SNMPINDEX}</snmp_oid>
              <key>CPUStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>CPU情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}>1</expression>
                  <name>CPU状态异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>硬盘发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.9.9.719.1.45.4.1.2]</snmp_oid>
          <key>DiskDiscovery</key>
          <delay>1d</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>硬盘运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.9.9.719.1.45.4.1.9.{#SNMPINDEX}</snmp_oid>
              <key>DiskStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>硬盘情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}>1</expression>
                  <name>硬盘状态异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>风扇发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.9.9.719.1.15.12.1.2]</snmp_oid>
          <key>FanDiscovery</key>
          <delay>1d</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>风扇运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.9.9.719.1.15.12.1.9.{#SNMPINDEX}</snmp_oid>
              <key>FanOperationStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>风扇情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}>1</expression>
                  <name>风扇异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>端口发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#IFDESCR},IF-MIB::ifDescr,{#IFALIAS},IF-MIB::ifAlias]</snmp_oid>
          <key>InterfaceDiscovery_status</key>
          <delay>1d</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#IFDESCR}</macro>
                <value>.*Vlan.*</value>
                <operator>NOT_MATCHES_REGEX</operator>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>端口:{#IFDESCR}({#IFALIAS})状态</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>IF-MIB::ifOperStatus.{#SNMPINDEX}</snmp_oid>
              <key>InterfaceOperationStatus.[{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <description>The current operational state of the interface.</description>
              <applications>
                <application>
                  <name>端口状态</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#2)}=1 and {last(#1)}&lt;>1 and {last(#1)}&lt;>2</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{last(#2)}=1 and {last(#1)}=1</recovery_expression>
                  <name>{#IFDESCR} 端口状态异常</name>
                  <priority>AVERAGE</priority>
                  <description>因2019年10月1日二长7楼163方向防火墙出现状态7的告警，特增加此触发器</description>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{last(#2)}=1 and {last(#1)}=2</expression>
                  <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                  <recovery_expression>{last(#2)}=1 and {last(#1)}=1</recovery_expression>
                  <name>端口:{#IFDESCR} down</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                  <tags>
                    <tag>
                      <tag>端口描述</tag>
                      <value>{#IFALIAS}</value>
                    </tag>
                  </tags>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>内存发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.9.9.719.1.30.11.1.2,{#PRESENSE},1.3.6.1.4.1.9.9.719.1.30.11.1.17]</snmp_oid>
          <key>MemoryDiscovery</key>
          <delay>1d</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#PRESENSE}</macro>
                <value>10</value>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>内存运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.9.9.719.1.30.11.1.13.{#SNMPINDEX}</snmp_oid>
              <key>MemoryStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>内存情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}>1</expression>
                  <name>内存状态异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>电源发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.9.9.719.1.15.56.1.2]</snmp_oid>
          <key>PowerSupplyDiscovery</key>
          <delay>1d</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>电源运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.9.9.719.1.15.56.1.7.{#SNMPINDEX}</snmp_oid>
              <key>PowerSupplyOperationStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>电源情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}>1</expression>
                  <name>电源异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>RAID电池发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.9.9.719.1.45.11.1.2,{#PRESENSE},1.3.6.1.4.1.9.9.719.1.45.11.1.10]</snmp_oid>
          <key>RaidBatteryDiscovery</key>
          <delay>1d</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#PRESENSE}</macro>
                <value>10</value>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>Raid电池运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.9.9.719.1.45.11.1.9.{#SNMPINDEX}</snmp_oid>
              <key>RaidBatteryStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>Raid卡情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}>1</expression>
                  <name>Raid电池状态异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>RAID控制器发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE}, .1.3.6.1.4.1.9.9.719.1.45.1.1.2,{#PRESENSE}, .1.3.6.1.4.1.9.9.719.1.45.1.1.12]</snmp_oid>
          <key>RaidControllerDiscovery</key>
          <delay>1d</delay>
          <filter>
            <conditions>
              <condition>
                <macro>{#PRESENSE}</macro>
                <value>10</value>
                <formulaid>A</formulaid>
              </condition>
            </conditions>
          </filter>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>Raid控制器运行状态 {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.9.9.719.1.45.1.1.6.{#SNMPINDEX}</snmp_oid>
              <key>RaidControllerStatus.[{#SNMPINDEX}]</key>
              <delay>30m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>Raid卡情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}>1</expression>
                  <name>Raid控制器状态异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
      <macros>
        <macro>
          <macro>{$CLOUD_SNMP_COMMUNITY}</macro>
          <value>public</value>
        </macro>
        <macro>
          <macro>{$ICMP_LOSS_WARN}</macro>
          <value>30</value>
        </macro>
        <macro>
          <macro>{$ICMP_RESPONSE_TIME_WARN}</macro>
          <value>0.1</value>
        </macro>
      </macros>
    </template>
  </templates>
</zabbix_export>