      <discovery_rules>
        <discovery_rule>
          <name>FAN Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#FANDESCR},.1.3.6.1.4.1.19046.11.1.1.3.2.1.2]</snmp_oid>
          <key>fan.discovery</key>
          <delay>1h</delay>
          <description>Discovering fans from LENOVO-XCC-MIB::fanDescr</description>
          <item_prototypes>
            <item_prototype>
              <name>{#FAN_DESCR}: Fan speed, %</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.19046.11.1.1.3.2.1.3.{#SNMPINDEX}</snmp_oid>
              <key>sensor.fan.speed.percentage[fanSpeed.{#SNMPINDEX}]</key>
              <delay>5m</delay>
              <history>30d</history>
              <units>%</units>
              <description>LENOVO-XCC-MIB Fan speed expressed in percent(%) of maximum RPM. An octet string expressed as 'ddd% of maximum' where: d is a decimal digit or blank space for a leading zero. If the fan is determined not to be running or the fan speed cannot be determined, the string will indicate 'Offline'.</description>
              <applications>
                <application>
                  <name>Fans</name>
                </application>
              </applications>
              <preprocessing>
                <step>
                  <type>REGEX</type>
                  <params>(\d{1,3}) *%( of maximum)? \1</params>
                </step>
              </preprocessing>
            </item_prototype>
            <item_prototype>
              <name>{#FAN_DESCR}: Fan status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.19046.11.1.1.3.2.1.10.{#SNMPINDEX}</snmp_oid>
              <key>sensor.fan.status[fanHealthStatus.{#SNMPINDEX}]</key>
              <delay>3m</delay>
              <history>2w</history>
              <trends>0</trends>
              <value_type>CHAR</value_type>
              <description>LENOVO-XCC-MIB A description of the fan measurement channel.</description>
              <applications>
                <application>
                  <name>Fans</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{count(#1,{$FAN_OK_STATUS},ne)}=1</expression>
                  <name>{#FAN_DESCR}: Fan is not in normal state</name>
                  <priority>INFO</priority>
                  <description>Last value: {ITEM.LASTVALUE1}. Please check the fan unit</description>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Physical Disk Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.19046.11.1.1.12.2.1.1]</snmp_oid>
          <key>physicalDisk.discovery</key>
          <delay>1h</delay>
          <description>LENOVO-XCC-MIB:diskIndex</description>
          <item_prototypes>
            <item_prototype>
              <name>{#SNMPINDEX}: Physical disk part number</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.19046.11.1.1.12.2.1.2.{#SNMPINDEX}</snmp_oid>
              <key>system.hw.physicaldisk.part_number[diskFruName.{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <history>2w</history>
              <trends>0</trends>
              <value_type>CHAR</value_type>
              <description>LENOVO-XCC-MIB disk module FRU name.</description>
              <applications>
                <application>
                  <name>Physical Disks</name>
                </application>
              </applications>
            </item_prototype>
            <item_prototype>
              <name>{#SNMPINDEX}: Physical disk status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.19046.11.1.1.12.2.1.3.{#SNMPINDEX}</snmp_oid>
              <key>system.hw.physicaldisk.status[diskHealthStatus.{#SNMPINDEX}]</key>
              <delay>3m</delay>
              <history>30d</history>
              <trends>0</trends>
              <value_type>CHAR</value_type>
              <description>LENOVO-XCC-MIB A description of the disk module status.</description>
              <applications>
                <application>
                  <name>Physical Disks</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{count(#1,{$DISK_OK_STATUS},ne)}=1</expression>
                  <name>{#SNMPINDEX}: Physical disk is not in OK state</name>
                  <priority>WARNING</priority>
                  <description>Last value: {ITEM.LASTVALUE1}. Please check physical disk for warnings or errors</description>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>PSU Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#PSU_DESCR},.1.3.6.1.4.1.19046.11.1.1.11.2.1.2]</snmp_oid>
          <key>psu.discovery</key>
          <delay>1h</delay>
          <description>LENOVO-XCC-MIB::powerFruName</description>
          <item_prototypes>
            <item_prototype>
              <name>{#PSU_DESCR}: Power supply status</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.19046.11.1.1.11.2.1.6.{#SNMPINDEX}</snmp_oid>
              <key>sensor.psu.status[powerHealthStatus.{#SNMPINDEX}]</key>
              <delay>3m</delay>
              <history>2w</history>
              <trends>0</trends>
              <value_type>CHAR</value_type>
