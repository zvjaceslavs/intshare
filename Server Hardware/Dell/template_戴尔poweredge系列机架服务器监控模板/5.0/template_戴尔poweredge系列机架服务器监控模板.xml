<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T08:04:52Z</date>
  <groups>
    <group>
      <name>Templates/Server hardware</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>monitor_RackServer_Dell_PowerEdge</template>
      <name>戴尔PowerEdge系列机架服务器监控模板</name>
      <groups>
        <group>
          <name>Templates/Server hardware</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>CPU情况</name>
        </application>
        <application>
          <name>ping检测</name>
        </application>
        <application>
          <name>trap消息</name>
        </application>
        <application>
          <name>内存情况</name>
        </application>
        <application>
          <name>整体情况</name>
        </application>
        <application>
          <name>硬盘情况</name>
        </application>
        <application>
          <name>系统信息</name>
        </application>
        <application>
          <name>风扇情况</name>
        </application>
      </applications>
      <items>
        <item>
          <name>存储状态</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.674.10892.5.2.3.0</snmp_oid>
          <key>GlobalStorageStatus</key>
          <delay>10m</delay>
          <history>30d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>整体情况</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}>4</expression>
              <name>存储异常</name>
              <priority>HIGH</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>系统健康状态</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.674.10892.5.2.1.0</snmp_oid>
          <key>GlobalSystemStatus</key>
          <delay>10m</delay>
          <history>30d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>整体情况</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}>4</expression>
              <name>系统异常</name>
              <priority>HIGH</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ping丢包率</name>
          <type>SIMPLE</type>
          <key>icmppingloss[,10,,,]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{avg(10m)}>{$ICMP_LOSS_WARN}</expression>
              <name>ICMP ping 丢包</name>
              <priority>HIGH</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_RackServer_Dell_PowerEdge:icmpping[,5,,,].last(#1)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ping时延</name>
          <type>SIMPLE</type>
          <key>icmppingsec[,10,,,,avg]</key>
          <delay>5m</delay>
          <history>7d</history>
          <trends>0d</trends>
          <value_type>FLOAT</value_type>
          <units>s</units>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{min(10m)}>{$ICMP_RESPONSE_TIME_WARN}</expression>
              <name>ICMP ping 时延高</name>
              <priority>HIGH</priority>
              <description>Last value: {ITEM.LASTVALUE1}.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
              <dependencies>
                <dependency>
                  <name>ICMP ping不可达</name>
                  <expression>{monitor_RackServer_Dell_PowerEdge:icmpping[,5,,,].last(#1)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>ICMP ping</name>
          <type>SIMPLE</type>
          <key>icmpping[,5,,,]</key>
          <history>7d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>ping检测</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}=0</expression>
              <name>ICMP ping不可达</name>
              <priority>HIGH</priority>
              <description>Last value: {ITEM.LASTVALUE1}. Last 2 attempts returned timeout. Please check device connectivity.</description>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
            <trigger>
              <expression>{last(#1)}=0</expression>
              <name>监控失效（ping不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>ping不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>系统信息</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.1.0</snmp_oid>
          <key>SystemDescription</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{nodata(12h)}=1</expression>
              <name>监控失效（snmp不通）</name>
              <priority>INFO</priority>
              <manual_close>YES</manual_close>
              <tags>
                <tag>
                  <tag>监控失效原因</tag>
                  <value>snmp不通</value>
                </tag>
              </tags>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>LCD状态</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.674.10892.5.2.2.0</snmp_oid>
          <key>SystemLCDStatus</key>
          <delay>10m</delay>
          <history>30d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>整体情况</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}>4</expression>
              <name>LCD异常</name>
              <priority>HIGH</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>电源状态</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.674.10892.5.2.4.0</snmp_oid>
          <key>SystemPowerStatus</key>
          <delay>10m</delay>
          <history>30d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>整体情况</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}>4</expression>
              <name>电源异常</name>
              <priority>HIGH</priority>
              <type>MULTIPLE</type>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>运行时间</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.2.1.1.3.0</snmp_oid>
          <key>SystemUpTime</key>
          <delay>1h</delay>
          <history>3d</history>
          <trends>0</trends>
          <value_type>TEXT</value_type>
          <applications>
            <application>
              <name>系统信息</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>CPU发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#CPUNAME},1.3.6.1.4.1.674.10892.5.4.1100.32.1.7]</snmp_oid>
          <key>CPUDiscovery</key>
          <delay>1d</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>CPU状态 [{#CPUNAME}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.674.10892.5.4.1100.32.1.5.{#SNMPINDEX}</snmp_oid>
              <key>CPUStatus.[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <history>7d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>CPU情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>3</expression>
                  <name>CPU异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>风扇发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#FANNAME},1.3.6.1.4.1.674.10892.5.4.700.12.1.8.1]</snmp_oid>
          <key>FanDiscovery</key>
          <delay>1d</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>风扇状态 {#FANNAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.674.10892.5.4.700.12.1.5.1.{#SNMPINDEX}</snmp_oid>
              <key>FanStatus.[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <history>7d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>风扇情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>3</expression>
                  <name>风扇异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>内存发现</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#MEMORYNAME},1.3.6.1.4.1.674.10892.5.4.1100.50.1.8]</snmp_oid>
          <key>MemoryDiscovery</key>
          <delay>1d</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>内存状态 {#MEMORYNAME}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.674.10892.5.4.1100.50.1.5.{#SNMPINDEX}</snmp_oid>
              <key>MemoryStatus.[{#SNMPINDEX}]</key>
              <delay>1h</delay>
              <history>7d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>内存情况</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>3</expression>
                  <name>内存异常</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
      <macros>
        <macro>
          <macro>{$CLOUD_SNMP_COMMUNITY}</macro>
          <value>public</value>
        </macro>
        <macro>
          <macro>{$ICMP_LOSS_WARN}</macro>
          <value>30</value>
        </macro>
        <macro>
          <macro>{$ICMP_RESPONSE_TIME_WARN}</macro>
          <value>0.1</value>
        </macro>
      </macros>
    </template>
  </templates>
</zabbix_export>