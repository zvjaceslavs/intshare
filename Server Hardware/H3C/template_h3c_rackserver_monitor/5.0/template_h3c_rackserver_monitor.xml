<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T08:04:41Z</date>
  <groups>
    <group>
      <name>Templates/Server hardware</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>H3C_RackServer_monitor_template</template>
      <name>H3C RackServer Monitor Template</name>
      <description>H3C_RackServer_monitor_template Known Issues: device : UIS-Cell-3030-G3,UniServer-R4900</description>
      <groups>
        <group>
          <name>Templates/Server hardware</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>CPU Status</name>
        </application>
        <application>
          <name>Disk Status</name>
        </application>
        <application>
          <name>Entity Status</name>
        </application>
        <application>
          <name>Fan Status</name>
        </application>
        <application>
          <name>Memory Status</name>
        </application>
        <application>
          <name>PCIE Status</name>
        </application>
        <application>
          <name>Power Status</name>
        </application>
      </applications>
      <items>
        <item>
          <name>Entity Health Status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25506.13.1.2.4.7.1.0</snmp_oid>
          <key>hh3cEntityHealthStatus</key>
          <delay>10m</delay>
          <history>3d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>Entity Status</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}&lt;>0</expression>
              <name>Entity Abnormal</name>
              <priority>AVERAGE</priority>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Fan Health Status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25506.13.1.2.4.7.3.0</snmp_oid>
          <key>hh3cFanHealthStatus</key>
          <delay>10m</delay>
          <history>3d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>Fan Status</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}&lt;>0</expression>
              <name>Fan Abnormal</name>
              <priority>AVERAGE</priority>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>Power Health Status</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>1.3.6.1.4.1.25506.13.1.2.4.7.2.0</snmp_oid>
          <key>hh3cPwrHealthStatus</key>
          <delay>10m</delay>
          <history>3d</history>
          <trends>0d</trends>
          <applications>
            <application>
              <name>Power Status</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(#1)}&lt;>0</expression>
              <name>Power Abnormal</name>
              <priority>AVERAGE</priority>
              <manual_close>YES</manual_close>
            </trigger>
          </triggers>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>CPU Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.25506.13.1.2.1.1.1.1.1.15]</snmp_oid>
          <key>CPUDiscovery</key>
          <delay>1h</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>CPU Status [{#SNMPVALUE}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.25506.13.1.2.1.1.1.1.1.6.{#SNMPINDEX}</snmp_oid>
              <key>CPUStatus.[{#SNMPINDEX}]</key>
              <delay>10m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>CPU Status</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}=0</expression>
                  <name>CPU Abnormal</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Disk Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.25506.13.1.2.1.1.9.1.1.2]</snmp_oid>
          <key>DisckDiscovery</key>
          <delay>1h</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>Disk Status [{#SNMPVALUE}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.25506.13.1.2.1.1.9.1.1.4.{#SNMPINDEX}</snmp_oid>
              <key>DiskStatus.[{#SNMPINDEX}]</key>
              <delay>10m</delay>
              <history>3d</history>
              <trends>0</trends>
              <value_type>CHAR</value_type>
              <applications>
                <application>
                  <name>Disk Status</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{iregexp(FAILED,#1)}=1</expression>
                  <name>Disk Abnormal</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>Memory Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.25506.13.1.2.1.1.2.2.1.2]</snmp_oid>
          <key>MemoryDiscovery</key>
          <delay>1h</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>Memory Status {#SNMPVALUE}</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.25506.13.1.2.1.1.2.2.1.5.{#SNMPINDEX}</snmp_oid>
              <key>MemoryStatus.[{#SNMPINDEX}]</key>
              <delay>10m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>Memory Status</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}=2</expression>
                  <name>Memory Abnormal</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
        <discovery_rule>
          <name>PCIE Discovery</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.25506.13.1.2.4.7.6.1.2]</snmp_oid>
          <key>NetCardDiscovery</key>
          <delay>1h</delay>
          <lifetime>0d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>PCIE Status Slot[{#SNMPVALUE}]</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>1.3.6.1.4.1.25506.13.1.2.4.7.6.1.3.{#SNMPINDEX}</snmp_oid>
              <key>NetCardStatus.[{#SNMPINDEX}]</key>
              <delay>10m</delay>
              <history>3d</history>
              <trends>0d</trends>
              <applications>
                <application>
                  <name>PCIE Status</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last(#1)}=1</expression>
                  <name>PCIE Abnormal</name>
                  <priority>HIGH</priority>
                  <type>MULTIPLE</type>
                  <manual_close>YES</manual_close>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
      <macros>
        <macro>
          <macro>{$CLOUD_SNMP_COMMUNITY}</macro>
          <value>public</value>
        </macro>
      </macros>
    </template>
  </templates>
</zabbix_export>