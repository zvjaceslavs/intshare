  <templates>
    <template>
      <template>Template APC ATS</template>
      <name>Template APC ATS</name>
      <description>APC Rack Automatic Transfer Switches (rack ATS) provide reliable, redundant rack mount power to single-corded equipment. The rack ATS has dual input power cords supplying power to the connected load. If the primary power source becomes unavailable, the rack ATS will seamlessly source power from the secondary source without interrupting critical loads.</description>
      <templates>
        <template>
          <name>Template SNMP Device</name>
        </template>
      </templates>
      <groups>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Power sources</name>
        </application>
      </applications>
      <items>
        <item>
          <name>atsInputVoltage.1</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.8.5.3.3.1.3.1.1.1</snmp_oid>
          <key>atsInputVoltage.1.1.1</key>
          <delay>30s</delay>
          <units>V</units>
        </item>
        <item>
          <name>atsInputVoltage.2</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.8.5.3.3.1.3.2.1.1</snmp_oid>
          <key>atsInputVoltage.2.1.1</key>
          <delay>30s</delay>
          <units>v</units>
        </item>
        <item>
          <name>#atsNumInputs</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.8.5.3.1.0</snmp_oid>
          <key>atsNumInputs.0</key>
          <delay>30s</delay>
          <triggers>
            <trigger>
              <expression>{last()}=1</expression>
              <name>Input Redundancy Fail</name>
              <priority>DISASTER</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>atsOutputCurrent</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.8.5.4.3.1.4.1.1.1</snmp_oid>
          <key>atsOutputCurrent.1.1.1</key>
          <delay>30s</delay>
          <units>A</units>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.1</params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>atsOutputVoltage</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.8.5.4.3.1.3.1.1.1</snmp_oid>
          <key>atsOutputVoltage.1.1.1</key>
          <delay>30s</delay>
          <units>v</units>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>Internal APC Sensor</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.318.1.1.8.5.4.3.1.3]</snmp_oid>
          <key>apc18953A</key>
          <delay>5</delay>
          <item_prototypes>
            <item_prototype>
              <name>atsOutputVoltage</name>
              <type>SNMP_AGENT</type>
              <snmp_oid>.1.3.6.1.4.1.318.1.1.8.5.4.3.1.3.{#SNMPINDEX}</snmp_oid>
              <key>atsOutputVoltage.["{#SNMPINDEX}"]</key>
              <delay>5</delay>
              <trends>180d</trends>
              <status>DISABLED</status>
              <description>{#SNMPVALUE}</description>
              <applications>
                <application>
                  <name>Power sources</name>
                </application>
              </applications>
            </item_prototype>
          </item_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
