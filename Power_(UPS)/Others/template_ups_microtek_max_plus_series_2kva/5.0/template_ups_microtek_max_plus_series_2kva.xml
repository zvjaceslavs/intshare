            </application>
          </applications>
          <preprocessing>
            <step>
              <type>MULTIPLIER</type>
              <params>0.1</params>
            </step>
          </preprocessing>
        </item>
        <item>
          <name>UPS output load (%)</name>
          <type>SNMP_AGENT</type>
          <snmp_oid>.1.3.6.1.2.1.33.1.4.4.1.5.1</snmp_oid>
          <key>upsAdvOutputLoad</key>
          <delay>60</delay>
          <history>7d</history>
          <value_type>FLOAT</value_type>
          <units>%</units>
          <applications>
            <application>
              <name>Operational</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0)}>95</expression>
              <name>Output load Critical on {HOST.NAME}</name>
              <priority>HIGH</priority>
              <dependencies>
                <dependency>
                  <name>Unavailable by ICMP ping</name>
                  <expression>{Template UPS Microtek MAX plus SERIES 2kVA:icmpping.max(#3)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
            <trigger>
              <expression>{last(0)}>90</expression>
              <name>Output load High on {HOST.NAME}</name>
              <priority>WARNING</priority>
              <dependencies>
                <dependency>
                  <name>Unavailable by ICMP ping</name>
                  <expression>{Template UPS Microtek MAX plus SERIES 2kVA:icmpping.max(#3)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>UPS output power</name>
          <type>SNMP_AGENT</type>
