<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
  <version>5.0</version>
  <date>2021-10-22T10:08:44Z</date>
  <groups>
    <group>
      <name>DB: Oracle</name>
    </group>
    <group>
      <name>Templates</name>
    </group>
  </groups>
  <templates>
    <template>
      <template>Template_OCI_Dbaas</template>
      <name>Template_OCI_Dbaas</name>
      <groups>
        <group>
          <name>DB: Oracle</name>
        </group>
        <group>
          <name>Templates</name>
        </group>
      </groups>
      <applications>
        <application>
          <name>Oracle</name>
        </application>
      </applications>
      <items>
        <item>
          <name>[Oracle] Database is active and open</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_active,1]</key>
          <delay>3600</delay>
          <history>7d</history>
          <trends>15d</trends>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0)}=0</expression>
              <name>[{$ORACLE_SID}] Oracle: The database is inactive or closed</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] ASM Data Area percentage used</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_asmdata,1]</key>
          <delay>600</delay>
          <history>7d</history>
          <trends>15d</trends>
          <units>%</units>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}>90</expression>
              <name>[{$ORACLE_SID}] ASM: High percentage used data space</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] ASM Recovery Area percentage used</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_asmreco,1]</key>
          <delay>600</delay>
          <history>7d</history>
          <trends>15d</trends>
          <units>%</units>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}>90</expression>
              <name>[{$ORACLE_SID}] ASM: High percentage used recovery space</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] Sessions blocks</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_block,1]</key>
          <delay>60</delay>
          <history>1d</history>
          <trends>5d</trends>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{change(0)}>0</expression>
              <name>[{$ORACLE_SID}] Oracle: Session blocks are increasing</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] Connections available</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_connection,1]</key>
          <delay>300</delay>
          <history>7d</history>
          <trends>15d</trends>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
        </item>
        <item>
          <name>[Oracle] Flash Recovery Area percentage used</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_fra,1]</key>
          <delay>600</delay>
          <history>7d</history>
          <trends>15d</trends>
          <units>%</units>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last()}>90</expression>
              <name>[{$ORACLE_SID}] FRA: High percentage used space</name>
              <priority>HIGH</priority>
            </trigger>
            <trigger>
              <expression>{last()}>80</expression>
              <name>[{$ORACLE_SID}] FRA: Warning percentage used space</name>
              <priority>WARNING</priority>
              <dependencies>
                <dependency>
                  <name>[{$ORACLE_SID}] FRA: High percentage used space</name>
                  <expression>{Template_OCI_Dbaas:zabbix-oci-dbaas[{$ORACLE_SID},db_fra,1].last()}>90</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] Invalid objects</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_invalid,1]</key>
          <delay>3600</delay>
          <history>7d</history>
          <trends>15d</trends>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{change(0)}>0</expression>
              <name>[{$ORACLE_SID}] Oracle: Invalid objects are increasing</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] Locks count</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_lock,1]</key>
          <delay>60</delay>
          <history>1d</history>
          <trends>5d</trends>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{change(0)}>0</expression>
              <name>[{$ORACLE_SID}] Oracle: Locks count are increasing</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] Response Time Per Txn</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_metric,'Response Time Per Txn']</key>
          <delay>60</delay>
          <history>30d</history>
          <trends>30d</trends>
          <value_type>FLOAT</value_type>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
        </item>
        <item>
          <name>[Oracle] Redo Logs count</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_redo,1]</key>
          <delay>600</delay>
          <history>7d</history>
          <trends>15d</trends>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
        </item>
        <item>
          <name>[Oracle] RMAN problems counter</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_rman,1]</key>
          <delay>86400</delay>
          <history>7d</history>
          <trends>15d</trends>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0)}>0</expression>
              <name>[{$ORACLE_SID}] Oracle: RMAN has problems</name>
              <priority>HIGH</priority>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] Database size</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_size,1]</key>
          <delay>86400</delay>
          <history>7d</history>
          <trends>15d</trends>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
        </item>
        <item>
          <name>[Oracle] Database uptime</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_uptime,1]</key>
          <delay>600</delay>
          <history>7d</history>
          <trends>30d</trends>
          <units>s</units>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
          <triggers>
            <trigger>
              <expression>{last(0)}&lt;600</expression>
              <name>[{$ORACLE_SID}] Oracle: The database has just been restarted</name>
              <status>DISABLED</status>
              <priority>INFO</priority>
              <dependencies>
                <dependency>
                  <name>[{$ORACLE_SID}] Oracle: The database is inactive or closed</name>
                  <expression>{Template_OCI_Dbaas:zabbix-oci-dbaas[{$ORACLE_SID},db_active,1].last(0)}=0</expression>
                </dependency>
              </dependencies>
            </trigger>
          </triggers>
        </item>
        <item>
          <name>[Oracle] Database version</name>
          <key>zabbix-oci-dbaas[{$ORACLE_SID},db_version,1]</key>
          <delay>86400</delay>
          <history>5d</history>
          <trends>0</trends>
          <value_type>CHAR</value_type>
          <applications>
            <application>
              <name>Oracle</name>
            </application>
          </applications>
        </item>
      </items>
      <discovery_rules>
        <discovery_rule>
          <name>[Oracle] Tablespaces</name>
          <key>zabbix-oci-dbaas.discovery[{$ORACLE_SID},tb_list,1,ORACLE_TBS]</key>
          <delay>86400</delay>
          <lifetime>1d</lifetime>
          <item_prototypes>
            <item_prototype>
              <name>[Oracle] Tablespace $3 percentage used</name>
              <key>zabbix-oci-dbaas[{$ORACLE_SID},tb_size,{#ORACLE_TBS}]</key>
              <delay>600</delay>
              <history>7d</history>
              <trends>30d</trends>
              <value_type>FLOAT</value_type>
              <units>%</units>
              <applications>
                <application>
                  <name>Oracle</name>
                </application>
              </applications>
              <trigger_prototypes>
                <trigger_prototype>
                  <expression>{last()}>95</expression>
                  <name>[{$ORACLE_SID}] Tablespace: High used space on {#ORACLE_TBS}</name>
                  <priority>HIGH</priority>
                </trigger_prototype>
                <trigger_prototype>
                  <expression>{last()}>90 and {last()}&lt;95</expression>
                  <name>[{$ORACLE_SID}] Tablespace: Warning used space on {#ORACLE_TBS}</name>
                  <priority>WARNING</priority>
                </trigger_prototype>
              </trigger_prototypes>
            </item_prototype>
          </item_prototypes>
          <graph_prototypes>
            <graph_prototype>
              <name>[Oracle][{$ORACLE_SID}]Tablespace used: {#ORACLE_TBS}</name>
              <type>STACKED</type>
              <ymin_type_1>FIXED</ymin_type_1>
              <ymax_type_1>FIXED</ymax_type_1>
              <graph_items>
                <graph_item>
                  <color>C80000</color>
                  <yaxisside>RIGHT</yaxisside>
                  <item>
                    <host>Template_OCI_Dbaas</host>
                    <key>zabbix-oci-dbaas[{$ORACLE_SID},tb_size,{#ORACLE_TBS}]</key>
                  </item>
                </graph_item>
              </graph_items>
            </graph_prototype>
          </graph_prototypes>
        </discovery_rule>
      </discovery_rules>
    </template>
  </templates>
</zabbix_export>